<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
background_visible="false"
default_tab_group="1"
follows="all"
height="570"
label="Outfits"
layout="topleft"
min_height="350"
min_width="240"
name="appearance panel"
top="0"
left="0"
width="333">
   <string
   name="No Outfit"
   value="No Outfit" />
   <string
   name="Unsaved Changes"
   value="Unsaved changes" />
   <string
   name="Now Wearing"
   value="Now wearing..." />
   <string
   name="Changing outfits"
   value="Changing outfits" />
   <panel
   background_opaque="true"
   background_visible="true"
   bg_opaque_color="White_02"
   left="10"
   top="5"
   follows="left|top|right"
   layout="topleft"
   width="315"
   height="26"
   name="panel_currentlook">
      <button
      follows="bottom|left"
      height="25"
      tool_tip="Create a new outfit"
      image_hover_unselected="Toolbar_Middle_Over"
       image_overlay="AddItem_Off"
       image_selected="Toolbar_Middle_Selected"
       image_unselected="Toolbar_Middle_Off"
      layout="topleft"
      left="1"
      top="1"
      name="newlook_btn"
      width="31" />
      <button
      follows="top|right"
      height="25"
      image_overlay="Edit_Wrench"
      image_hover_unselected="Toolbar_Middle_Over"
       image_selected="Toolbar_Middle_Selected"
       image_unselected="Toolbar_Middle_Off"
      label=""
      layout="topleft"
      right="-1"
      name="edit_outfit_btn"
      tool_tip="Edit this outfit"
      top="1"
      width="31" />
      <loading_indicator
      follows="top|right"
      height="24"
      layout="topleft"
      left_delta="4"
      name="wearables_loading_indicator"
      top="2"
      width="24" />
   </panel>
   <layout_stack
   follows="top|left|right"
   height="27"
   layout="topleft"
   border_size="0"
   mouse_opaque="false"
   name="appearance_panel_ls"
   left="43"
   orientation="horizontal"
   top="5"
   width="250">	
      <layout_panel
      follows="top|left|right"
      height="27"
      layout="topleft"
      left="0"
      mouse_opaque="false"
      name="outfit_name_lp"
      auto_resize="true"
      min_width="216"
      width="216">
         <icon
      follows="top|left|right"
      height="25"
      name="bg"
      mouse_opaque="false"
      image_name="Toolbar_Middle_Off"
      left="0"
      top="1"
      width="215" />
         <text
         font="SansSerifSmall"
         text_color="BeaconColor"
         width="210"
         height="10"
         follows="top|left|right"
         layout="topleft"
         left="3"
         top="1"
         mouse_opaque="false"
         name="currentlook_status" >
         (Status)
         </text>
         <text
         font="SansSerifSmall"
         height="10"
         left="3"
         text_color="White"
         top="12"
         use_ellipses="true"
         width="210"
         follows="top|left|right"
         word_wrap="false"
         mouse_opaque="false"
         name="currentlook_name">
         MyOutfit With a really Long Name like MOOSE
         </text>
  </layout_panel>
      <layout_panel
      follows="top|left|right"
      height="27"
      layout="topleft"
      left="0"
      mouse_opaque="false"
      name="search_btn_lp"
      auto_resize="false"
      min_width="32"
      width="32">
         <button
         follows="top|left"
         height="25"
         image_overlay="Search_Icon"
         image_hover_unselected="Toolbar_Middle_Over"
          image_selected="Toolbar_Middle_Selected"
          image_unselected="Toolbar_Middle_Off"
         label=""
         layout="topleft"
         left="0"
         name="show_search_btn"
         tool_tip="Show search filters"
         top="1"
         width="31" />
      </layout_panel>
      <layout_panel
      follows="top|left|right"
      height="27"
      layout="topleft"
      left="0"
      mouse_opaque="false"
      visible="false"
      name="search_lp"
      auto_resize="true"
      width="216">
         <icon
      follows="top|left|right"
      height="25"
      name="bg"
      mouse_opaque="false"
      image_name="Toolbar_Middle_Off"
      left="0"
      top="1"
      width="215" />
         <filter_editor
         height="23"
         follows="left|top|right"
         layout="topleft"
         left="2"
         label="Filter Outfits"
         name="Filter"
         background_image="TextField_Off"
         background_image_disabled="TextField_Disabled"
         background_image_focused="TextField_Active"
         top="2"
         width="211" />
      </layout_panel>
   </layout_stack>
   <panel
   class="panel_outfits_inventory"
   filename="panel_outfits_inventory.xml"
   name="panel_outfits_inventory"
   height="533"
   min_height="410"
   width="320"
   visible="false"
   left="0"
   tab_group="1"
   top_pad="2"
   follows="all" />
   <panel
   class="panel_outfit_edit"
   filename="panel_outfit_edit.xml"
   height="565"
   follows="all"
   layout="topleft"
   left="5"
   min_height="410"
   name="panel_outfit_edit"
   top="2"
   visible="false" 
   width="320"/>
   <panel
   class="panel_edit_wearable"
   filename="panel_edit_wearable.xml"
   follows="all"
   layout="topleft"
   left="0"
   name="panel_edit_wearable"
   top="0"
   visible="false" />
</panel>
