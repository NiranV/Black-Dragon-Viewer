<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 follows="top|right|left"
 height="21"
 layout="topleft"
 left="0"
 name="group_list_item"
 top="0"
 width="320">
    <icon
     follows="top|right|left"
     height="21"
     image_name="ListItem_Over"
     layout="topleft"
     left="0"
     name="hovered_icon"
     top="0"
     visible="false"
     width="320" />
    <icon
     height="21"
     follows="top|right|left"
     image_name="ListItem_Select"
     layout="topleft"
     left="0"
     name="selected_icon"
     top="0"
     visible="false"
     width="320" />
    <icon
     height="18"
     image_name="Generic_Group"
     name="group_icon"
     mouse_opaque="true"
     left="1"
     top="1"
     use_draw_context_alpha="false"
     width="18" />
   <text
     parse_urls="false"
     follows="left|right"
     font="SansSerifSmall"
     height="14"
     layout="topleft"
     left_pad="25"
     name="group_name"
     top="5"
     use_ellipses="true"
     value="Unknown"
     width="275" />
    <button
     follows="top|left"
     height="18"
     image_pressed="transparent.j2c"
     image_unselected="transparent.j2c"
     left="1"
     name="info_btn"
     tool_tip="More info"
     tab_stop="false"
     top="2"
     width="18" />
   <!--*TODO: Should only appear on rollover-->
    <button
     follows="top|left"
     height="18"
     image_overlay="Web_Profile_Off"
     image_pressed="transparent.j2c"
    image_unselected="transparent.j2c"
     layout="topleft"
     left="22"
     name="profile_btn"
     tab_stop="false"
     visible="false"
     tool_tip="View profile"
     top="2"
     width="18" />
</panel>
