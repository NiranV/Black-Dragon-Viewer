<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 follows="all"
 height="557"
 label="Group Profile"
 layout="topleft"
 min_height="325"
 left="0"
 top="20"
 name="GroupInfo"
 width="325">
    <panel.string
     name="default_needs_apply_text">
        There are unsaved changes
    </panel.string>
    <panel.string
     name="want_apply_text">
        Do you want to save these changes?
    </panel.string>
    <panel.string
     name="group_join_btn">
        Join (L$[AMOUNT])
    </panel.string>
    <panel.string
     name="group_join_free">
        Free
    </panel.string>
    <panel
      name="group_info_top"
      follows="top|left|right"
      background_visible="true"
      bg_alpha_color="White_02"
      top="1"
      left="10"
      height="25"
      width="308"
      layout="topleft">
      <button
      follows="top|left"
      height="25"
      image_hover_unselected="Toolbar_Middle_Over"
       image_overlay="BackArrow_Off"
       image_selected="Toolbar_Middle_Selected"
       image_unselected="Toolbar_Middle_Off"
      layout="topleft"
      left="0"
      top="1"
      name="back"
      width="31" />
      <icon
      follows="top|left|right"
      height="25"
      name="bg"
      mouse_opaque="false"
      image_name="Toolbar_Middle_Off"
      left_pad="1"
      top_delta="0"
      width="275" />
    <!--<button
     follows="top|right"
     height="24"
     image_hover_unselected="BackButton_Over"
     image_pressed="BackButton_Press"
     image_unselected="BackButton_Off"
     layout="topleft"
     name="back"
     left="7"
     tab_stop="false"
     top="2"
     width="30"
     use_draw_context_alpha="false" />-->
    <text_editor
     allow_scroll="false"
     bg_visible="false"
     read_only = "true"
     layout="topleft"
     name="group_name"
     v_pad="0"
     value="(Loading...)"
      font="SansSerif"
     h_pad="0"
      height="14"
      left_delta="3"
     text_color="LtGray"
      top="6"
     use_ellipses="true"
      width="267"
      follows="top|left|right"
      word_wrap="false"
      mouse_opaque="false"/>
    <line_editor
     follows="left|top|right"
     font="SansSerif"
     label="Type your new group name here"
     layout="topleft"
     left_delta="-1"
     max_length_bytes="35"
     name="group_name_editor"
     top_delta="-4"
     width="270"
     height="23"
     visible="true" />
    </panel>
      <tab_container
    border="false"
     follows="all"
     height="483"
     halign="center"
     layout="topleft"
     left="4"
     name="roles_tab_container"
     tab_position="top"
     tab_height="21"
     tab_min_width="77"
     top_pad="13"
     width="316">
                   <panel
                      border="false"
                      class="panel_group_general"
                      filename="panel_group_general.xml"
                      layout="topleft"
                      left="0"
                      label="General"
                      follows="left|top|right"
                      help_topic="group_general_tab"
                      name="group_general_tab_panel"
                      top="0" />
                <panel
                   border="false"
                   class="panel_group_roles"
                   background_visible="true"
     bg_alpha_color="White_02"
                   filename="panel_group_roles.xml"
                   follows="left|top|right"
                   layout="topleft"
                   left="0"
                   label="Roles"
                   name="group_roles_tab_panel"
                   top="0" />
                <panel
                    border="false"
                    class="panel_group_notices"
                    background_visible="true"
     bg_alpha_color="White_02"
                    filename="panel_group_notices.xml"
                    follows="left|top|right"
                    layout="topleft"
                    left="0"
                    label="Notices"
                    help_topic="group_notices_tab"
                    name="group_notices_tab_panel"
                    top="0" />
                 <panel
					 border="false"
                     class="panel_group_land_money"
                     background_visible="true"
     bg_alpha_color="White_02"
                     filename="panel_group_land_money.xml"
                     follows="left|top|right"
                     layout="topleft"
                     left="0"
                     label="Land"
                     name="group_land_tab_panel"
                     top="0" />
      </tab_container>
   
      <panel
      name="group_info_top"
      follows="bottom|left|right"
      background_visible="true"
      bg_alpha_color="White_02"
      top_pad="-1"
      left="6"
      height="25"
      width="312"
      layout="topleft">
   		   <layout_stack
	     	follows="bottom|left|right"
			height="26"
			layout="topleft"
			name="button_row_ls"
            border_size="0"
			left="0"
			orientation="horizontal"
			top="0"
			width="312">
	
				<layout_panel
				follows="bottom|left|right"
				height="26"
				layout="bottomleft"
				left="0"
				name="btn_refresh_lp"
			    auto_resize="false"
				width="32">
					<button
				     follows="bottom|left|right"
				     height="25"
				     image_overlay="Refresh_Off"
                     image_hover_unselected="Toolbar_Middle_Over"
                     image_selected="Toolbar_Middle_Selected"
                     image_unselected="Toolbar_Middle_Off"
				     layout="topleft"
				     left="1"
				     top="0"
				     name="btn_refresh"
				     width="31" />
				</layout_panel>
				
				<layout_panel
				follows="bottom|left|right"
				height="26"
				layout="bottomleft"
				name="btn_chat_lp"
			    auto_resize="true"
				width="95">
                  <icon
                  follows="left|bottom|right"
                  height="25"
                  name="no_chat"
                  layout="topleft"
                  mouse_opaque="false"
                  image_name="Toolbar_Middle_Off"
                  left="1"
                  top="0"
                  width="94" />
					<button
				     follows="bottom|left|right"
				     label="Chat"
				     name="btn_chat"
                     image_hover_unselected="Toolbar_Middle_Over"
                     image_selected="Toolbar_Middle_Selected"
                     image_unselected="Toolbar_Middle_Off"
				     left="1"     
				     height="25"
				     top="0"
				     width="94" />		
				</layout_panel>
				
				<layout_panel
				follows="bottom|left|right"
				height="26"
				layout="bottomleft"
				name="call_btn_lp"
			    auto_resize="true"
				width="95">
                  <icon
                  follows="left|bottom|right"
                  height="25"
                  name="no_call"
                  mouse_opaque="false"
                  layout="topleft"
                  image_name="Toolbar_Middle_Off"
                  left="1"
                  top="0"
                  width="94" />
					<button
			         follows="bottom|left|right"
			         left="1"
			         height="25"
				     name="btn_call"
                     image_hover_unselected="Toolbar_Middle_Over"
                     image_selected="Toolbar_Middle_Selected"
                     image_unselected="Toolbar_Middle_Off"
				     label="Group Call"
			         layout="topleft"
			         tool_tip="Call this group"
			         top="0"
			         width="94" />		
				</layout_panel>
				
				<layout_panel
				follows="bottom|left|right"
				height="26"
				layout="bottomleft"
				name="btn_apply_lp"
			    auto_resize="true"
				width="95">
					<button
				     follows="bottom|left|right"
				     height="25"
				     label="Save"
				     label_selected="Save"
				     name="btn_apply"
                     image_hover_unselected="Toolbar_Middle_Over"
                     image_selected="Toolbar_Middle_Selected"
                     image_unselected="Toolbar_Middle_Off"
				     left="1"
				     top="0"
				     width="94" />
					<button
    				 follows="bottom|left|right"
	                 height="25"
	                 layout="topleft"
	                 left="1"
	                 top="0"
	                 label="Create Group"
     				 name="btn_create"
                     image_hover_unselected="Toolbar_Middle_Over"
                     image_selected="Toolbar_Middle_Selected"
                     image_unselected="Toolbar_Middle_Off"
               		 visible="true"
                 	 tool_tip="Create a new Group"
                 	 width="94" />	
				</layout_panel>
		   </layout_stack>
      </panel>
</panel>
