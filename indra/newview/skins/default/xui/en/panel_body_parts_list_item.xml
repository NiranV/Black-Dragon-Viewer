<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 follows="top|right|left"
 height="18"
 layout="topleft"
 left="0"
 name="wearable_item"
 top="0"
 width="380">
    <icon
     follows="top|right|left"
     height="17"
     image_name="ListItem_Over"
     layout="topleft"
     left="0"
     name="hovered_icon"
     top="1"
     visible="false"
     width="380" />
    <icon
     height="17"
     follows="top|right|left"
     image_name="ListItem_Select"
     layout="topleft"
     left="0"
     name="selected_icon"
     top="1"
     visible="false"
     width="380" />
    <icon
     height="16"
     follows="top|left"
     image_name="Inv_Object"
     layout="topleft"
     left="1"
     name="item_icon"
     top="3"
     width="16" />
    <text
     follows="left|right"
     height="14"
     layout="topleft"
     left_pad="5"
     parse_urls="false"
     use_ellipses="true"
     name="item_name"
     text_color="white"
     top="3"
     value="..."
     width="329" />
    <panel
     background_visible="false"
     name="btn_edit_panel"
     layout="topleft"
     follows="top|right"
     top="1"
     left_pad="3"
     height="18"
     width="18"
     tab_stop="false">
         <button 
          name="btn_edit"
          layout="topleft"
          follows="top|right"
          image_unselected="Edit_Wrench"
          image_selected="Edit_Wrench"
          top="1"
          left="1"
          height="16"
          width="16"
          tab_stop="false"
          tool_tip="Edit this shape"/>
      </panel>
    <panel
     background_visible="false"
     name="btn_lock"
     layout="topleft"
     follows="top|right"
     top="0"
     left="1"
     height="18"
     width="20"
     tab_stop="false"
     tool_tip="You don't have permission to edit">
        <icon 
         name="btn_lock1"
         layout="topleft"
         follows="top|right"
         image_name="Lock"
         top="0"
         left="0"
         scale_image="false"
         height="18"
         width="18"
         tab_stop="false" />
    </panel>
</panel>
