<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
  legacy_header_height="18"
  can_resize="true"
  height="400"
  layout="topleft"
  follows="all"
  min_height="400"
  min_width="200"
  name="floater_web_content"
  save_rect="true"
  title=""
  width="560">
    <icon
       follows="all"
       height="362"
       image_name="Panel_Background"
       layout="topleft"
       left="7"
       top="18"
       name="dummy_icon"
       width="547"/>
  <layout_stack
    top="20"
    follows="left|right|top|bottom"
    layout="topleft"
    left="8"
    width="549"
    height="380"
    name="stack1"
    orientation="vertical">
    <layout_panel
      auto_resize="false"
      height="22"
      layout="topleft"
      left="0"
      min_height="22"
      name="nav_controls"
      width="395">
      <button
        image_overlay="Arrow_Left_Off"
        image_hover_unselected="Toolbar_Middle_Over"
        image_selected="Toolbar_Middle_Selected"
        image_unselected="Toolbar_Middle_Off"
        hover_glow_amount="0.15"
        tool_tip="Navigate back"
        follows="left|top"
        height="23"
        layout="topleft"
        left="1"
        name="back"
        top="0"
        width="25">
        <button.commit_callback
          function="WebContent.Back" />
      </button>
      <button
        image_overlay="Home_Off"
        image_hover_unselected="Toolbar_Middle_Over"
        image_selected="Toolbar_Middle_Selected"
        image_unselected="Toolbar_Middle_Off"
        chrome="true"
        tool_tip="Open start page"
        enabled="true"
        follows="left|top"
        height="23"
        layout="topleft"
        left_pad="1"
        name="web_test_home_page"
        top="0"
        width="25">
        <button.commit_callback
          function="WebContent.TestURL"
          parameter="https://callum-linden.s3.amazonaws.com/ceftests.html"/>
      </button>
      <button
        image_overlay="Arrow_Right_Off"
        image_hover_unselected="Toolbar_Middle_Over"
        image_selected="Toolbar_Middle_Selected"
        image_unselected="Toolbar_Middle_Off"
        tool_tip="Navigate forward"
        follows="left|top"
        height="23"
        layout="topleft"
        left_pad="1"
        name="forward"
        top_delta="0"
        width="25">
        <button.commit_callback
          function="WebContent.Forward" />
      </button>
      <button
        image_overlay="Stop_Off"
        image_hover_unselected="Toolbar_Middle_Over"
        image_selected="Toolbar_Middle_Selected"
        image_unselected="Toolbar_Middle_Off"
        tool_tip="Stop navigation"
        follows="left|top"
        height="23"
        layout="topleft"
        left_pad="1"
        name="stop"
        top_delta="0"
        width="25">
        <button.commit_callback
          function="WebContent.Stop" />
      </button>
      <button
        image_overlay="Refresh_Off"
        image_hover_unselected="Toolbar_Middle_Over"
        image_selected="Toolbar_Middle_Selected"
        image_unselected="Toolbar_Middle_Off"
        tool_tip="Reload page"
        visible="false"
        follows="left|top"
        height="23"
        layout="topleft"
        left_delta="0"
        name="reload"
        top_delta="0"
        width="25">
        <button.commit_callback
          function="WebContent.Reload" />
      </button>
      <combo_box
        allow_text_entry="true"
        follows="left|top|right"
        tab_group="1"
        height="23"
        layout="topleft"
        left_pad="1"
        max_chars="1024"
        name="address"
        combo_editor.select_on_focus="true"
        tool_tip="Enter URL here"
        top_delta="0"
        width="267">
          <combo_box.combo_editor
          font="SansSerif"
          font_shadow="soft"
          text_tentative_color="0.7 0.7 0.7 0.7"
          cursor_color="White"
          text_color="0.75 0.75 0.75 0.7"
          background_image="Toolbar_Middle_Off"
          background_image_disabled="Toolbar_Middle_Selected"
          background_image_focused="Toolbar_Middle_Off"/>
          <combo_box.combo_button
          image_unselected="ComboToolbarButton_Off"
          image_selected="ComboToolbarButton_Selected"
          image_pressed="ComboToolbarButton_Selected" 
          image_pressed_selected="ComboToolbarButton_Selected"/>
          <combo_box.drop_down_button
          image_unselected="ComboToolbarButton_Off"
          image_selected="ComboToolbarButton_Selected"
          image_pressed="ComboToolbarButton_Selected" 
          image_pressed_selected="ComboToolbarButton_Selected"
          image_disabled="ComboToolbarButton_Disabled"/>
            <combo_box.combo_list
              bg_writeable_color="0 0 0 0.9"
              background_visible="true"/>
        <combo_box.commit_callback
          function="WebContent.EnterAddress" />
      </combo_box>
      <icon
        name="media_secure_lock_flag"
        height="16"
        follows="top|left"
        image_name="Lock2"
        layout="topleft"
        left_delta="2"
        top_delta="3"
        visible="true" 
        tool_tip="Secured Browsing"
        width="16" />
      <button
        image_overlay="ExternalBrowser_Off"
        image_hover_unselected="Toolbar_Middle_Over"
        image_selected="Toolbar_Middle_Selected"
        image_unselected="Toolbar_Middle_Off"
        tool_tip="Open current URL in your desktop browser"
        follows="right|top"
        enabled="true" 
        height="23"
        layout="topleft"
        name="popexternal"
        right="390"
        top="0"
        width="25">
        <button.commit_callback
          function="WebContent.PopExternal" />
      </button>
      <progress_bar
        color_bar="1 1 1 0"
        color_bg="1 1 1 0"
        follows="left|top|right"
        height="0"
        top_delta="21"
        left="1"
        right="-5"
        layout="topleft"
        name="statusbarprogress"/>
    </layout_panel>
