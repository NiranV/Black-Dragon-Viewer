    <layout_panel
      height="40"
      layout="topleft"
      left_delta="0"
      name="external_controls"
      top_pad="0"
      width="390">
      <web_browser
        bottom="-22"
        follows="all"
        layout="topleft"
        left="1"
        width="384"
        name="webbrowser"
        top="0"/>
      <text
        type="string"
        length="200"
        follows="left|bottom|right"
        height="20"
        layout="topleft"
        left_delta="0"
        name="statusbartext"
        parse_urls="false"
        top_pad="3"
        width="385"/>
    </layout_panel>
  </layout_stack>
</floater>
