<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
  legacy_header_height="18"
  can_resize="true"
  height="400"
  layout="topleft"
  follows="all"
  min_height="400"
  min_width="200"
  name="floater_web_content"
  background_color="FloaterDefaultBackgroundColor"
  save_rect="true"
  title=""
  width="560">
  <layout_stack
    top="20"
    follows="left|right|top|bottom"
    layout="topleft"
    left="5"
    width="555"
    height="382"
    name="stack1"
    orientation="vertical">
    <layout_panel
      auto_resize="false"
      height="28"
      layout="topleft"
      left="0"
      min_height="28"
      name="nav_controls"
      width="395">
      <button
        image_overlay="Arrow_Left_Off"
        image_disabled="PushButton_Disabled"
        image_disabled_selected="PushButton_Disabled"
        image_selected="PushButton_Selected"
        image_unselected="PushButton_Off"
        hover_glow_amount="0.15"
        tool_tip="Navigate back"
        follows="left|top"
        height="22"
        layout="topleft"
        left="1"
        name="back"
        top="0"
        width="23">
        <button.commit_callback
          function="WebContent.Back" />
      </button>
      <button
        image_overlay="Arrow_Right_Off"
        image_disabled="PushButton_Disabled"
        image_disabled_selected="PushButton_Disabled"
        image_selected="PushButton_Selected"
        image_unselected="PushButton_Off"
        tool_tip="Navigate forward"
        follows="left|top"
        height="22"
        layout="topleft"
        left="25"
        name="forward"
        top_delta="0"
        width="25">
        <button.commit_callback
          function="WebContent.Forward" />
      </button>
      <button
        image_overlay="Stop_Off"
        image_disabled="PushButton_Disabled"
        image_disabled_selected="PushButton_Disabled"
        image_selected="PushButton_Selected"
        image_unselected="PushButton_Off"
        tool_tip="Stop navigation"
        enabled="true"
        follows="left|top"
        height="22"
        layout="topleft"
        left="57"
        name="stop"
        top_delta="0"
        width="25">
        <button.commit_callback
          function="WebContent.Stop" />
      </button>
      <button
        image_overlay="Refresh_Off"
        image_disabled="PushButton_Disabled"
        image_disabled_selected="PushButton_Disabled"
        image_selected="PushButton_Selected"
        image_unselected="PushButton_Off"
        tool_tip="Reload page"
        follows="left|top"
        height="22"
        layout="topleft"
        left="57"
        name="reload"
        top_delta="0"
        width="25">
        <button.commit_callback
          function="WebContent.Reload" />
      </button>
      <combo_box
        allow_text_entry="true"
        follows="left|top|right"
        tab_group="1"
        height="22"
        layout="topleft"
        left_pad="4"
        max_chars="1024"
        name="address"
        combo_editor.select_on_focus="true"
        tool_tip="Enter URL here"
        top_delta="0"
        width="280">
        <combo_box.commit_callback
          function="WebContent.EnterAddress" />
      </combo_box>
      <icon
        name="media_secure_lock_flag"
        height="16"
        follows="top|left"
        image_name="Lock2"
        layout="topleft"
        left_delta="2"
        top_delta="3"
        visible="true" 
        tool_tip="Secured Browsing"
        width="16" />
      <progress_bar
        color_bar="1 1 1 1"
        follows="left|top|right"
        height="9"
        top_delta="18"
        left="0"
        right="-5"
        layout="topleft"
        name="statusbarprogress"/>
      <button
        image_overlay="ExternalBrowser_Off"
        image_disabled="PushButton_Disabled"
        image_disabled_selected="PushButton_Disabled"
        image_selected="PushButton_Selected"
        image_unselected="PushButton_Off"
        tool_tip="Open current URL in your desktop browser"
        follows="right|top"
        enabled="true" 
        height="22"
        layout="topleft"
        name="popexternal"
        right="390"
        top="0"
        width="25">
        <button.commit_callback
          function="WebContent.PopExternal" />
      </button>
    </layout_panel>
    <layout_panel
      height="40"
      layout="topleft"
      left_delta="0"
      name="external_controls"
      top_pad="0"
      width="390">
      <web_browser
        bottom="-22"
        follows="all"
        layout="topleft"
        left="0"
        width="385"
        name="webbrowser"
        top="0"/>
      <text
        type="string"
        length="200"
        follows="left|bottom|right"
        height="20"
        layout="topleft"
        left_delta="0"
        name="statusbartext"
        parse_urls="false"
        text_color="EmphasisColor"
        top_pad="3"
        width="385"/>
    </layout_panel>
  </layout_stack>
</floater>
