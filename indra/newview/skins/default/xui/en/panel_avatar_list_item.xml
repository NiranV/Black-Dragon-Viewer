<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 follows="top|right|left"
 height="21"
 layout="topleft"
 left="0"
 name="avatar_list_item"
 top="0"
 width="320">
    <!-- 
    Strings used to localize last interaction time.
    See last_interaction textbox below.
    -->
    <string name="FormatSeconds">[COUNT]s</string>
    <string name="FormatMinutes">[COUNT]m</string>
    <string name="FormatHours">[COUNT]h</string>
    <string name="FormatDays">[COUNT]d</string>
    <string name="FormatWeeks">[COUNT]w</string>
    <string name="FormatMonths">[COUNT]mon</string>
    <string name="FormatYears">[COUNT]y</string>
    <icon
    follows="top|right|left"
    height="21"
    image_name="ListItem_Over"
    layout="topleft"
    left="0"
    name="hovered_icon"
    top="0"
    visible="false"
    width="320" />
    <icon
    height="21"
    follows="top|right|left"
    image_name="ListItem_Select"
    layout="topleft"
    left="0"
    name="selected_icon"
    top="0"
    visible="false"
    width="320" />
    <avatar_icon
    follows="top|left"
    height="18"
    default_icon_name="Generic_Person"
    layout="topleft"
    left="1"
    mouse_opaque="true"
    top="2"
    width="18" />
    <button
    follows="top|left"
    height="18"
    image_pressed="transparent.j2c"
    image_unselected="transparent.j2c"
    layout="topleft"
    left="1"
    name="info_btn"
    tab_stop="false"
    top="1"
    visible="true"
    width="18" />
    <text
    follows="top|left|right"
    font="SansSerifSmall"
    font_shadow="soft"
    height="13"
    layout="topleft"
    left="25"
    word_wrap="false"
    name="avatar_name"
    parse_urls="false"
    top="5"
    use_ellipses="true"
    value="(loading)"
    width="292"/>
    <text
    follows="top|left|right"
    font="SansSerifSmall"
    font_shadow="soft"
    height="11"
    layout="topleft"
    halign="right"
    right="-4"
    top="5"
    name="extra_information"
    text_color="1 1 1 0.7"
    value=""
    width="100" />
    <output_monitor
    auto_update="true"
    follows="top|right"
    draw_border="false"
    height="20"
    layout="topleft"
    right="-62"
    top="0"
    visible="true"
    mouse_opaque="true"
    name="speaking_indicator"
    width="20" />
    <icon
    height="16"
    follows="right|top"
    image_name="Permission_Edit_Objects_Theirs"
    layout="topleft"
    right="-47"
    visible="false"
    color="1 1 1 0.5"
    name="permission_edit_theirs_icon"
    tool_tip="You can edit this friend&apos;s objects"
    top="3"
    width="16" />
    <icon
    height="16"
    follows="right|top"
    image_name="Permission_Edit_Objects_Mine"
    layout="topleft"
    right="-32"
    visible="false"
    color="1 1 1 0.5"
    name="permission_edit_mine_icon"
    tool_tip="This friend can edit, delete or take your objects"
    top="3"
    width="16" />
    <icon
    height="16"
    follows="right|top"
    image_name="Permission_Visible_Map"
    layout="topleft"
    tool_tip="This friend can locate you on the map"
    right="-17"
    visible="false"
    color="1 1 1 0.5"
    name="permission_map_icon"
    top="3"
    width="16" />
    <icon
    height="16"
    follows="right|top"
    image_name="Permission_Visible_Online"
    layout="topleft"
    right="-2"
    visible="false"
    color="1 1 1 0.5"
    name="permission_online_icon"
    tool_tip="This friend can see when you&apos;re online"
    top="3"
    width="16" />
</panel>
