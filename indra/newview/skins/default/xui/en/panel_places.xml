<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
background_visible="false"
 default_tab_group="1"
 follows="all"
 height="570"
 label="Places"
 background_color="FloaterColorPlaces"
 layout="topleft"
 min_height="350"
 name="places panel"
 top="0"
 left="0"
 width="333">
    <string
     name="landmarks_tab_title"
     value="My Landmarks" />
    <string
     name="teleport_history_tab_title"
     value="Teleport History" />
    <tab_container
     follows="all"
     halign="center"
	 bottom="-7"
     layout="topleft"
     left="4"
	 right="-6"
     name="Places Tabs"
     tab_min_width="150"
     tab_max_width="157"
     tab_height="20"
     tab_group="2"
     tab_position="top"
     top="4"/>
    <panel
     class="panel_place_profile"
     filename="panel_place_profile.xml"
     follows="all"
     height="533"
     layout="topleft"
     left="7"
     name="panel_place_profile"
     top="35"
     visible="false"
     width="300" />
    <panel
     class="panel_landmark_info"
     filename="panel_landmark_info.xml"
     follows="all"
     height="533"
     layout="topleft"
     left="7"
     name="panel_landmark_info"
     top="35"
     visible="false"
     width="300" />
    <panel
     follows="bottom|left|right"
     height="25"
     layout="topleft"
     left="4"
	 bottom="-9"
     name="button_panel"
     width="315">
	  <filter_editor
     text_pad_left="10"
     follows="left|bottom|right"
	 background_image="TextField_Off"
	 background_image_disabled="TextField_Disabled"
	 background_image_focused="TextField_Active"
     font="SansSerifSmall"
     height="22"
     layout="topleft"
     left="6"
     label="Filter My Places"
     max_length="300"
     name="Filter"
     tab_group="1"
     top="1"
     width="216" />
   
	      <menu_button
	       follows="bottom|left"
		   image_hover_unselected="Toolbar_Middle_Over"
		   image_selected="Toolbar_Middle_Selected"
		   image_unselected="Toolbar_Middle_Off"
	       height="25"
		   label="▼"
	       layout="topleft"
	       mouse_opaque="false"
	       name="overflow_btn"
	       tool_tip="Show additional options"
	       top="0"
	       left="5"		         
	       width="30" />
		  <button
	       follows="bottom|left|right"
		   image_hover_unselected="Toolbar_Middle_Over"
		   image_selected="Toolbar_Middle_Selected"
		   image_unselected="Toolbar_Middle_Off"
	       height="25"
	       label="Edit"
	       layout="topleft"
	       left_pad="1"
	       mouse_opaque="false"
	       name="edit_btn"
	       tool_tip="Edit landmark information"
	       top="0"
	       width="93" />
		
			<button
	  follows="bottom|right"
	  image_hover_unselected="Toolbar_Middle_Over"
	  image_selected="Toolbar_Middle_Selected"
	  image_unselected="Toolbar_Middle_Off"
	  height="25"
	  label="Teleport"
	  layout="topleft"
	  left_pad="1"
	  name="teleport_btn"
	  tool_tip="Teleport to the selected area"
	  top="0"
	  width="106" />		

	 <button
	  follows="bottom|right"
	  image_hover_unselected="Toolbar_Middle_Over"
	  image_selected="Toolbar_Middle_Selected"
	  image_unselected="Toolbar_Middle_Off"
	  height="25"
	  label="Map"
	  layout="topleft"
	  left_pad="1"
	  name="map_btn"
	  tool_tip="Show the corresponding area on the World Map"
	  top="0"
	  width="83" />	
		  
		     <button
		      follows="bottom|right"
			  image_hover_unselected="Toolbar_Middle_Over"
			  image_selected="Toolbar_Middle_Selected"
			  image_unselected="Toolbar_Middle_Off"
		      height="25"
		      label="Close"
		      layout="topleft"
		      mouse_opaque="false"
		      name="close_btn"
		      left_pad="1"
		      top="0"
		      width="50" />
	
		
	 
	   <button
	    follows="bottom|left|right"
		image_hover_unselected="Toolbar_Middle_Over"
		image_selected="Toolbar_Middle_Selected"
		image_unselected="Toolbar_Middle_Off"
	    height="25"
	    label="Save"
	    layout="topleft"
	    mouse_opaque="false"
	    name="save_btn"
	    left="5"
	    top="0"
	    width="157"/>
	   <button
	    follows="bottom|right"
		image_hover_unselected="Toolbar_Middle_Over"
		image_selected="Toolbar_Middle_Selected"
		image_unselected="Toolbar_Middle_Off"
	    height="25"
	    label="Cancel"
	    layout="topleft"
	    mouse_opaque="false"
	    name="cancel_btn"
	    left_pad="1"
	    top="0"
	    width="157" />
</panel>
</panel>