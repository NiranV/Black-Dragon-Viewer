	  <layout_panel
	  follows="top|left|right"
	  layout="topleft"
	  height="19"
	  min_width="19"
	  mouse_opaque="false"
	  auto_resize="false"
	  visibility_control="ShowNavbarFavoritesPanel"
	  name="favorites_layout_panel"
	  width="320">
		 <favorites_bar
		 follows="left|right|top"
		 font="SansSerifSmall"
		 height="18"
		 layout="topleft"
		 left="2"
		 top="1"
		 name="favorite"
		 image_drag_indication="Accordion_ArrowOpened_Off"
		 tool_tip="Drag Landmarks here for quick access to your favorite places in Second Life!"
		 width="320">
			<label
			follows="left|top"
			height="13"
			layout="topleft"
			left="0"
			name="favorites_bar_label"
			text_color="LtGray"
			tool_tip="Drag Landmarks here for quick access to your favorite places in Second Life!"
			top="14"
			width="102">
			| Favorites Bar |
			</label>
			<!-- More button actually is a text box. -->
			<more_button
			follows="left|bottom"
			name=">>"
			tab_stop="false"
			tool_tip="Show more of My Favorites"
			top="14"
			width="50"
			bottom="0"
			valign="bottom">
			More &#9660;
			</more_button>
		 </favorites_bar>
	  </layout_panel>
	  <layout_panel
	  follows="top|left|right"
	  auto_resize="false"
	  mouse_opaque="false"
	  layout="topleft"
	  height="26"
	  min_height="26"
	  name="navigation_layout_panel"
	  width="272">
		<layout_stack
		follows="all"
		height="26"
		layout="topleft"
		mouse_opaque="false"
		border_size="-5"
		left="0"
		name="nvpb_stack"
		orientation="horizontal"
		top="0"
		right="-260"
		width="272">
		<layout_panel
		follows="top|left|right"
		auto_resize="true"
		mouse_opaque="false"
		layout="topleft"
		height="24"
		name="navigationbar_layout_panel"
		width="272">
		 <pull_button
		 follows="left|top"
		 direction="down"
		 height="18"
		 image_overlay="Arrow_Left_Off"
		 image_selected="transparent.j2c"
		 image_unselected="transparent.j2c"
		 image_bottom_pad="1"
		 layout="topleft"
		 left="86"
		 name="back_btn"
		 tool_tip="Go back to previous location"
		 top="4"
		 width="18" />
		 <button
		 follows="left|top"
		 height="18"
		 image_bottom_pad="1"
		 image_overlay="Home_Off"
		 image_selected="transparent.j2c"
		 image_unselected="transparent.j2c"
		 layout="topleft"
		 left_pad="0"
		 name="home_btn"
		 tool_tip="Teleport to my home location"
		 top_delta="0"
		 width="18" />
		 <pull_button
		 follows="left|top"
		 direction="down"
		 height="18"
		 image_overlay="Arrow_Right_Off"
		 image_selected="transparent.j2c"
		 image_unselected="transparent.j2c"
		 image_bottom_pad="1"
		 layout="topleft"
		 left_pad="0"
		 name="forward_btn"
		 tool_tip="Go forward one location"
		 top_delta="0"
		 width="18" />
		 <location_input
		 follows="top|left|right"
		 halign="right"
		 height="23"
		 label="Location"
		 layout="topleft"
		 left="147"
		 max_chars="254"
		 mouse_opaque="false"
		 name="location_combo"
		 top="1"
		 width="127">
		 <combo_list
		 mouse_wheel_opaque="true"/>
		 </location_input>
		</layout_panel>
		 <layout_panel
		visibility_control="ShowTopSearchBar"
		follows="top|left|right"
		auto_resize="false"
		mouse_opaque="false"
		layout="topleft"
		height="24"
		name="navigation_layout_panel"
		width="120">
		  <search_combo_box
		  follows="left|top"
		  height="23"
		  label="Search"
		  layout="topleft"
		  left="2"
		  mouse_opaque="false"
		  name="search_combo_box"
		  tool_tip="Search for something in Second Life"
		  top="1"
		  width="112" >
		  <combo_editor
		  background_image="TextField_Off"
		  background_image_disabled="TextField_Disabled"
		  background_image_focused="TextField_Active"
		  label="Search"
		  text_pad_left="19" 
		  text_pad_right="4" 
		  name="search_combo_editor"/>
		  <search_button 
		  left_pad="4" />
		  <combo_list
		  draw_border="false"
		  border.highlight_light_color="FocusColor"
		  border.highlight_dark_color="FocusColor"
		  border.shadow_light_color="FocusColor"
		  border.shadow_dark_color="FocusColor"
		  mouse_wheel_opaque="true" />
		  </search_combo_box>
		</layout_panel>
      <layout_panel
		visibility_control="MenuSearch"
		follows="top|left|right"
		auto_resize="false"
		mouse_opaque="false"
		layout="topleft"
		height="24"
		name="navigation_layout_panel"
		width="121">
       <filter_editor
		 follows="left|top"
		 height="23"
		 label="Search Menus"
		 layout="topleft"
		 left="2"
       width="118"
		 max_length_bytes="255"
		 name="search_menu_edit"
       background_image="TextField_Off"
       background_image_disabled="TextField_Disabled"
       background_image_focused="TextField_Active"
		 text_pad_left="6"
		 tool_tip="Type the search term you are interested in here. Results will be displayed for partial fulltext matches within the menu."
		 top="1"/>
      </layout_panel>
      <layout_panel
		visibility_control="ShowFPSCounter"
		follows="top|left|right"
		auto_resize="false"
		mouse_opaque="false"
		layout="topleft"
		height="24"
		name="fps_counter_layout"
		width="40"/>
		</layout_stack>
	  </layout_panel>
   </layout_stack>
</panel>
