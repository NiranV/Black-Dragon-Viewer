		 <layout_panel
			follows="top|left|right"
			auto_resize="true"
			layout="topleft"
			height="23"
           name="navigation_layout_panel"
           width="272">
		 </layout_panel>
		 <layout_panel
			visibility_control="ShowTopSearchBar"
			follows="top|left|right"
			auto_resize="false"
			layout="topleft"
			visible="false"
			height="30"
           name="navigation_layout_panel"
           width="160">
									
									<search_combo_box
			follows="left|top"
			height="23"
			label="Search"
			layout="topleft"
			left="3"
			mouse_opaque="false"
			name="search_combo_box"
			tool_tip="Search for something in Second Life"
			top_pad="1"
			width="114" >
			<combo_editor
			label="Search"
			text_pad_right="8" 
			name="search_combo_editor"/>
			<combo_list
			draw_border="false"
			border.highlight_light_color="FocusColor"
			border.highlight_dark_color="FocusColor"
			border.shadow_light_color="FocusColor"
			border.shadow_dark_color="FocusColor"
			mouse_wheel_opaque="true" />
			</search_combo_box>
		 </layout_panel>
		 <layout_panel
			follows="top|left|right"
			visibility_control="ShowBalance"
			auto_resize="false"
			layout="topleft"
			height="23"
           name="navigation_layout_panel"
           width="63"/>
		 <layout_panel
			follows="top|left|right"
			visibility_control="ShowBalance"
			auto_resize="false"
			layout="topleft"
			height="23"
           name="navigation_layout_panel"
           width="205"/>
			</layout_stack>
         </panel>
        </layout_panel>
		 <layout_panel
			follows="top|left|right"
			auto_resize="false"
			layout="topleft"
			invisibility_control="HideTopbar"
			height="32"
			min_height="32"
           name="navigation_layout_panel"
           width="272">
			<icon
	 follows="top|right|left"
	 image_name="Window_NoTitle_Foreground"
	 invisibility_control="HideTopbar"
	 mouse_opaque="false"
	 name="bg_icon_l1"
	 scale_image="false"
	 visible="true"
	 left="2"
	 top_pad="-1"
	 height="30"
	 width="268"/>
	        <location_input
                       follows="top|left|right"
	         halign="right"
	         height="23"
	         label="Location"
	         layout="topleft"
	         left="5"
			 right="-28"
	         max_chars="254"
	         mouse_opaque="false"
	         name="location_combo"
	         top="3"
                width="266">
             <combo_list
             mouse_wheel_opaque="true"/>
	        </location_input>
			<button
					control_name="HideTopbar"
			image_overlay="Toast_CloseBtn"
			image_selected="Transparent.j2c"
			image_unselected="Transparent.j2c"
			follows="right|top"
			height="20"
			layout="topleft"
			right="-8"
			is_toggle="true"
			name="Close"
			label=""
			top="5"
			width="20"/>
		 </layout_panel>
         <layout_panel
           follows="top|left|right"
	         layout="topleft"
										height="18"
										min_width="18"
										auto_resize="false"
           name="favorites_layout_panel"
		   visible="true"
           width="320">

           <favorites_bar
             follows="left|right|top"
             font="SansSerifSmall"
			 invisibility_control="HideTopbar"
             height="15"
             layout="topleft"
             left="5"
             top="0"
             name="favorite"
             image_drag_indication="Accordion_ArrowOpened_Off"
             tool_tip="Drag Landmarks here for quick access to your favorite places in Second Life!"
             width="320">
            <label
             follows="left|top"
             height="13"
             layout="topleft"
             left="0"
             name="favorites_bar_label"
             text_color="LtGray"
             tool_tip="Drag Landmarks here for quick access to your favorite places in Second Life!"
             top="13"
	     valign="bottom"
             width="102">
              Favorites Bar
			</label>
              <!-- More button actually is a text box. -->
              <more_button
                follows="left|bottom"
                name=">>"
                tab_stop="false"
                tool_tip="Show more of My Favorites"
                top="13"
                width="50"
                bottom="0"
                valign="bottom">
                More &#9660;
                </more_button>
         </favorites_bar>
         </layout_panel>
     
     </layout_stack>
</panel>
