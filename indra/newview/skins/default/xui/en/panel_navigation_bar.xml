background_visible="false"
follows="left|top|right"
focus_root="true" 
height="51"
layout="topleft"
name="navigation_bar"
mouse_opaque="false"
chrome="true"
width="800">
   <layout_stack
   follows="top|left|right"
   height="45"
   layout="topleft"
   border_size="-5"
   left="0"
   mouse_opaque="false"
   open_time_constant="0.11"
   close_time_constant="0.11"
   name="nvp_stack"
   orientation="vertical"
   top="0"
   width="800">
	  <layout_panel
	  follows="top|left|right"
	  layout="topleft"
	  mouse_opaque="false"
	  height="45"
	  min_width="24"
	  auto_resize="true"
	  name="favorites_layout_panel"
	  width="800">
		 <icon
		 follows="all"
		 image_name="Window_Background"
		 mouse_opaque="false"
		 name="bg_icon_l1"
		 left_pad="-801"
		 top_pad="-1"
		 height="45"
		 width="802"/>
	  </layout_panel>
	  <layout_panel
	  follows="top|left|right"
	  layout="topleft"
	  mouse_opaque="false"
	  height="23"
	  min_width="23"
	  auto_resize="false"
	  invisibility_control="ShowNavbarFavoritesPanel"
	  name="favorites_layout_panel"
	  width="800"/>
   </layout_stack>
   <layout_stack
   follows="top|left|right"
   height="46"
   layout="topleft"
   border_size="0"
   left="0"
   mouse_opaque="false"
   open_time_constant="0.11"
   close_time_constant="0.11"
   name="nvp_stack"
   orientation="vertical"
   top="0"
   width="800">
