<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 background_opaque="false"
 background_visible="false"
 follows="left|top|right"
 focus_root="true" 
 height="45"
 layout="topleft"
 name="navigation_bar"
 chrome="true"
 width="800">
	<icon
	 follows="top|left"
	 image_name="navbar/NavBar_BG_l1.png"
	 mouse_opaque="false"
	 name="bg_icon_l1"
	 scale_image="false"
	 visible="true"
	 left="0"
	 top="0"
	 height="50"
	 width="1920"/>
	 <layout_stack
       follows="top|left|right"
       height="50"
       layout="topleft"
       border_size="-5"
       left="0"
       mouse_opaque="false"
       name="nvp_stack"
       orientation="vertical"
       top="0"
       width="800">
         <layout_panel
			follows="top|left|right"
			auto_resize="false"
			layout="topleft"
			height="32"
			min_height="32"
           name="navigation_layout_panel"
           width="800">
	        <panel
	         follows="left|top|right"
	         top="0"
	         height="27"
	         layout="topleft"
          left="0"
	         name="navigation_panel"
          width="800">
										<view_border
						bevel_style="in"
						follows="top|left"
						height="20"
						layout="topleft"
						left="57"
						name="border1"
						top="4"
						width="1"/>
			<layout_stack
       follows="top|left|right"
       height="30"
       layout="topleft"
       border_size="-5"
       left="60"
       mouse_opaque="false"
       name="nvp_stack"
       orientation="horizontal"
       top="3"
	   right="-100"
       width="800">
         <layout_panel
			follows="top|left|right"
			auto_resize="false"
			layout="topleft"
			height="23"
           name="navigation_layout_panel"
           width="42">
	         <pull_button
	         follows="left|top"
	         direction="down"
	         height="23"
	         image_overlay="Arrow_Left_Off"
										image_selected="transparent.j2c"
										image_unselected="transparent.j2c"
	         image_bottom_pad="1"
	         layout="topleft"
	         left="0"
	         name="back_btn"
	         tool_tip="Go back to previous location"
	         top="0"
	         width="20" />
	        <pull_button
	         follows="left|top"
	         direction="down"
	         height="23"
	         image_overlay="Arrow_Right_Off"
										image_selected="transparent.j2c"
										image_unselected="transparent.j2c"
	         image_bottom_pad="1"
	         layout="topleft"
	         left_pad="-3"
	         name="forward_btn"
	         tool_tip="Go forward one location"
	         top="0"
	         width="20" />
		 </layout_panel>
		 <layout_panel
			visibility_control="ShowQuickHomeButton"
			follows="top|left|right"
			auto_resize="false"
			layout="topleft"
			height="23"
           name="navigation_layout_panel"
           width="40">
									
									<view_border
						bevel_style="in"
						follows="top|left"
						height="20"
						layout="topleft"
						left="1"
						name="border1"
						top="1"
						width="1"/>
	        <button
	         follows="left|top"
	         height="23"
	         image_bottom_pad="1"
	         image_overlay="Home_Off"
										image_selected="transparent.j2c"
										image_unselected="transparent.j2c"
	         layout="topleft"
	         left_pad="3"
	         name="home_btn"
	         tool_tip="Teleport to my home location"
	         top_delta="-1"
	         width="28" />
		 </layout_panel>
		 <layout_panel
			follows="top|left|right"
			auto_resize="false"
			layout="topleft"
			height="23"
           name="navigation_layout_panel"
           width="10">
									
		 <view_border
						bevel_style="in"
						follows="top|left"
						height="20"
						layout="topleft"
						left="1"
						name="border1"
						top="1"
						width="1"/>
		 </layout_panel>
		 <layout_panel
					   visibility_control="ShowQuickWindlightButton"
			follows="top|left|right"
			auto_resize="false"
			layout="topleft"
			height="23"
           name="navigation_layout_panel"
           width="31">
			<button
			image_overlay="Windlightbutton.tga"
			image_selected="Transparent.j2c"
			image_unselected="Transparent.j2c"
			follows="left|top"
			height="23"
			layout="topleft"
			left="0"
			name="Windlight"
			label=""
			tool_tip="Opens Windlight Floater"
			top_pad="-1"
			width="24">
			<button.init_callback
			      function="Button.SetFloaterToggle"
			      parameter="env_settings" />
			</button>
		 </layout_panel>
		 <layout_panel
					   visibility_control="ShowQuickSkyButton"
			follows="top|left|right"
			auto_resize="false"
			layout="topleft"
			height="23"
           name="navigation_layout_panel"
           width="30">
			<button
			image_overlay="Skybutton.tga"
			image_selected="Transparent.j2c"
			image_unselected="Transparent.j2c"
			halign="center"
			follows="left|top"
			height="23"
			layout="topleft"
			left="0"
			name="Sky"
			label=""
			tool_tip="Opens Sky Settings Floater"
			top_pad="-1"
			width="24">
			<button.init_callback
			      function="Button.SetFloaterToggle"
			      parameter="env_edit_sky" />
			</button>
		 </layout_panel>
		 <layout_panel
					   visibility_control="ShowQuickWaterButton"
			follows="top|left|right"
			auto_resize="false"
			layout="topleft"
			height="23"
           name="navigation_layout_panel"
           width="25">
			<button
			image_overlay="Waterbutton.tga"
			image_selected="Transparent.j2c"
			image_unselected="Transparent.j2c"
			follows="left|top"
			height="23"
			layout="topleft"
			left="0"
			name="Water"
			label=""
			tool_tip="Opens Water Settings Floater"
			top_pad="-1"
			width="24">
			<button.init_callback
			      function="Button.SetFloaterToggle"
			      parameter="env_edit_water" />
			</button>
		 </layout_panel>
		 <layout_panel
					   visibility_control="ShowQuickBrowserButton"
			follows="top|left|right"
			auto_resize="false"
			layout="topleft"
			height="23"
           name="navigation_layout_panel"
           width="44">
			<button
			image_overlay="icons\ExternalBrowser_Off.png"
			image_selected="Transparent.j2c"
			image_unselected="Transparent.j2c"
			follows="left|top"
			height="23"
			layout="topleft"
			left="14"
			name="Browser"
			label=""
			tool_tip="Opens a new Browser Window"
			top_pad="-1"
			width="24">
			<button.commit_callback
			      function="Advanced.WebContentTest"
			      parameter="www.google.com" />
			</button>
		 </layout_panel>
		 <layout_panel
			follows="top|left|right"
			auto_resize="false"
			layout="topleft"
			height="23"
           name="navigation_layout_panel"
           width="10">
									
		 <view_border
						bevel_style="in"
						follows="top|left"
						height="20"
						layout="topleft"
						left="4"
						name="border1"
						top="1"
						width="1"/>
		 </layout_panel>
		 <layout_panel
		  follows="left"
		  visibility_control="ShowQuickAboutLandButton"
		  auto_resize="false"
		  height="23"
		  left_pad="3"
		  layout="topleft"
		  mouse_opaque="false"
		  name="about_land_panel"
		  user_resize="false"
		  width="34">
			<button
			image_overlay="toolbar_icons\land.png"
			image_selected="transparent.j2c"
			image_unselected="transparent.j2c"
			image_overlay_color="1 1 1 0.6"
			follows="left|top"
			height="23"
			layout="topleft"
			left="7"
			name="About_Land"
			label="" 
			tool_tip="Show information about the location you are in"
			top_pad="-1"
			width="24">
			<button.init_callback
			      function="Button.SetFloaterToggle"
			      parameter="about_land" />
			</button>
		  </layout_panel>
		 <layout_panel
			follows="top|left|right"
			auto_resize="true"
			layout="topleft"
			height="23"
           name="navigation_layout_panel"
           width="272">
	        <location_input
                       follows="top|left|right"
	         halign="right"
	         height="23"
	         label="Location"
	         layout="topleft"
	         left="7"
	         max_chars="254"
	         mouse_opaque="false"
	         name="location_combo"
	         top="0"
                width="260">
             <combo_list
             mouse_wheel_opaque="true"/>
	        </location_input>
		 </layout_panel>
		 <layout_panel
					   visibility_control="ShowTopSearchBar"
			follows="top|left|right"
			auto_resize="false"
			layout="topleft"
			height="23"
           name="navigation_layout_panel"
           width="120">
									
									<search_combo_box
			follows="left|top"
			height="23"
			label="Search"
			layout="topleft"
			left="3"
			mouse_opaque="false"
			name="search_combo_box"
			tool_tip="Search for something in Second Life"
			top="0"
			width="114" >
			<combo_editor
			label="Search"
			text_pad_right="8" 
			name="search_combo_editor"/>
			<combo_list
			draw_border="true"
			border.highlight_light_color="FocusColor"
			border.highlight_dark_color="FocusColor"
			border.shadow_light_color="FocusColor"
			border.shadow_dark_color="FocusColor"
			mouse_wheel_opaque="true" />
			</search_combo_box>
		 </layout_panel>
			</layout_stack>
         </panel>
        </layout_panel>
         <layout_panel
           follows="top|left|right"
	         layout="topleft"
										height="18"
										min_width="18"
										auto_resize="false"
           name="favorites_layout_panel"
           width="320">

           <favorites_bar
             follows="left|right|top"
             font="SansSerifSmall"
             height="15"
             layout="topleft"
             left="0"
             top="0"
             name="favorite"
             image_drag_indication="Accordion_ArrowOpened_Off"
             tool_tip="Drag Landmarks here for quick access to your favorite places in Second Life!"
             width="320">
            <label
             follows="left|top"
             height="13"
             layout="topleft"
             left="10"
             name="favorites_bar_label"
             text_color="LtGray"
             tool_tip="Drag Landmarks here for quick access to your favorite places in Second Life!"
             top="13"
	     valign="bottom"
             width="102">
              Favorites Bar
            </label>
              <!-- More button actually is a text box. -->
              <more_button
                follows="left|bottom"
                name=">>"
                tab_stop="false"
                tool_tip="Show more of My Favorites"
                top="13"
                width="50"
                bottom="0"
                valign="bottom">
                More &#9660;
                </more_button>
         </favorites_bar>
         </layout_panel>
     
     </layout_stack>
</panel>
