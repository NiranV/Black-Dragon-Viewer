 layout="topleft"
 left="8"
 name="wearable_type_panel"
 top_delta="0"
 width="313">
  <icon
  follows="top|left|right"
  height="25"
  name="bg"
  mouse_opaque="false"
  image_name="Toolbar_Middle_Off"
  left="0"
  width="313"
  top_pad="2" />
  <line_editor
  follows="all"
  height="22"
  layout="topleft"
  left="2"
  max_length_bytes="63"
  name="description"
  prevalidate_callback="ascii"
  select_on_focus="true"
  text_color="LtGray"
  top_delta="1"
  width="309" />
  <radio_group
  control_name="AvatarSex"
  follows="top|left"
  height="14"
  layout="topleft"
  left="0"
  name="sex_radio"
  top_pad="6"
  width="70">
  <radio_item
  follows="top|left"
  height="14"
  layout="topleft"
  left="0"
  top="0"
  label="Male"
  name="sex_male"
  tool_tip="Male"
  value="1"
  width="50">
  <radio_item.check_button
  image_unselected="Checkbox_Off"
  image_selected="Checkbox_On"
  image_disabled="Checkbox_Off_Disabled"
  image_disabled_selected="Checkbox_On_Disabled"
  image_pressed="Checkbox_Press"
  image_pressed_selected="Checkbox_On_Press" />
  </radio_item>
  <radio_item
  follows="top|left"
  height="14"
  layout="topleft"
  left_pad="2"
  top_delta="0"
  label="Female"
  name="sex_female"
  tool_tip="Female"
  value="0"
  width="50">
  <radio_item.check_button
  image_unselected="Checkbox_Off"
  image_selected="Checkbox_On"
  image_disabled="Checkbox_Off_Disabled"
  image_disabled_selected="Checkbox_On_Disabled"
  image_pressed="Checkbox_Press"
  image_pressed_selected="Checkbox_On_Press" />
  </radio_item>
  </radio_group>
  <!--  graphical labels for the radio buttons above -->
 </panel>
