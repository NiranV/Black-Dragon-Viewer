  halign="left"
  layout="topleft"
  label="  Save"
  left="0"
  name="back_btn"
  tool_tip="Return to Edit Outfit"
  top="0"
  pad_left="7"
  width="31"
  use_draw_context_alpha="false" />
  <icon
  follows="top|left|right"
  height="25"
  name="title_bg"
  mouse_opaque="false"
  image_name="Toolbar_Middle_Off"
  left_pad="1"
  right="-1"
  top_delta="0" />
  <text
  follows="top|left|right"
  font="SansSerif"
  height="22"
  layout="topleft"
  left_delta="3"
  name="edit_wearable_title"
  text_color="LtGray"
  top="5"
  value="Editing Shape"
  use_ellipses="true"
  width="273" />
 </panel>
