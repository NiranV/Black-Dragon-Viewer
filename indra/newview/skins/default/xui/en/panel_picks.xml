<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
background_visible="false"
background_opaque="true"
follows="all"
height="571"
label="Picks"
layout="topleft"
left="7"
name="panel_picks"
top_pad="0"
width="313">
 <string
 name="no_picks"
 value="No Picks" />
 <string
 name="no_classifieds"
 value="No Classifieds" />
 <tab_container
 follows="all"
 halign="center"
 height="565"
 layout="topleft"
 left="0"
 name="Picks Tabs"
 tab_min_width="154"
 tab_max_width="154"
 tab_height="20"
 tab_group="1"
 tab_position="top"
 top="0"
 width="319">
  <panel
  follows="all"
  bg_alpha_image="Panel_Background"
  background_visible="true"
  height="539"
  layout="topleft"
  left="0"
  title="Picks"
  name="tab_picks"
  top="0"
  width="319">
  <flat_list_view
  color="Transparent"
  follows="all"
  layout="topleft"
  left="0"
  name="picks_list"
  visible="true"
  top="2"
  height="510"
  width="317" />
 </panel>
 <panel
 follows="all"
 bg_alpha_image="Panel_Background"
 background_visible="true"
 height="539"
 layout="topleft"
 left="0"
 title="Classifieds"
 name="tab_classifieds"
 top="0"
 width="319">
  <flat_list_view
  color="Transparent"
  follows="all"
  layout="topleft"
  left="0"
  name="classifieds_list"
  visible="true"
  top="2"
  height="510"
  width="317" />
  </panel>
 </tab_container>
 <panel
 bevel_style="none"
 enabled="false"
 follows="bottom|left|right"
 left="3"
 height="25"
 label="bottom_panel"
 layout="topleft"
 name="edit_panel"
 top_pad="-27"
 width="313">
  <layout_stack
  animate="false"
  border_size="0"
  follows="left|right|bottom"
  height="25"
  layout="topleft"
  orientation="horizontal"
  top_pad="0"
  left="0"
  name="bottom_panel"
  width="314">
   <layout_panel
   follows="bottom|left"
   height="25"
   auto_resize="false"
   layout="topleft"
   name="gear_menu_btn"
   width="33">
    <button
    follows="bottom|left"
    height="25"
    image_selected="Toolbar_Middle_Selected"
    image_unselected="Toolbar_Middle_Off"
    image_overlay="AddItem_Off"
    layout="topleft"
    left="1"
    name="new_btn"
    tool_tip="Create a new pick or classified at the current location"
    top="0"
    width="31" />
   </layout_panel>
   <layout_panel
   follows="top|left|right"
   height="25"
   layout="bottomleft" 
   name="teleport_btn_lp"
   auto_resize="true"
   width="79">
    <button
    enabled="false"
    follows="top|left|right"
    height="25"
    label="Teleport"
    image_hover_unselected="Toolbar_Middle_Over"
    image_selected="Toolbar_Middle_Selected"
    image_unselected="Toolbar_Middle_Off"
    layout="topleft"
    name="teleport_btn"
    top="0"
    tab_stop="false"
    tool_tip="Teleport to the corresponding area"
    width="78" />
   </layout_panel>
   <layout_panel
   follows="left|right"
   height="25"
   layout="bottomleft"
   name="show_on_map_btn_lp"
   auto_resize="false" 
   width="104">
    <button
    enabled="false"
    follows="top|left"
    height="25"
    label="Show on Map"
    image_hover_unselected="Toolbar_Middle_Over"
    image_selected="Toolbar_Middle_Selected"
    image_unselected="Toolbar_Middle_Off"
    layout="topleft"
    top="0"
    name="show_on_map_btn"
    tab_stop="false"
    tool_tip="Show the corresponding area on the World Map"
    width="103" />
   </layout_panel>
   <layout_panel
   follows="bottom|left"
   height="25"
   layout="topleft"
   auto_resize="false"
   name="trash_btn_lp"
   width="32">
    <button
    follows="bottom|left"
    height="25"
    image_overlay="TrashItem_Off"
    image_selected="Toolbar_Middle_Selected"
    image_unselected="Toolbar_Middle_Off"
    layout="topleft"
    name="trash_btn"
    top="0"
    left="0"
    width="31" />
   </layout_panel>
  </layout_stack>
 </panel>
 <text
 type="string"
 follows="all"
 height="490"
 layout="topleft"
 left="6"
 name="picks_panel_text"
 wrap="true"
 top="20"
 width="313"/>
</panel>
