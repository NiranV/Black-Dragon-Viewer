<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 bg_opaque_color="DkGray2"
 background_visible="false"
 background_opaque="true"
 follows="all"
 height="571"
 label="Picks"
 layout="topleft"
 left="8"
 name="panel_picks"
 top_pad="0"
 width="313">
 <string
  name="no_picks"
  value="No Picks" />
 <string
  name="no_classifieds"
  value="No Classifieds" />
  <tab_container
  follows="all"
  halign="center"
  height="536"
  layout="topleft"
  left="0"
  name="Picks Tabs"
  tab_min_width="150"
  tab_max_width="157"
  tab_height="20"
  tab_group="1"
  tab_position="top"
  top="0"
  width="315">
    <panel
    follows="all"
    height="513"
    layout="topleft"
    left="0"
    title="Picks"
    name="tab_picks"
    top="0"
    width="315">
      <flat_list_view
      color="White_02"
      follows="all"
      layout="topleft"
      left="0"
      name="picks_list"
      visible="true"
      top="1"
      height="513"
      width="313" />
    </panel>
    <panel
    follows="all"
    height="513"
    layout="topleft"
    left="0"
    title="Classifieds"
    name="tab_classifieds"
    top="0"
    width="315">
      <flat_list_view
      color="White_02"
      follows="all"
      layout="topleft"
      left="0"
      name="classifieds_list"
      visible="true"
      top="0"
      height="513"
      width="313" />
    </panel>
    </tab_container>
  <!--<text
  type="string"
  follows="all"
  height="490"
  layout="topleft"
  left="6"
  name="picks_panel_text"
  wrap="true"
  top="20"
  width="313"/> -->
    <panel
    bg_opaque_color="White_02"
    background_visible="true"
    background_opaque="true"
    bevel_style="none"
    enabled="false"
    follows="bottom|left|right"
    left="2"
    height="27"
    label="bottom_panel"
    layout="topleft"
    name="edit_panel"
    top_pad="0"
    width="312">
      <layout_stack
      animate="false"
      border_size="0"
      follows="left|right|bottom"
      height="25"
      layout="topleft"
      orientation="horizontal"
      top_pad="1"
      left="0"
      name="bottom_panel"
      width="312">
	<layout_panel
	follows="bottom|left"
	height="25"
	auto_resize="false"
	layout="topleft"
	name="gear_menu_btn"
	width="33">
	 <button
	 follows="bottom|left"
	 height="25"
	 image_selected="Toolbar_Middle_Selected"
	 image_unselected="Toolbar_Middle_Off"
	 image_overlay="AddItem_Off"
	 layout="topleft"
	 left="1"
	 name="new_btn"
	 tool_tip="Create a new pick or classified at the current location"
	 top="0"
	 width="31" />
      </layout_panel>
    <layout_panel
			  follows="left|right"
			  height="25"
			  layout="topleft"
			  name="info_btn_lp"
		      auto_resize="false"
			  width="32">
		       <button
		         enabled="false"
		         follows="top|left"
                 image_overlay="Command_HowTo_Icon"
                 image_hover_unselected="Toolbar_Middle_Over"
                 image_selected="Toolbar_Middle_Selected"
                 image_unselected="Toolbar_Middle_Off"
		         height="25"
		         layout="topleft"
                 top="0"
		         name="info_btn"
		         tab_stop="false"
		         tool_tip="Show pick information"
		         width="31" />
		  </layout_panel>
		  
		  <layout_panel
			  follows="top|left|right"
			  height="25"
			  layout="bottomleft" 
			  name="teleport_btn_lp"
		      auto_resize="true"
			  width="117">
		        <button
		         enabled="false"
		         follows="top|left|right"
		         height="25"
		         label="Teleport"
                 image_hover_unselected="Toolbar_Middle_Over"
                 image_selected="Toolbar_Middle_Selected"
                 image_unselected="Toolbar_Middle_Off"
		         layout="topleft"
		         name="teleport_btn"
                 top="0"
		         tab_stop="false"
		         tool_tip="Teleport to the corresponding area"
		         width="116" />
		  </layout_panel>
		  
		  <layout_panel
			  follows="left|right"
			  height="25"
			  layout="bottomleft"
			  name="show_on_map_btn_lp"
		      auto_resize="false" 
			  width="32">
		        <button
		         enabled="false"
		         follows="top|left"
		         height="25"
                 image_overlay="Command_Map_Icon"
                 image_hover_unselected="Toolbar_Middle_Over"
                 image_selected="Toolbar_Middle_Selected"
                 image_unselected="Toolbar_Middle_Off"
		         layout="topleft"
                 top="0"
		         name="show_on_map_btn"
		         tab_stop="false"
		         tool_tip="Show the corresponding area on the World Map"
		         width="31" />
		  </layout_panel>
      <layout_panel
      follows="bottom|left"
      height="25"
      layout="topleft"
      auto_resize="false"
      name="trash_btn_lp"
      width="32">
	<button
	follows="bottom|left"
	height="25"
	image_overlay="TrashItem_Off"
	image_selected="Toolbar_Middle_Selected"
	image_unselected="Toolbar_Middle_Off"
	layout="topleft"
	name="trash_btn"
	top="0"
	left="0"
	width="31" />
      </layout_panel>
      </layout_stack>
      </panel>
</panel>
