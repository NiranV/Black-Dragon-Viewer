<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 bg_opaque_color="DkGray2"
 background_visible="false"
 background_opaque="true"
 follows="all"
 height="571"
 label="Picks"
 layout="topleft"
 left="8"
 name="panel_picks"
 top_pad="0"
 width="313">
 <string
  name="no_picks"
  value="No Picks" />
 <string
  name="no_classifieds"
  value="No Classifieds" />
  <tab_container
  follows="all"
  halign="center"
  height="513"
  layout="topleft"
  left="0"
  name="Picks Tabs"
  tab_min_width="150"
  tab_max_width="157"
  tab_height="20"
  tab_group="1"
  tab_position="top"
  top="0"
  width="315">
    <panel
    follows="all"
    height="513"
    layout="topleft"
    left="0"
    title="Picks"
    name="tab_picks"
    top="0"
    width="315">
      <flat_list_view
      color="Black_50"
      follows="all"
      layout="topleft"
      left="0"
      name="picks_list"
      visible="true"
      top="0"
      height="513"
      width="313" />
    </panel>
    <panel
    follows="all"
    height="513"
    layout="topleft"
    left="0"
    title="Classifieds"
    name="tab_classifieds"
    top="0"
    width="315">
      <flat_list_view
      color="Black_50"
      follows="all"
      layout="topleft"
      left="0"
      name="classifieds_list"
      visible="true"
      top="0"
      height="513"
      width="313" />
    </panel>
    </tab_container>
