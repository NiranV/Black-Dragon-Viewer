<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<toolbar_view
follows="all"
layout="topleft"
name="toolbar view"
height="500"
width="1024"
left="0"
top="0" 
mouse_opaque="false"
tab_stop="false"
visible="true">
	<layout_stack
	name="main"
	orientation="vertical"
	left="0"
	top="0"
	width="1024"
	height="500"
	follows="all"
	mouse_opaque="false">
		<layout_panel
		name="toolbar_panel"
		height="500"
		auto_resize="true"
		width="1024"
		mouse_opaque="false">
			<layout_stack
			name="main"
			orientation="horizontal"
			left="0"
			top="0"
			width="1024"
			height="500"
			follows="all"
			mouse_opaque="false">
				<layout_panel
				name="main_panel"
				height="474"
				width="1024"
				mouse_opaque="false">
					<layout_stack
					name="horizontal_toolbar_stack"
					orientation="vertical"
					left="0"
					top="0"
					width="1024"
					height="474"
					follows="all"
					mouse_opaque="false">
						<!--<layout_panel
						name="top_mini_location_space_panel"
						auto_resize="false"
						height="24"
						width="1024"
						mouse_opaque="false"/>-->
						<layout_panel
						name="top_toolbar_panel"
						auto_resize="false"
						height="30"
						width="1024"
						mouse_opaque="false">
							<toolbar
							layout="topleft"
							button_panel.bg_opaque_image="transparent.j2c"
							name="toolbar_top"
							height="30"
							left="1"
							width="1028"
							top="0"
							side="top"
							follows="left|top|right"
							button_layout_mode="center"
							button_display_mode="icons_small"
							visible="true">
								<icon layout="topleft"
								height="28"
								width="15"
								follows="left|top"
								top="20"
								left="10"
								tab_stop="false" 
								visible="false"
								use_draw_context_alpha="false"
								image_name="Caret_Top_Icon"
								name="caret" />
							</toolbar>
						</layout_panel>
						<layout_panel
						name="vertical_toolbar_panel"
						auto_resize="true"
						width="1024"
						height="474"
						mouse_opaque="false">
							<layout_stack
							name="vertical_toolbar_stack"
							orientation="horizontal"
							left="1"
							top="0"
							width="1024"
							height="474"
							follows="all"
							mouse_opaque="false">
								<layout_panel
								name="left_toolbar_panel"
								auto_resize="false"
								height="474"
								width="30"
								mouse_opaque="false">
									<toolbar
									follows="left|top|bottom"
									button_panel.bg_opaque_image="transparent.j2c"
									name="toolbar_left"
									width="30"
									left="0"
									top="0"
									height="479"
									side="left"
									button_layout_mode="center"
									button_display_mode="icons_only">
										<icon
										layout="topleft"
										height="15"
										width="28"
										follows="left|top"
										top="20"
										left="10"
										tab_stop="false" 
										visible="false"
										image_name="Caret_Left_Icon"
										name="caret" />
									</toolbar>
								</layout_panel>
								<layout_panel
								name="non_toolbar_panel"
								auto_resize="true"
								mouse_opaque="false"
								height="100"
								width="200">
									<panel
									follows="top|right"
									layout="topleft"
									height="35"
									left="0"
									mouse_opaque="false"
									name="chiclet_container"
									tab_stop="false"
									top="0"
									width="190"/>
									<view
									top="0"
									follows="all"
									height="100"
									left="0"
									mouse_opaque="false"
									name="floater_snap_region"
									width="200"/>
									<view
									top="0"
									follows="left|top|bottom"
									height="100"
									left="0"
									mouse_opaque="false"
									name="minimized_floaters_region"
									width="160"/>
									<panel
									bottom="100"
									visibility_control="ShowStatusButtonContainer"
									follows="left|right|bottom"
									height="23"
									left="0"
									mouse_opaque="false"
									tab_stop="false"
									name="state_management_buttons_container"
									visible="false"
									width="200"/>
								</layout_panel>
								<layout_panel
								name="right_toolbar_panel"
								auto_resize="false"
								height="474"
								width="30"
								mouse_opaque="false">
									<toolbar
									button_panel.bg_opaque_image="transparent.j2c"
									follows="right|top|bottom"
									name="toolbar_right"
									width="30"
									height="478"
									left="0"
									top="0"
									side="right"
									button_layout_mode="center"
									button_display_mode="icons_only">
										<icon
										layout="topleft"
										height="15"
										width="28"
										follows="left|top"
										top="20"
										left="10"
										tab_stop="false" 
										visible="false"
										image_name="Caret_Right_Icon"
										name="caret" />
									</toolbar>
								</layout_panel>
							</layout_stack>
						</layout_panel>
						<layout_panel
						name="bottom_toolbar_panel"
						auto_resize="false"
						height="30"
						width="1024"
						mouse_opaque="false">
							<toolbar
							layout="topleft"
							button_panel.bg_opaque_image="transparent.j2c"
							name="toolbar_bottom"
							height="30"
							left="1"
							width="1028"
							top="0"
							side="bottom"
							button_layout_mode="center"
							follows="left|right|bottom"
							button_display_mode="icons_with_text"
							visible="true">
								<icon layout="topleft"
								height="28"
								width="15"
								follows="left|top"
								top="20"
								left="10"
								tab_stop="false" 
								visible="false"
								use_draw_context_alpha="false"
								image_name="Caret_Bottom_Icon"
								name="caret" />
							</toolbar>
						</layout_panel>
					</layout_stack>
				</layout_panel>
				<layout_panel
				auto_resize="false"
				follows="right|top|bottom"
				height="474"
				layout="topleft"
				mouse_opaque="false"
				visible="false"
				tab_stop="false" 
				name="machinima_layout_panel"
				width="345"
				visibility_control="MachinimaSidebar">
					<panel
					top="0"
					follows="top|right|bottom"
					height="474"
					left="-345"
					mouse_opaque="false"
					name="machinima"
					width="345"/>
				</layout_panel>
			</layout_stack>
		</layout_panel>
	</layout_stack>
</toolbar_view>
