<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 bevel_style="none"
 border_style="line"
 follows="left|top|right|bottom"
 height="496"
 layout="topleft"
 left="0"
 top="5"
 name="script panel"
 width="497">
    <panel.string
     name="loading">
        Loading...
    </panel.string>
    <panel.string
     name="can_not_view">
        You can not view or edit this script, since it has been set as &quot;no copy&quot;. You need full permissions to view or edit a script inside an object.
    </panel.string>
    <panel.string
     name="public_objects_can_not_run">
        Public Objects cannot run scripts
    </panel.string>
    <panel.string
     name="script_running">
        Running
    </panel.string>
    <panel.string
     name="Title">
        Script: [NAME]
    </panel.string>
    <panel.string
     name="external_editor_not_set">
        Select an editor by setting the environment variable LL_SCRIPT_EDITOR or the ExternalEditor setting.
    </panel.string>
    <menu_bar
     bg_visible="false"
     follows="left|top"
     height="18"
     layout="topleft"
     left="0"
     top="10"
     mouse_opaque="false"
     name="script_menu"
     width="476">
