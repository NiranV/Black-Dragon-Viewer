     background_image="TextField_Off3"
     background_image_disabled="TextField_Disabled3"
     background_image_focused="TextField_Active3"
     follows="right|top"
     text_pad_left="5"
     height="12"
     layout="topleft"
     left_pad="22"
     halign="left"
     top_delta="0"
     name="col_entry"
     value="0"
     width="60" />
    <text_editor
     left="0"
     type="string"
     length="1"
     max_length="65536"
     follows="left|top|right|bottom"
     font="Monospace"
     height="375"
     ignore_tab="false"
     layout="topleft"
     name="Script Editor"
     bg_writeable_color="0.13 0.13 0.13 0.75"
     bg_focus_color="0.13 0.13 0.13 0.75"
     bg_readonly_color="0.09 0.09 0.09 0.75"
     bg_selected_color="EmphasisColor_13"
     default_color="0.7 0.7 0.7 1"
     text_color="0.7 0.7 0.7 1"
     cursor_color="White"
     text_selected_color="0.1 0.1 0.1 1"
     text_readonly_color="0.7 0.7 0.7 1"
     width="487"
     top_pad="1"
     show_line_numbers="true" 
     enable_tooltip_paste="true"
     word_wrap="true">
        Loading...
    </text_editor>
    <scroll_list
     top_pad="5"
     left="0"
     follows="left|right|bottom"
     height="60"
     layout="topleft"
     name="lsl errors"
     width="487" />
    
    <combo_box
     follows="left|bottom"
     height="23"
     label="Insert..."
     layout="topleft"
     name="Insert..."
     width="128" />
    <button
     follows="right|bottom"
     height="23"
     label="Save"
     label_selected="Save"
     layout="topleft"
     top_delta="0"
     right="487"
     name="Save_btn"
     width="81" />
    <button
     enabled="false"
     follows="right|bottom"
     height="23"
     label="Edit..."
     layout="topleft"
     top_pad="-23"
     right="400"
     name="Edit_btn"
     width="81" />
