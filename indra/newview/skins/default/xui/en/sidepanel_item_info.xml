<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
  follows="all"
  height="500"
  layout="topleft"
  name="item properties"
  help_topic="item_properties"
  title="Item Profile"
  width="312">
  <panel.string
	name="loading_experience">
    (loading)
  </panel.string>
  <panel.string
    name="unknown">
    (unknown)
  </panel.string>
  <panel.string
    name="unknown_multiple">
    (unknown / multiple)
  </panel.string>
  <panel.string
    name="public">
    (public)
  </panel.string>
  <panel.string
    name="you_can">
    You can:
  </panel.string>
  <panel.string
    name="owner_can">
    Owner can:
  </panel.string>
  <panel.string
    name="acquiredDate">
    [wkday,datetime,local] [mth,datetime,local] [day,datetime,local] [hour,datetime,local]:[min,datetime,local]:[second,datetime,local] [year,datetime,local]
  </panel.string>
  <panel.string
    name="origin_inventory">
    (Inventory)
  </panel.string>
  <panel.string
    name="origin_inworld">
    (Inworld)
  </panel.string>
  <icon
  follows="all"
  height="300"
  color="1 1 1 1"
  image_name="Panel_Background"
  layout="topleft"
  name="vicon"
  mouse_opaque="false"
  visible="true"
  width="281"
  top="34"
  bottom="-5"
  left="8"/>
  <icon
    follows="top|right"
    height="18"
    image_name="Lock"
    layout="topleft"
    left="260"
    mouse_opaque="true"
    name="IconLocked"
    top="7"
    width="18" />
  <button
    follows="top|left"
    height="25"
    image_hover_unselected="Toolbar_Middle_Over"
       image_overlay="BackArrow_Off"
       image_selected="Toolbar_Middle_Selected"
       image_unselected="Toolbar_Middle_Off"
    layout="topleft"
    left="9"
    name="back_btn"
    tab_stop="false"
    top="0"
    width="31"
    use_draw_context_alpha="false" />
  <icon
      follows="top|left|right"
      height="25"
      name="bg"
	  layout="topleft"
      mouse_opaque="false"
      image_name="Toolbar_Middle_Off"
      left_pad="1"
      top_delta="0"
      width="246" />
  <text
    follows="top|left|right"
    height="11"
    layout="topleft"
    left_delta="3"
    name="title"
    top="0"
    use_ellipses="true"
    value="Item Profile"
    width="250" />
  <text
    follows="top|left|right"
    height="13"
    layout="topleft"
    left_delta="0"
    top_pad="0"
    name="origin"
    text_color="Friend"
    use_ellipses="true"
    value="(Inventory)"
    width="150" />
    <panel
      follows="left|top|bottom"
      height="490"
      label=""
      layout="topleft"
      left="10"
      name="item_profile"
      top_pad="10"
      width="295">
      <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="LabelItemNameTitle"
        top="5"
        width="90">
        Name:
      </text>
      <line_editor
        border_style="line"
        border_thickness="1"
        follows="left|top|right"
        height="19"
        layout="topleft"
        left_pad="0"
        max_length_bytes="63"
        name="LabelItemName"
        top_delta="0"
        width="197" />
        tool_tip="The name is limited to 63 characters. Longer prim names are cut short. Names can only consist of printable characters found in the ASCII-7 (non-extended) character set, with the exception of the vertical bar/pipe &apos;|&apos;." />
      <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="LabelItemDescTitle"
        top_pad="3"
        width="90">
        Description:
      </text>
      <line_editor
        border_style="line"
        border_thickness="1"
        follows="left|top|right"
        height="19"
        layout="topleft"
        left_pad="0"
        max_length_bytes="127"
        name="LabelItemDesc"
        top_delta="0"
        width="197" />
        tool_tip="When people have &apos;Hover Tips on All Objects&apos; selected in the viewer's settings, they'll see the object description pop-up for any object under their mouse pointer. The prim description is limited to 127 bytes any string longer then that will be truncated." />
      <text
        type="string"
        length="1"
        follows="left|top"
        height="29"
        layout="topleft"
        left="5"
        name="LabelCreatorTitle"
        top_pad="3"
        width="90">
        Creator:
      </text>
      <text
        type="string"
        follows="left|right|top"
        font="SansSerifSmall"
        height="29"
        layout="topleft"
        left_pad="0"
        name="LabelCreatorName"
        top_delta="0"
        translate="false"
        use_ellipses="true"
        width="195">
        TestString PleaseIgnore
      </text>
      <text
        type="string"
        length="1"
        follows="left|top"
        height="29"
        layout="topleft"
        left="5"
        name="LabelOwnerTitle"
        top_pad="3"
        width="90">
        Owner:
      </text>
      <text
        type="string"
        follows="left|right|top"
        font="SansSerifSmall"
        height="29"
        layout="topleft"
        left_pad="0"
        name="LabelOwnerName"
        top_delta="0"
        translate="false"
        use_ellipses="true"
        width="195">
        TestString PleaseIgnore
      </text>
      <text
        type="string"
        length="1"
        follows="left|top"
        height="29"
        layout="topleft"
        left="5"
        name="LabelAcquiredTitle"
        top_pad="3"
        width="90">
        Acquired:
      </text>
      <text
        type="string"
        length="1"
        follows="left|top|right"
        height="29"
        layout="topleft"
        left_pad="0"
        name="LabelAcquiredDate"
        top_delta="0"
        width="195">
        99.99.9999:99:99
      </text>
      <text
        type="string"
        length="1"
        follows="left|top"
        height="29"
        layout="topleft"
        left="5"
        name="LabelItemExperienceTitle"
        top_pad="3"
        width="90"
        visible="true">
        Experience:
      </text>
      <text
        type="string"
        length="1"
        follows="left|top|right"
        height="29"
        layout="topleft"
        left_pad="0"
        name="LabelItemExperience"
        top_delta="0"
        width="195"
        visible="true">
        (None)
        </text>
      <text
          type="string"
          length="1"
          follows="left|top"
          height="29"
          layout="topleft"
          left="5"
          name="GroupLabel"
          top_pad="3"
          width="90">
          Group:
        </text>
        <check_box
          height="18"
          label="Share"
          layout="topleft"
          left_pad="0"
          top_delta="-2"
          name="CheckShareWithGroup"
          tool_tip="Allow all members of the set group to share your modify permissions for this object. You must Deed to enable role restrictions."
          width="150" />
        <view_border
			bevel_style="none"
			follows="top|left"
			height="0"
			layout="topleft"
			left="5"
			name="cost_text_border"
			top_pad="3"
			width="285"/>
        <check_box
        height="20"
        label="For Sale"
        layout="topleft"
        left="5"
        name="CheckPurchase"
        top_pad="4"
        width="95" />
        <spinner
        follows="left|top"
        decimal_digits="0"
        increment="1"
        control_name="Edit Cost"
        name="Edit Cost"
        left_pad="-7"
        width="86"
        min_val="0"
        height="20"
        max_val="999999999"/>
      <combo_box
        height="22"
        left_pad="1"
        top_pad="-20"
        layout="topleft"
        follows="left|top"
        name="ComboBoxSaleType"
        width="110">
        <combo_box.item
             name="Copy"
             label="Copy"
             value="2" />
        <combo_box.item
             name="Contents"
             label="Contents"
             value="3" />
        <combo_box.item
             name="Original"
             label="Original"
             value="1" />
      </combo_box>
      <view_border
			bevel_style="none"
			follows="top|left"
			height="0"
			layout="topleft"
			left="5"
			name="cost_text_border"
			top_pad="3"
			width="285"/>
      <panel
        border="false"
        follows="left|top"
        layout="topleft"
        mouse_opaque="false"
        name="perms_inv"
        left="0"
        top_pad="10"
        height="97"
        width="290">
        <text
          type="string"
          length="1"
          left="5"
          top_pad="1"
          height="14"
          follows="left|top|right"
          layout="topleft"
          name="perm_modify"
          width="285">
          You can:
        </text>
        <check_box
          height="16"
          label="Modify"
          layout="topleft"
          left_delta="0"
          name="CheckOwnerModify"
          top_pad="4"
          width="100" />
        <check_box
          height="16"
          label="Copy"
          layout="topleft"
          top_pad="0"
          name="CheckOwnerCopy"
          width="100" />
        <check_box
          height="16"
          label="Transfer"
          layout="topleft"
          top_pad="0"
          name="CheckOwnerTransfer"
          width="100" />
        <text
          type="string"
          length="1"
          follows="left|top"
          height="14"
          layout="topleft"
          tool_tip="Anyone can take a copy of the object . Object and all of its contents must be copy and transfer permissive."
          left_pad="0"
          name="NextOwnerLabel"
          top_pad="-66"
          width="250"
          word_wrap="true">
          Next owner:
        </text>
        <check_box
          height="16"
          label="Modify"
          layout="topleft"
          left_delta="0"
          top_pad="4"
          name="CheckNextOwnerModify"
          tool_tip="Next owner can edit properties like item name or scale of this object."
          width="100" />
        <check_box
          height="16"
          label="Copy"
          layout="topleft"
          top_pad="0"
          name="CheckNextOwnerCopy"
          tool_tip="Next owner can make unlimited copies of this object. Copies maintain creator information, and can never be more permissive than the item being copied."
          width="100" />
        <check_box
          height="16"
          label="Transfer"
          layout="topleft"
          top_pad="0"
          name="CheckNextOwnerTransfer"
          tool_tip="Next owner can give away or resell this object."
          width="100" />
        <text
          type="string"
          length="1"
          follows="left|top"
          height="14"
          layout="topleft"
          left_pad="0"
          name="AnyoneLabel"
          top_pad="-66"
          width="100">
          Anyone:
        </text>
        <check_box
          height="18"
          label="Copy"
          layout="topleft"
          left_delta="0"
          name="CheckEveryoneCopy"
          top_pad="4"
        width="100"
        tool_tip="Lets people buy this object, its content or it copy inworld for specified price." />
      </panel>
        tool_tip="Object cost." />
    </panel>

  <!--</scroll_container>-->
  <panel
    height="25"
    layout="topleft"
    name="button_panel"
    left="5"
    bottom="-6"
    width="290">
    <button
      follows="bottom|right|left"
      image_hover_unselected="Toolbar_Middle_Over"
                 image_selected="Toolbar_Middle_Selected"
                 image_unselected="Toolbar_Middle_Off"
      height="25"
      label="Cancel"
      layout="topleft"
      name="cancel_btn"
      left="5"
      top="0"
      width="292" />
  </panel>
</panel>
