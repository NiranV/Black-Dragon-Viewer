    height="493"
    width="313"
    top="45">-->
    <panel
      follows="left|top|bottom"
      height="490"
      label=""
      layout="topleft"
      left="10"
      name="item_profile"
      top_pad="10"
      width="295">
      <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="LabelItemNameTitle"
        top="5"
        width="90">
        Name:
      </text>
      <line_editor
        border_style="line"
        border_thickness="1"
        follows="left|top|right"
        height="19"
        layout="topleft"
        left_pad="0"
        max_length_bytes="63"
        name="LabelItemName"
        top_delta="0"
        width="197" />
      <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="LabelItemDescTitle"
        top_pad="3"
        width="90">
        Description:
      </text>
      <line_editor
        border_style="line"
        border_thickness="1"
        follows="left|top|right"
        height="19"
        layout="topleft"
        left_pad="0"
        max_length_bytes="127"
        name="LabelItemDesc"
        top_delta="0"
        width="197" />
      <text
        type="string"
        length="1"
        follows="left|top"
        height="29"
        layout="topleft"
        left="5"
        name="LabelCreatorTitle"
        top_pad="3"
        width="90">
        Creator:
      </text>
      <!--<avatar_icon
        follows="top|left"
        height="20"
        default_icon_name="Generic_Person"
        layout="topleft"
        left_pad="0"
        top_delta="-6"
