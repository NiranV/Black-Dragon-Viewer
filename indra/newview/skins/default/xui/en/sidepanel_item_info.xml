        width="20" />-->
      <text
        type="string"
        follows="left|right|top"
        font="SansSerifSmall"
        height="29"
        layout="topleft"
        left_pad="0"
        name="LabelCreatorName"
        top_delta="0"
        use_ellipses="true"
        width="195">
        TestString PleaseIgnore
      </text>
      <!--<button
        follows="top|right"
        height="16"
        image_selected="Inspector_I"
        image_unselected="Inspector_I"
        layout="topleft"
        right="-5"
        name="BtnCreator"
        top_delta="-6"
        width="16" />-->
      <text
        type="string"
        length="1"
        follows="left|top"
        height="29"
        layout="topleft"
        left="5"
        name="LabelOwnerTitle"
        top_pad="3"
        width="90">
        Owner:
      </text>
      <!--<avatar_icon
        follows="top|left"
        height="20"
        default_icon_name="Generic_Person"
        layout="topleft"
        left_pad="0"
        top_delta="-6"
        mouse_opaque="true"
        width="20" />-->
      <text
        type="string"
        follows="left|right|top"
        font="SansSerifSmall"
        height="29"
        layout="topleft"
        left_pad="0"
        name="LabelOwnerName"
        top_delta="0"
        use_ellipses="true"
        width="195">
        TestString PleaseIgnore
      </text>
      <!--<button
        follows="top|right"
        height="16"
        image_selected="Inspector_I"
        image_unselected="Inspector_I"
        layout="topleft"
        right="-5"
        name="BtnOwner"
        top_delta="-3"
        width="16" />-->
      <text
        type="string"
        length="1"
        follows="left|top"
        height="29"
        layout="topleft"
        left="5"
        name="LabelAcquiredTitle"
        top_pad="3"
        width="90">
        Acquired:
      </text>
      <text
        type="string"
        length="1"
        follows="left|top|right"
        height="29"
        layout="topleft"
        left_pad="0"
        name="LabelAcquiredDate"
        top_delta="0"
        width="195">
        99.99.9999:99:99
      </text>
      <text
        type="string"
        length="1"
        follows="left|top"
        height="29"
        layout="topleft"
        left="5"
        name="LabelItemExperienceTitle"
        top_pad="3"
        width="90"
        visible="true">
        Experience:
      </text>
      <text
        type="string"
        length="1"
        follows="left|top|right"
        height="29"
        layout="topleft"
        left_pad="0"
        name="LabelItemExperience"
        top_delta="0"
        width="195"
        visible="true">
        (None)
        </text>
      <text
          type="string"
          length="1"
          follows="left|top"
          height="29"
          layout="topleft"
          left="5"
          name="GroupLabel"
          top_pad="3"
          width="90">
          Group:
        </text>
        <check_box
          height="18"
          label="Share"
          layout="topleft"
          left_pad="0"
          top_delta="-2"
          name="CheckShareWithGroup"
          tool_tip="Allow all members of the set group to share your modify permissions for this object. You must Deed to enable role restrictions."
          width="150" />
        <view_border
			bevel_style="none"
			follows="top|left"
			height="0"
			layout="topleft"
			left="5"
			name="cost_text_border"
			top_pad="3"
			width="285"/>
        <check_box
        height="20"
        label="For Sale"
        layout="topleft"
        left="5"
        name="CheckPurchase"
        top_pad="4"
        width="95" />
        <spinner
        follows="left|top"
        decimal_digits="0"
        increment="1"
        control_name="Edit Cost"
        name="Edit Cost"
        left_pad="-7"
        width="86"
        min_val="0"
        height="20"
        max_val="999999999"/>
      <combo_box
        height="22"
        left_pad="1"
        top_pad="-20"
        layout="topleft"
        follows="left|top"
        name="ComboBoxSaleType"
        width="110">
        <combo_box.item
             name="Copy"
             label="Copy"
             value="2" />
        <combo_box.item
             name="Contents"
             label="Contents"
             value="3" />
        <combo_box.item
             name="Original"
             label="Original"
             value="1" />
      </combo_box>
      <view_border
			bevel_style="none"
			follows="top|left"
			height="0"
			layout="topleft"
			left="5"
			name="cost_text_border"
			top_pad="3"
			width="285"/>
      <panel
        border="false"
        follows="left|top"
        layout="topleft"
        mouse_opaque="false"
        name="perms_inv"
        left="0"
        top_pad="10"
        height="97"
        width="290">
        <text
          type="string"
          length="1"
          left="5"
          top_pad="1"
          height="14"
          follows="left|top|right"
          layout="topleft"
          name="perm_modify"
          width="285">
          You can:
        </text>
        <check_box
          height="16"
          label="Modify"
          layout="topleft"
          left_delta="0"
          name="CheckOwnerModify"
          top_pad="4"
          width="100" />
        <check_box
          height="16"
          label="Copy"
          layout="topleft"
          top_pad="0"
          name="CheckOwnerCopy"
          width="100" />
        <check_box
          height="16"
          label="Transfer"
          layout="topleft"
          top_pad="0"
          name="CheckOwnerTransfer"
          width="100" />
        <text
          type="string"
          length="1"
          follows="left|top"
          height="14"
          layout="topleft"
          left_pad="0"
          name="NextOwnerLabel"
          top_pad="-66"
          width="250"
          word_wrap="true">
          Next owner:
        </text>
        <check_box
          height="16"
          label="Modify"
          layout="topleft"
          left_delta="0"
          top_pad="4"
          name="CheckNextOwnerModify"
          width="100" />
        <check_box
          height="16"
          label="Copy"
          layout="topleft"
          top_pad="0"
          name="CheckNextOwnerCopy"
          width="100" />
        <check_box
          height="16"
          label="Transfer"
          layout="topleft"
          top_pad="0"
          name="CheckNextOwnerTransfer"
          tool_tip="Next owner can give away or resell this object"
          width="100" />
        <text
          type="string"
          length="1"
          follows="left|top"
          height="14"
          layout="topleft"
          left_pad="0"
          name="AnyoneLabel"
          top_pad="-66"
          width="100">
          Anyone:
        </text>
        <check_box
          height="18"
          label="Copy"
          layout="topleft"
          left_delta="0"
          name="CheckEveryoneCopy"
          top_pad="4"
          width="100" />
      </panel>
    </panel>

  <!--</scroll_container>-->
  <panel
    height="25"
    layout="topleft"
    name="button_panel"
    left="5"
    bottom="-6"
    width="290">
    <button
      follows="bottom|right|left"
      image_hover_unselected="Toolbar_Middle_Over"
                 image_selected="Toolbar_Middle_Selected"
                 image_unselected="Toolbar_Middle_Off"
      height="25"
      label="Cancel"
      layout="topleft"
      name="cancel_btn"
      left="5"
      top="0"
      width="292" />
  </panel>
</panel>
