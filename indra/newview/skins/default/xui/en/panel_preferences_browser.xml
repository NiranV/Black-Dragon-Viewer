<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
follows="all"
height="408"
label="Browser"
layout="topleft"
left="0"
name="Browser Panel"
top="45"
width="510">
      <panel
 follows="left|top|right"
 height="20"
 bg_visible="true"
 bg_alpha_color="0.10 0.43 0.77 0.28"
 layout="topleft"
 mouse_opaque="false"
 left="1"
 name="title"
 top="1"
 width="508"/>
		     <text
		     type="string"
		     length="1"
		     follows="left|top"
		     height="12"
		     layout="topleft"
		     left="10"
		     name="web_label"
		     top_delta="4"
		     width="120">Web Options:</text>
      <text
 type="string"
 length="1"
 follows="left|top"
 height="12"
 layout="topleft"
 left="10"
 name="InternalBrowserBehavior"
 top_pad="15"
 width="220">
 Internal Browser Usage:
 </text>
                  <radio_group
                  control_name="PreferredBrowserBehavior"
                  height="19"
                  layout="topleft"
                  left="10"
                  name="PreferredBrowserBehavior"
                  top_pad="5"
                  width="600">
                        <radio_item
                        height="16"
                        label="Never"
                        value="0"
                        layout="topleft"
				tool_tip="This option disallows the internal browser to be used when clicking on links in chat."
                        name="0"
                        top="0"
                        width="70">
                              <radio_item.check_button
                              image_unselected="Checkbox_Off"
                              image_selected="Checkbox_On"
                              image_disabled="Checkbox_Off_Disabled"
                              image_disabled_selected="Checkbox_On_Disabled"
                              image_pressed="Checkbox_Press"
                              image_pressed_selected="Checkbox_On_Press" />
                        </radio_item>
                        <radio_item
                        height="16"
                        label="SL Links"
                        value="1"
                        layout="topleft"
				tool_tip="This option allows the internal browser only to be used when clicking on Second Life specific links such as profile or search links."
                        name="0"
                        left_pad="10"
                        top_delta="0"
                        width="70">
                              <radio_item.check_button
                              image_unselected="Checkbox_Off"
                              image_selected="Checkbox_On"
                              image_disabled="Checkbox_Off_Disabled"
                              image_disabled_selected="Checkbox_On_Disabled"
                              image_pressed="Checkbox_Press"
                              image_pressed_selected="Checkbox_On_Press" />
                        </radio_item>
                        <radio_item
                        height="16"
                        label="Always"
                        halign="right"
				tool_tip="This option allows the internal browser to be used for all links regardless of if they are Second Life specific or external links."
                        value="2"
                        layout="topleft"
                        name="2"
                        left_pad="10"
                        top_delta="0"
                        width="70">
                              <radio_item.check_button
                              image_unselected="Checkbox_Off"
                              image_selected="Checkbox_On"
                              image_disabled="Checkbox_Off_Disabled"
                              image_disabled_selected="Checkbox_On_Disabled"
                              image_pressed="Checkbox_Press"
                              image_pressed_selected="Checkbox_On_Press" />
                        </radio_item>
                  </radio_group>
      <check_box
      top_delta="2"
      enabled="true"
      follows="left|top"
      layout="topleft"
      height="16"
      initial_value="true"
      control_name="BrowserPluginsEnabled"
	tool_tip="This option allows the internal browser to use plugins.
||&lt;red&gt;It is currently unknown what this option does or if it has any impact.&lt;/red&gt;"
      label="Enable plugins"
      left_delta="0"
      mouse_opaque="true"
      name="browser_plugins_enabled"
      radio_style="false"
      width="400"
      top_pad="5"/>
      <check_box
      top_delta="4"
      enabled="true"
      follows="left|top"
      layout="topleft"
      height="16"
      initial_value="true"
	tool_tip="This option allows the internal browser to use and execute javascripts on opened webpages."
      control_name="BrowserJavascriptEnabled"
      label="Enable Javascript"
      left_delta="0"
      mouse_opaque="true"
      name="browser_javascript_enabled"
      radio_style="false"
      width="400"
      top_pad="0"/>
      <check_box
      top_delta="4"
      enabled="true"
      follows="left|top"
      layout="topleft"
      height="16"
      initial_value="true"
	tool_tip="This option enables the internal browser to use the javascript bridge for executing javascripts on opened webpages.
||&lt;red&gt;It is currently unknown what this option does or if it has any impact.&lt;/red&gt;"
      enabled_control="BrowserJavascriptEnabled"
      control_name="BrowserEnableJSObject"
      label="Enable Javascript Bridge"
      left_delta="0"
      mouse_opaque="true"
      name="browser_javascript_object_enabled"
      radio_style="false"
      width="400"
      top_pad="0"/>
      <check_box
      top_delta="4"
      enabled="true"
      follows="left|top"
      layout="topleft"
      height="16"
      initial_value="false"
      control_name="MediaEnablePopups"
      label="Enable Media Browser Pop-ups"
	tool_tip="This option allows popups being opened by webpages opened in the internal browser."
      left_delta="0"
      mouse_opaque="true"
      name="media_popup_enabled"
      width="400"           
      top_pad="0"/>
      <check_box
      enabled="true"
      follows="left|top"
      layout="topleft"
      height="16"
      initial_value="false"
      control_name="AutoLoadWebProfiles"
      label="Auto load all Webprofiles without asking"
	tool_tip="This option allows opening all webprofiles without asking the user first.
||&lt;red&gt;It is currently unknown what this option does or if it has any impact.&lt;/red&gt;"
      left_delta="0"
      mouse_opaque="true"
      name="AutoLoadWebProfiles"
      width="400"
      top_pad="0"/>
	  <slider
   control_name="MediaBrowserWindowLimit"
   can_edit_text="true"
   decimal_digits="0"
   follows="left|top"
   height="15"
   increment="1"
   initial_value="8"
   label="Max Browser Limit:"
   label_width="150"
   layout="topleft"
   left="10"
   max_val="100"
   min_val="0"
   tool_tip="This option sets maximum amount of opened internal browsers instances. Setting this to 0 disables the limit."
   top_pad="5"
   name="MediaBrowserWindowLimit"/>
  <button
 follows="left|top"
 control_name="MediaBrowserWindowLimit"
 height="15"
 is_toggle="false"
 label="⌊default⌉"
 image_selected="PushButton_Default_Off"
 image_unselected="PushButton_Default_Off"
 layout="topleft"
 left="370"
 top_delta="0"
 name="default"
 width="60">
       <button.commit_callback
                                 function="Pref.Default"/>
      </button>
      
      <panel
 follows="left|top|right"
 height="20"
 bg_visible="true"
 bg_alpha_color="0.10 0.43 0.77 0.28"
 layout="topleft"
 mouse_opaque="false"
 left="1"
 name="title"
 top_pad="10"
 width="508"/>
		     <check_box
      enabled="true"
      follows="left|top"
      layout="topleft"
      height="16"
      initial_value="false"
      control_name="BrowserProxyEnabled"
	tool_tip="This option enables browsing in the internal browser through a proxy. It is highly recommended not to use this feature unless you know exactly what you are doing."
      label="Web Proxy"
      left="5"
      mouse_opaque="true"
      name="web_proxy_enabled"
      radio_style="false"
      width="400"
      top_delta="1"/>
      <line_editor
      control_name="BrowserProxyAddress"
      enabled_control="BrowserProxyEnabled"
      follows="left|top|right"
      font="SansSerif"
      height="23"
      layout="topleft"
      left="10"
      name="web_proxy_editor"
      tool_tip="This option sets name or IP address of the proxy you would like to use for the internal browser."
      top_pad="15"
      width="488" />
	  <slider
   control_name="BrowserProxyPort"
   enabled_control="BrowserProxyEnabled"
   can_edit_text="true"
   decimal_digits="0"
   follows="left|top"
   height="15"
   increment="1"
   initial_value="80"
   label="Port:"
   tool_tip="This option sets the port used by the browser when using a proxy."
   label_width="150"
   layout="topleft"
   left="10"
   max_val="12000"
   min_val="10"
   top_pad="5"
   name="web_proxy_port"/>
  <button
 follows="left|top"
 control_name="BrowserProxyPort"
 height="15"
 is_toggle="false"
 label="⌊default⌉"
 image_selected="PushButton_Default_Off"
 image_unselected="PushButton_Default_Off"
 layout="topleft"
 left="370"
 top_delta="0"
 name="default"
 width="60">
       <button.commit_callback
                                 function="Pref.Default"/>
      </button>
      <panel
 follows="left|top|right"
 height="20"
 bg_visible="true"
 bg_alpha_color="0.10 0.43 0.77 0.28"
 layout="topleft"
 mouse_opaque="false"
 left="1"
 name="title"
 top_pad="10"
 width="508"/>
		     <text
		     type="string"
		     length="1"
		     follows="left|top"
		     height="12"
		     layout="topleft"
		     left="10"
		     name="web_label"
		     top_delta="4"
		     width="120">Prim Media Options:</text>
      <check_box
      follows="left|top"
      layout="topleft"
      height="16"
      initial_value="false"
      control_name="MediaShowOutsideParcel"
      label="Show Media outside your current Parcel"
	tool_tip="This option toggles visibility of media on prims outside of your current parcel."
      left="10"
      name="MediaShowOutsideParcel"
      width="300"
      top_pad="15"/>
      <check_box
      follows="left|top"
      layout="topleft"
      height="16"
      initial_value="true"
      control_name="MediaShowWithinParcel"
      label="Show Media within your current Parcel"
	tool_tip="This option toggles visibility of media on prims inside your current parcel."
      left="10"
      name="MediaShowWithinParcel"
      width="300"
      top_pad="0"/>
      <check_box
      follows="left|top"
      layout="topleft"
      height="16"
      control_name="PrimMediaDragNDrop"
      label="Allow Drag n Drop of URLs onto Media Prims"
	tool_tip="This option allows dragging URL links on any media-on-a-prim surface to automatically set the dragged link as media on that surface."
      left="10"
      name="PrimMediaDragNDrop"
      width="300"
      top_pad="0"/>
	  <slider
   control_name="MediaControlTimeout"
   can_edit_text="true"
   decimal_digits="1"
   follows="left|top"
   height="15"
   increment="0.5"
   initial_value="8"
   label="Media UI Timeout:"
   label_width="150"
   tool_tip="This option sets the amount of time in seconds after which the interface for media-on-a-prim surfaces starts fading out when not being used."
   layout="topleft"
   left="10"
   max_val="100"
   min_val="0"
   top_pad="5"
   name="MediaControlTimeout" />
  <button
 follows="left|top"
 control_name="MediaControlTimeout"
 height="15"
 is_toggle="false"
 label="⌊default⌉"
 image_selected="PushButton_Default_Off"
 image_unselected="PushButton_Default_Off"
 layout="topleft"
 left="370"
 top_delta="0"
 name="default"
 width="60">
       <button.commit_callback
                                 function="Pref.Default"/>
      </button>
  <slider
   control_name="PluginInstancesTotal"
   can_edit_text="true"
   decimal_digits="0"
   follows="left|top"
   height="15"
   increment="1"
   initial_value="8"
   label="Max Plugin Limit:"
   label_width="150"
   layout="topleft"
   tool_tip="This option sets the maximum amount of media-on-a-prim instances allowed to run simultaneously."
   left="10"
   max_val="100"
   min_val="10"
   top_pad="1"
   name="PluginInstancesTotal"/>
  <button
 follows="left|top"
 control_name="PluginInstancesTotal"
 height="15"
 is_toggle="false"
 label="⌊default⌉"
 image_selected="PushButton_Default_Off"
 image_unselected="PushButton_Default_Off"
 layout="topleft"
 left="370"
 top_delta="0"
 name="default"
 width="60">
       <button.commit_callback
                                 function="Pref.Default"/>
      </button>
</panel>

