<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 background_visible="true"
 bg_alpha_image="Toast_Background" 
 border_visible="false"
 border="false"
 chrome="true"
 follows="bottom"
 height="155"
 layout="topleft"
 name="presets_pulldown"
 width="225">
  <text
    type="string"
    length="1"
    follows="left|top"
    height="12"
    layout="topleft"
    top="5"
    left_delta="6"
    name="Graphic Presets"
    width="180">
      Graphic Presets
  </text>
  <scroll_list
    follows="left|top"
    layout="topleft"
    column_padding="2"
    height="100"
    width="215"
    draw_heading="false"
    draw_stripes="false"
    top_delta="15"
    left_delta="0"
    name="preset_list">
    <scroll_list.columns
      name="icon"
      width="16" />
    <scroll_list.columns
      relative_width="1"
      name="preset_name" />
      <scroll_list.commit_callback
      function="Presets.RowClick" />
  </scroll_list>
  <view_border
    bevel_style="none"
    follows="top|left"
    height="0"
    layout="topleft"
    left="5"
    name="horiz_separator"
    top_delta="105"
    width="215" />
  <button
    name="open_prefs_btn"
    label="Open Display Preferences"
    tool_tip="Open the preferences window"
    image_hover_unselected="Toolbar_Middle_Over"
	 image_selected="Toolbar_Middle_Selected"
	 image_unselected="Toolbar_Middle_Off"
    top_delta="2"
    left="5"
    height="23"
    width="215">
    <button.commit_callback
      function="Presets.GoGraphicsPrefs" />
  </button>
</panel>
