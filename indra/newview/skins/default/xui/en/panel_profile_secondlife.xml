<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 name="panel_profile"
 label="Profile"
 top="0"
 left="0"
 height="480"
 width="420"
 follows="all"
 layout="topleft"
>
   <string 
    name="date_format"
    value="SL birthdate: [mth,datetime,slt] [day,datetime,slt], [year,datetime,slt]" />
   <string
    name="age_format"
    value="[AGE]" />
   <string
    name="partner_text"
    value="Partner: [LINK]" />
   <string
    name="CaptionTextAcctInfo">
Account: [ACCTTYPE]
[PAYMENTINFO]
    </string>
   
   <panel
 follows="left|top|right"
 height="21"
 bg_visible="true"
 bg_alpha_color="0.10 0.43 0.77 0.28"
 layout="topleft"
 mouse_opaque="false"
 left="3"
 name="title"
 top="1"
 width="415"/>
   <text
 name="show_ims_in_label"
 follows="left|top"
 layout="topleft"
 left="10"
 height="12"
 width="40"
 top_delta="5">
 Name:
 </text>
   
   <text
       name="display_name"
       value="(loading...)"
       width="300"
       top_delta="0"
       left_pad="0"
       height="12"
       follows="left|top"
       layout="topleft"/>
   <!--<line_editor
       name="user_name"
       border_thickness="0"
       use_bg_color="false"
       background_image_disabled=""
       background_image_focused=""
       enabled="false"
       value="(loading...)"
       top_pad="0"
       left_delta="0"
       right="-1"
       height="16"
       follows="left|top|right"
       layout="topleft"/>-->
   
   <layout_stack
   name="online_stack"
   top_delta="0"
   right="-1"
   height="20"
   width="65"
   border_size="0"
   follows="right|top"
   layout="topleft"
   animate="false"
   orientation="vertical">
   
   </layout_stack>
   
   <icon
       name="2nd_life_pic"
       image_name="Generic_Person_Large"
       layout="topleft"
       follows="top|left"
       interactable="true"
       width="160"
       height="160"
       top_pad="2"
       left="6"/>

      <loading_indicator
       name="image_upload_indicator"
       top="69"
       left="69"
       height="23"
       width="23"
       follows="top|left"
       layout="topleft"
       visible="false"/>
      
      <menu_button
       layout="topleft"
       follows="left|top"
       left="6"
       top="190"
       height="25"
       width="160"
       label="Actions"
       halign="left"
       image_unselected="ToolbarDropDown_Off"
       image_selected="ToolbarDropDown_Press"
       image_pressed="ToolbarDropDown_Press"
       image_pressed_selected="ToolbarDropDown_Press"
       image_disabled="ToolbarDropDown_Off"
       name="agent_actions_menu" />

  <layout_stack
   name="image_stack"
   top="25"
   left_pad="6"
   height="191"
   width="245"
   border_size="0"
   follows="left|top|right"
   layout="topleft"
   animate="false"
   orientation="vertical">
   <layout_panel
     name="online_layout"
     follows="all"
     layout="topleft"
     height="16"
     auto_resize="false"
     user_resize="false"
     visible="true">
    <icon
       name="online_icon"
       image_name="Profile_Friend_Online"
       layout="topleft"
       follows="left|top"
       top="3"
       left="0"
       height="10"
       width="10"/>
    <text
       name="online_text"
       value="Online"
       top="2"
       left_pad="3"
       width="200"
       height="16"
       follows="left|top"
       layout="topleft"
       translate="false"
       visible="true"/>
      
      
    </layout_panel>
    <layout_panel
     name="offline_layout"
     follows="all"
     layout="topleft"
     height="16"
     auto_resize="false"
     user_resize="false"
     visible="false">
     <icon
       name="offline_icon"
       image_name="Profile_Friend_Offline"
       layout="topleft"
       follows="left|top"
       top="3"
       left="0"
       height="10"
       width="10"/>
     <text
       name="offline_text"
       value="Offline"
       top="2"
       left_pad="3"
       height="16"
       width="200"
       follows="left|top"
       layout="topleft"
       translate="false"
       visible="true"/>
      
      
    </layout_panel>

    <layout_panel
     name="basics_panel"
     follows="all"
     layout="topleft"
     height="36"
     auto_resize="false"
     user_resize="false"
        >
      

      <text
       name="sl_birth_date"
       enabled="false"
       value="(loading...)"
       top_pad="0"
       left="0"
       right="-1"
       height="16"
       follows="left|top|right"
       layout="topleft"/>

      <text
       name="user_age"
       enabled="false"
       value="(loading...)"
       top_pad="0"
       left="0"
       right="-1"
       height="16"
       follows="left|top|right"
       layout="topleft"/>
    </layout_panel>
    <layout_panel
     name="partner_layout"
     follows="all"
     layout="topleft"
     height="32"
     auto_resize="false"
     user_resize="false"
     visible="true">
      <text
       type="string"
       name="partner_link"
       value="Partner: (loading...)"
       top="0"
       left="0"
       right="-1"
       height="28"
       follows="left|top|right"
       layout="topleft"
       translate="false"
       use_ellipses="true"
       word_wrap="true"
       visible="true"/>
    </layout_panel>
    
    <layout_panel
     name="frind_layout"
     follows="all"
     layout="topleft"
     height="16"
     auto_resize="false"
     user_resize="false"
     visible="false">
      <text
       name="frind_text"
       value="You are friends"
       text_color="ConversationFriendColor"
       top="0"
       left="0"
       right="-1"
       height="16"
       follows="left|top|right"
       layout="topleft"
       translate="false"
       visible="true"/>
    </layout_panel>
    
    <layout_panel
     name="account_layout"
     follows="all"
     layout="topleft"
     height="33"
     auto_resize="false"
     user_resize="false">
      <text
       name="account_info"
       value="Account: (loading...)"
       top="0"
       left="0"
       right="-1"
       height="16"
       follows="left|top|right"
       layout="topleft"
       word_wrap="true"/>
    </layout_panel>
    <layout_panel
     name="indicator_stack"
     follows="all"
     layout="topleft"
     height="23"
     auto_resize="false"
     user_resize="false">
      <loading_indicator
       name="progress_indicator"
       left="67"
       top="0"
       height="23"
       width="23"
       follows="left|top"
       layout="topleft"
       visible="true"/>
    </layout_panel>
    <layout_panel
     name="settings_panel"
     follows="all"
     layout="topleft"
     height="50"
     auto_resize="false"
     user_resize="false">
      <!-- only for self -->
      <text
       name="search_label"
       value="Show my profile in search:"
       top="1"
       left="0"
       right="-1"
       height="16"
       follows="left|top|right"
       layout="topleft"/>
      <combo_box
       name="show_in_search"
       tool_tip="Let people see you in search results"
       left="1"
       top="18"
       height="23"
       width="140"
       follows="left|top"
       layout="topleft"
       visible="true"
       enabled="false">
       <combo_box.combo_button
            image_unselected="ToolbarDropDown_Off"
            image_selected="ToolbarDropDown_Press"
            image_disabled="ToolbarDropDown_Off" />
            <combo_box.drop_down_button
            image_unselected="ToolbarDropDown_Off"
            image_selected="ToolbarDropDown_Press"
            image_pressed="ToolbarDropDown_Press" 
            image_pressed_selected="ToolbarDropDown_Press"
            image_disabled="ToolbarDropDown_Off" />
        <combo_box.item
           name="Hide"
           label="Hide"
           value="0" />
        <combo_box.item
           name="Show"
           label="Show"
           value="1" />
      </combo_box>
    </layout_panel>
    
  </layout_stack>
  
  <group_list
       name="group_list"
       top_pad="3"
       left="6"
       height="80"
       width="410"
       follows="top|left|right"
       layout="topleft"
       border_visible="false"
       color="ScrollBgWriteableColor"
       for_agent="false"/>
  
  <panel
 follows="left|top|right"
 height="21"
 bg_visible="true"
 bg_alpha_color="0.10 0.43 0.77 0.28"
 layout="topleft"
 mouse_opaque="false"
 left="3"
 name="title"
 top_pad="4"
 width="415"/>
   <text
 name="show_ims_in_label"
 follows="left|top"
 layout="topleft"
 left="10"
 height="12"
 width="40"
 top_delta="5">
 About:
 </text>
  
  <text_editor
       name="sl_description_edit"
       trusted_content="true"
       always_show_icons="true"
       commit_on_focus_lost="false"
       enabled="false"
       top_pad="10"
       height="122"
       width="409"
       left="6"
       follows="all"
       layout="topleft"
       bg_readonly_color="Transparent"
       border_visible="false"
       font="SansSerifSmall"
       h_pad="2"
       max_length="65000"
       parse_urls="true"
       word_wrap="true"
        />
        <button
         name="save_description_changes"
         image_hover_unselected="Toolbar_Middle_Over"
 image_selected="Toolbar_Middle_Selected"
 image_unselected="Toolbar_Middle_Off"
         label="Save"
         top_pad="5"
         left="3"
         height="23"
         width="207"
         enabled="false"
         follows="bottom|left"
         layout="topleft"/>
        <button
         name="discard_description_changes"
         image_hover_unselected="Toolbar_Middle_Over"
 image_selected="Toolbar_Middle_Selected"
 image_unselected="Toolbar_Middle_Off"
         label="Discard"
         top_delta="0"
         left_pad="1"
         height="23"
         width="207"
         enabled="false"
         follows="bottom|left|right"
         layout="topleft"/>

      

      <icon
       tool_tip="Friend can see my online status"
       mouse_opaque="true"
       name="can_see_online"
       image_name="Permission_Visible_Online"
       layout="topleft"
       follows="right|top"
       interactable="true"
       top="3"
       right="-35"
       height="16"
       width="16" />

      <icon
       tool_tip="Friend can not see my online status"
       mouse_opaque="true"
       name="cant_see_online"
       color="1 1 1 0.4"
       image_name="Permission_Visible_Online"
       layout="topleft"
       follows="right|top"
       interactable="true"
       top="3"
       right="-35"
       height="16"
       width="16"/>

      <icon
       tool_tip="Friend can see me on map"
       mouse_opaque="true"
       name="can_see_on_map"
       image_name="Permission_Visible_Map"
       layout="topleft"
       follows="right|top"
       interactable="true"
       top="3"
       right="-20"
       height="16"
       width="16" />

      <icon
       tool_tip="Friend can not see me on map"
       mouse_opaque="true"
       name="cant_see_on_map"
       image_name="Permission_Visible_Map"
       layout="topleft"
       color="1 1 1 0.4"
       follows="right|top"
       interactable="true"
       top="3"
       right="-20"
       height="16"
       width="16"
       left_pad="2" />

      <icon
       tool_tip="Friend can edit my objects"
       mouse_opaque="true"
       name="can_edit_objects"
       image_name="Permission_Edit_Objects_Theirs"
       layout="topleft"
       follows="right|top"
       interactable="true"
       top="3"
       right="-5"
       height="16"
       width="16" />

      <icon
       tool_tip="Friend can not edit my objects"
       mouse_opaque="true"
       name="cant_edit_objects"
       image_name="Permission_Edit_Objects_Theirs"
       layout="topleft"
       color="1 1 1 0.4"
       follows="right|top"
       interactable="true"
       top="3"
       right="-5"
       height="16"
       width="16" />

      

      
</panel>
