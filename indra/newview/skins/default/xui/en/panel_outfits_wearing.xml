<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 background_visible="false"
 follows="all"
 height="430"
 layout="topleft"
 left="0"
 name="Wearing"
 top="0"
 width="312">
<panel.string
    name="no_attachments">
    No attachments worn.
   </panel.string>
     <accordion
     fit_parent="true"
     follows="all"
     height="402"
     layout="topleft"
     left="3"
     single_expansion="true"
     top="1"
     name="wearables_accordion"
     background_visible="false"
     width="309">
     <accordion_tab
     layout="topleft"
     name="tab_wearables"
     title="Wearables">
    <wearable_items_list
     follows="all"
     height="402"
     layout="topleft"
     left="3"
     multi_select="true"
     color="Transparent"
     name="cof_items_list"
     show_item_widgets="true"
     standalone="false"
     top="1"
     width="307"
     worn_indication_enabled="false" />
     </accordion_tab>
     <accordion_tab
     layout="topleft"
     name="tab_temp_attachments"
     title="Temporary attachments">
    <scroll_list
     draw_heading="false"
     background_visible="false"
     left="3"
     width="309"
     height="400"
     follows="all"
     name="temp_attachments_list">
        <scroll_list.columns
         name="icon"
         width="15" />
        <scroll_list.columns
         name="text"
         width="210" />
  </scroll_list>
     </accordion_tab>
     </accordion>
    <panel
     background_visible="false"
     follows="bottom|left|right"
     height="26"
     layout="topleft"
     left="1"
     name="bottom_panel"
     top_pad="0"
     width="313">
        <menu_button
         follows="bottom|left"
         height="25"
         image_hover_unselected="Toolbar_Middle_Over"
         image_overlay="OptionsMenu_Off"
         image_selected="Toolbar_Middle_Selected"
         image_unselected="Toolbar_Middle_Off"
         layout="topleft"
         left="2"
         name="options_gear_btn"
         tool_tip="Show additional options"
         top="1"
         width="31" />
        <icon
         follows="bottom|right"
         height="25"
         image_name="Toolbar_Middle_Off"
         layout="topleft"
         left_pad="149"
         name="dummy_icon"
         width="127" />
    </panel>
</panel>
