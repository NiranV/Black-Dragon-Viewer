<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
	   name="Teleport History"
	   bg_alpha_image="Panel_Background"
			   background_visible="true"
	   bottom="0"
	   height="400"
	   left="0"
	   width="313"
     help_topic="panel_teleport_history"
     border="false" follows="left|top|right|bottom">     
    <accordion
     follows="left|top|right|bottom"
     height="371"
     layout="topleft"
     left="3"
     top="2"
     name="history_accordion"
     background_visible="false"
     bg_alpha_color="Black_50"
     width="310">
        <no_matched_tabs_text
         name="no_matched_teleports_msg"
         value="Didn't find what you're looking for? Try [secondlife:///app/search/places/[SEARCH_TERM] Search]." />
        <no_visible_tabs_text
         name="no_teleports_msg"
         value="Teleport history is empty. Try [secondlife:///app/search/places/ Search]." />
	    <accordion_tab
         layout="topleft"
         name="today"
         title="Today">
            <flat_list_view
			 color="Transparent"
             follows="all"
             height="150"
             layout="topleft"
             left="0"
             name="today_items"
             top="0"
             width="307">
	        </flat_list_view>
	    </accordion_tab>
        <accordion_tab
         layout="topleft"
         name="yesterday"
         title="Yesterday">
            <flat_list_view
			 color="Transparent"
             follows="all"
             height="150"
             layout="topleft"
             left="0"
             name="yesterday_items"
             top="0"
             width="307">
	        </flat_list_view>
	    </accordion_tab>
        <accordion_tab
         layout="topleft"
         name="2_days_ago"
         title="2 days ago">
            <flat_list_view
			 color="Transparent"
             follows="all"
             height="150"
             layout="topleft"
             left="0"
             name="2_days_ago"
             top="0"
             width="307">
	        </flat_list_view>
	    </accordion_tab>
        <accordion_tab
         layout="topleft"
         name="3_days_ago"
         title="3 days ago">
            <flat_list_view
			 color="Transparent"
             follows="all"
             height="150"
             layout="topleft"
             left="0"
             name="3_days_ago"
             top="0"
             width="307">
	        </flat_list_view>
	    </accordion_tab>
        <accordion_tab
         layout="topleft"
         name="4_days_ago"
         title="4 days ago">
            <flat_list_view
			 color="Transparent"
             follows="all"
             height="150"
             layout="topleft"
             left="0"
             name="4_days_ago"
             top="0"
             width="307">
	        </flat_list_view>
	    </accordion_tab>
        <accordion_tab
         layout="topleft"
         name="5_days_ago"
         title="5 days ago">
            <flat_list_view
			 color="Transparent"
             follows="all"
             height="150"
             layout="topleft"
             left="0"
             name="5_days_ago_items"
             top="0"
             width="307">
	        </flat_list_view>
	    </accordion_tab>
        <accordion_tab
         layout="topleft"
         name="6_days_and_older"
         title="6 days and older">
            <flat_list_view
			 color="Transparent"
             follows="all"
             height="150"
             layout="topleft"
             left="0"
             name="6_days_and_older_items"
             top="0"
             width="307">
	        </flat_list_view>
	    </accordion_tab>
        <accordion_tab
         layout="topleft"
         name="1_month_and_older"
         title="1 month and older">
            <flat_list_view
			 color="Transparent"
             follows="all"
             height="150"
             layout="topleft"
             left="0"
             name="1_month_and_older_items"
             top="0"
             width="307">
	        </flat_list_view>
	    </accordion_tab>
        <accordion_tab
         layout="topleft"
         name="6_months_and_older"
         title="6 months and older">
            <flat_list_view
			 color="Transparent"
             follows="all"
             height="150"
             layout="topleft"
             left="0"
             name="6_months_and_older_items"
             top="0"
             width="307">
	        </flat_list_view>
	    </accordion_tab>
    </accordion>
    <panel
     background_visible="false"
	 bg_alpha_color="Black_50"
     bevel_style="none"
     bottom="1"
     follows="left|right|bottom"
     height="25"
     layout="bottomleft"
     left="3"
     name="bottom_panel"
     width="307">
	  <layout_stack
		   animate="false"
		   border_size="0"
		   follows="left|right|bottom"
		   height="25"
		   layout="topleft"
		   orientation="horizontal"
		   top="0"
		   left="0"
		   name="bottom_panel"
		   width="307">
           <layout_panel
		       auto_resize="true"
		       height="25"
		       layout="topleft"
		       name="dummy_panel"
		       width="209">
			<icon
             follows="left|bottom|right"
             image_name="Toolbar_Middle_Off"
             height="25"
             width="209"
             layout="topleft"
             left="0"
             top="0"/>
		   </layout_panel>
		   <layout_panel
		       auto_resize="false"
		       height="25"
		       layout="topleft"
		       name="gear_btn_panel"
		       width="96">
        <menu_button
         follows="bottom|left"
         tool_tip="Show additional options"
         height="25"
         image_hover_unselected="Toolbar_Middle_Over"
         image_overlay="OptionsMenu_Off"
         image_selected="Toolbar_Middle_Selected"
         image_unselected="Toolbar_Middle_Off"
         layout="topleft"
         left="1"
         name="gear_btn"
         top="0"
         width="31" />
		<icon
		 follows="left|top|right"
		 image_name="Toolbar_Middle_Off"
		 height="25"
		 width="64"
		 layout="topleft"
		 left_pad="1"
		 top="0"/>
		   </layout_panel>
	  </layout_stack>
    </panel>
</panel>
