<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel 
 border="false" 
 enabled="true" 
 follows="left|top|right|bottom"
 height="460" 
 label="Customize" 
 left="0" 
 mouse_opaque="true"
 name="Media settings for controls" 
 width="260">

  <text
   top="4"
   follows="top|left"
   layout="topleft"
   height="15"
   left="5"
   name="controls_label" >
    Controls:
  </text>
  <combo_box
   allow_text_entry="false"
   top_delta="-1"
   follows="left|top"
   layout="topleft"
   height="18"
   left="100"
   max_chars="20"
   name="controls"
   width="120">
    <combo_item
	 name="Standard"
	 value="Standard">
      Standard
    </combo_item>
    <combo_item
	 name="Mini"
	 value="Mini">
      Mini
    </combo_item>
  </combo_box>

  <text 
   top_pad="3"
   enabled="false"
   follows="top|left"
   layout="topleft"
   height="15" 
   left="5" 
   name="owner_label" >
    Owner
  </text>
  
  <check_box 
   top_pad="5"
   enabled="true" 
   follows="left|top"
   layout="topleft"
   height="14" 
   initial_value="false"
   label="Allow Navigation &amp; Interactivity" 
   left="5" 
   mouse_opaque="true"
   name="perms_owner_interact" 
   radio_style="false" 
   width="250" />

  <check_box 
   top_pad="1" 
   enabled="true" 
   follows="left|top" 
   layout="topleft"
   height="14" 
   initial_value="false"
   label="Show Control Bar" 
   left="5" 
   mouse_opaque="true"
   name="perms_owner_control" 
   radio_style="false" 
   width="250" />

  <text 
   top_pad="7"
   enabled="false"
   follows="top|left"
   layout="topleft"
   height="15" 
   left="5"
   name="group_label" >
    Group:
  </text>
  
  <name_box 
   top_delta="0"
   enabled="false" 
   follows="left|top"
   layout="topleft"
   height="15"
   left="60" 
   name="perms_group_name"
   value="" 
   width="200" />
   
  <check_box 
   top_pad="5"
   enabled="true" 
   follows="left|top" 
   layout="topleft"
   height="14" 
   initial_value="false"
   label="Allow Navigation &amp; Interactivity" 
   left="5" 
   mouse_opaque="true"
   name="perms_group_interact" 
   radio_style="false" 
   width="250" />

  <check_box 
   top_pad="1"
   enabled="true" 
   follows="left|top" 
   layout="topleft"
   height="14" 
   initial_value="false"
   label="Show Control Bar" 
   left="5" 
   mouse_opaque="true"
   name="perms_group_control" 
   radio_style="false" 
   width="250" />

  <text 
   top_pad="7"
   enabled="false"
   follows="top|left"
   layout="topleft"
   height="15" 
   left="5" 
   name="anyone_label" >
    Anyone
  </text>
  
  <check_box 
   top_pad="5"
   enabled="true" 
   follows="left|top" 
   layout="topleft"
   height="14" 
   initial_value="false"
   label="Allow Navigation &amp; Interactivity" 
   left="5" 
   mouse_opaque="true"
   name="perms_anyone_interact" 
   radio_style="false" 
   width="250" />

  <check_box 
   top_pad="1"
   enabled="true" 
   follows="left|top" 
   layout="topleft"
   height="14" 
   initial_value="false"
   label="Show Control Bar" 
   left="5" 
   mouse_opaque="true"
   name="perms_anyone_control" 
   radio_style="false" 
   width="250" />

</panel>
