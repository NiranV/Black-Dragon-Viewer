<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 legacy_header_height="18"
 can_minimize="false"
 height="165"
 layout="topleft"
 name="Give Money"
 save_rect="true"
 width="250">
    <string
     name="payee_group">
Pay Group
    </string>
    <string
     name="payee_resident">
Pay Resident
    </string>
    <text
     follows="right|left"
     height="16"
     layout="topleft"
     left="6"
     top_pad="20"
     halign="left" 
     name="payee_name"
     use_ellipses="false" 
     width="230">
      Ericacita Moostopolison
    </text>
    <text
     type="string"
     length="1"
     follows="left|top"
     halign="left"
     height="14"
     layout="topleft"
     left="10"
     name="object_name_label"
     top_pad="3"
     width="180">
        Via object:
    </text>
   <icon
     height="16"
     width="16"
     image_name="Inv_Object"
     mouse_opaque="true"
     name="icon_object"
     tool_tip="Objects"
     top_pad="3"
     left="10"
     />
    <text
     type="string"
     length="1"
     follows="left|top"
     height="14"
     layout="topleft"
     left_pad="4"
     name="object_name_text"
     top_delta="2"
     use_ellipses="true"
     word_wrap="false"
     width="210">
        My awesome object with a really damn long name
    </text>
   <button
     height="23"
     label="L$1"
     label_selected="L$1"
     image_hover_unselected="Toolbar_Middle_Over"
     image_selected="Toolbar_Middle_Selected"
     image_unselected="Toolbar_Middle_Off"
     layout="topleft"
     left="10"
     name="fastpay 1"
     top_pad="8"
     width="56" />
    <button
     height="23"
     label="L$5"
     label_selected="L$5"
     image_hover_unselected="Toolbar_Middle_Over"
     image_selected="Toolbar_Middle_Selected"
     image_unselected="Toolbar_Middle_Off"
     layout="topleft"
     left_pad="1"
     name="fastpay 5"
     width="57" />
    <button
     height="23"
     label="L$10"
     label_selected="L$10"
     image_hover_unselected="Toolbar_Middle_Over"
     image_selected="Toolbar_Middle_Selected"
     image_unselected="Toolbar_Middle_Off"
     layout="topleft"
     left_pad="1"
     name="fastpay 10"
     top_delta="0"
     width="56" />
    <button
     height="23"
     label="L$20"
     label_selected="L$20"
     image_hover_unselected="Toolbar_Middle_Over"
     image_selected="Toolbar_Middle_Selected"
     image_unselected="Toolbar_Middle_Off"
     layout="topleft"
     left_pad="1"
     top_delta="0"
     name="fastpay 20"
     width="57" />
    <text
     type="string"
     length="1"
     follows="left|top"
     height="14"
     layout="topleft"
     left="10"
     name="amount text"
     top_pad="7"
     width="180">
        Or, choose amount:
    </text>
    <line_editor
     border_style="line"
     follows="left|top|right"
     height="21"
     top_delta="-3"
     layout="topleft"
     left="159"
     max_length_bytes="9"
     name="amount"
     width="80" />
    <button
     enabled="false"
     height="23"
     label="Pay"
     label_selected="Pay"
     image_hover_unselected="Toolbar_Middle_Over"
     image_selected="Toolbar_Middle_Selected"
     image_unselected="Toolbar_Middle_Off"
     layout="topleft"
     left="10"
     name="pay btn"
     top_pad="4"
     width="114" />
    <button
     height="23"
     label="Cancel"
     label_selected="Cancel"
     image_hover_unselected="Toolbar_Middle_Over"
     image_selected="Toolbar_Middle_Selected"
     image_unselected="Toolbar_Middle_Off"
     layout="topleft"
     left_pad="1"
     name="cancel btn"
     width="114" />
</floater>
