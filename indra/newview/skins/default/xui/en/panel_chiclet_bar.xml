<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 background_visible="false"
 chrome="true"
 follows="left|top|right"
 height="55"
 layout="topleft"
 mouse_opaque="false"
 name="chiclet_bar"
 width="1310">
  <layout_stack
     clip="false"
     follows="all"
     height="55"
     layout="topleft"
     left="0"
     mouse_opaque="false"
     name="toolbar_stack"
     orientation="vertical"
     top="0"
     width="1310">
    <layout_panel
         auto_resize="false"
         follows="top|left"
         height="26"
         min_height="26"
         layout="topleft"
         name="notification_well_panel"
         width="75">
      <chiclet_notification
             follows="right|top|left"
             height="26"
             layout="topleft"
             left="0"
             max_displayed_count="99"
             name="notification_well"
             top="0"
             width="50">
        <button
                 auto_resize="false"
                 follows="top|right"
                 layout="topleft"
                 halign="left"
                 height="26"
                 image_pressed="Sys_BG_Pressed"
                 image_pressed_selected="Sys_BG_Pressed"
                 image_selected="Sys_BG"
                 image_unselected="Sys_BG"
                 left="0"
                 top_delta="0"
                 name="Unread"
                 tool_tip="Notifications"
                 width="48">
          <init_callback
                     function="Button.SetDockableFloaterToggle"
                     parameter="notification_well_window" />
        </button>
      </chiclet_notification>
    </layout_panel>
    <layout_panel
         follows="left|right"
         height="29"
         layout="topleft"
         min_width="95"
         mouse_opaque="false"
         name="chiclet_list_panel"
         auto_resize="false"
         top="0"
         width="189">
      <chiclet_panel
             chiclet_padding="4"
             follows="left|top|right"
             height="29"
             layout="topleft"
             left="1"
             min_width="95"
             mouse_opaque="false"
             name="chiclet_list"
             top="0"
             width="189">
        <button
                 auto_resize="false"
                 follows="top|right"
                 height="29"
                 image_hover_selected="SegmentedBtn_Left_Over"
                 image_hover_unselected="SegmentedBtn_Left_Over"
                 image_overlay="Arrow_Small_Left"
                 image_pressed="SegmentedBtn_Left_Press"
                 image_pressed_selected="SegmentedBtn_Left_Press"
                 image_selected="SegmentedBtn_Left_Off"
                 image_unselected="SegmentedBtn_Left_Off"
                 layout="topleft"
                 name="chicklet_left_scroll_button"
                 tab_stop="false"
                 top="0"
                 visible="false"
                 width="12" />
        <button
                 auto_resize="false"
                 follows="top|right"
                 height="29"
                 image_hover_selected="SegmentedBtn_Right_Over"
                 image_hover_unselected="SegmentedBtn_Right_Over"
                 image_overlay="Arrow_Small_Right"
                 image_pressed="SegmentedBtn_Right_Press"
                 image_pressed_selected="SegmentedBtn_Right_Press"
                 image_selected="SegmentedBtn_Right_Off"
                 image_unselected="SegmentedBtn_Right_Off"
                 layout="topleft"
                 name="chicklet_right_scroll_button"
                 tab_stop="false"
                 top="0"
                 visible="false"
                 width="12" />
      </chiclet_panel>
    </layout_panel>
  </layout_stack>
</panel>
