<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 background_visible="false"
 chrome="true"
 follows="left|top|right"
 height="55"
 layout="topleft"
 mouse_opaque="false"
 name="chiclet_bar"
 width="1310">
  <layout_stack
     clip="false"
     follows="all"
     height="55"
     layout="topleft"
     left="0"
     mouse_opaque="false"
     name="toolbar_stack"
     orientation="vertical"
     top="0"
     width="1310">
    <layout_panel
         follows="left|right"
         height="54"
         layout="topleft"
         min_width="95"
         mouse_opaque="false"
         name="chiclet_list_panel"
         auto_resize="true"
         top="0"
         width="189">
      <chiclet_panel
             chiclet_padding="2"
             follows="left|top|right"
             height="29"
             layout="topleft"
             left="1"
             min_width="95"
             mouse_opaque="false"
             name="chiclet_list"
             top="0"
             width="189">
        <button
                 auto_resize="false"
                 follows="top|right"
                 height="26"
                 image_hover_selected="Window_NoTitle_Background"
                 image_hover_unselected="Window_NoTitle_Background"
                 image_pressed="Window_NoTitle_Background"
                 image_overlay="Arrow_Left"
                 image_pressed_selected="Window_NoTitle_Background"
                 image_selected="Window_NoTitle_Background"
                 image_unselected="Window_NoTitle_Background"
                 layout="topleft"
                 name="chicklet_left_scroll_button"
                 tab_stop="false"
                 top="3"
                 width="15" />
        <button
                 auto_resize="false"
                 follows="top|right"
                 height="26"
                 image_hover_selected="Window_NoTitle_Background"
                 image_hover_unselected="Window_NoTitle_Background"
                 image_overlay="Arrow_Right"
                 image_pressed="Window_NoTitle_Background"
                 image_pressed_selected="Window_NoTitle_Background"
                 image_selected="Window_NoTitle_Background"
                 image_unselected="Window_NoTitle_Background"
                 layout="topleft"
                 name="chicklet_right_scroll_button"
                 tab_stop="false"
                 top="3"
                 width="15" />
      </chiclet_panel>
    </layout_panel>
  </layout_stack>
</panel>
