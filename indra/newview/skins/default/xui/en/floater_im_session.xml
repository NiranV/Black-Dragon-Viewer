<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 background_visible="true"
 height="355"
 help_topic="floater_im_box"
 layout="topleft"
 name="panel_im"
 can_dock="false"
 can_minimize="true"
 can_close="true"
 save_rect="true"
 visible="false"
 width="394"
 can_resize="true"
 can_tear_off="false"
 min_height="190"
 positioning="relative">
    <floater.string name="call_btn_start">Conv_toolbar_open_call</floater.string>
    <floater.string name="call_btn_stop">Conv_toolbar_hang_up</floater.string>
    <floater.string
     name="collapseline_icon"
     value="Conv_collapse_to_one_line"/>
    <floater.string
     name="expandline_icon"
     value="Conv_expand_one_line"/>
    <floater.string
     name="collapse_icon"
     value="Conv_toolbar_expand"/>
    <floater.string
     name="expand_icon"
     value="Conv_toolbar_collapse"/>
    <floater.string
     name="tear_off_icon"
     value="Conv_toolbar_arrow_ne"/>
    <floater.string
     name="return_icon"
     value="Conv_toolbar_arrow_sw"/>
    <floater.string
     name="participant_added"
     value="[NAME] was invited to the conversation."/>
    <floater.string
     name="multiple_participants_added"
     value="[NAME] were invited to the conversation."/>
    <floater.string
     name="tooltip_to_separate_window"
     value="Move this conversation to a separate window"/>
    <floater.string
     name="tooltip_to_main_window"
     value="Move this conversation back to main window"/>
    <floater.string
     name="start_call_button_tooltip"
     value="Open voice connection"/>
    <floater.string
     name="end_call_button_tooltip"
     value="Close voice connection"/>
    <floater.string
     name="expcol_button_not_tearoff_tooltip"
     value="Collapse this pane"/>
    <floater.string
     name="expcol_button_tearoff_and_expanded_tooltip"
     value="Collapse participant list"/>
    <floater.string
     name="expcol_button_tearoff_and_collapsed_tooltip"
     value="Expand participant list"/>
    <view
     follows="all"
     layout="topleft"
     name="contents_view"
     top="0"
     height="355"
     left="0"
     right="-1"
     bottom="-4">
     <icon
	  follows="all"
	  height="220"
	  color="1 1 1 1"
	  image_name="Panel_Background"
	  layout="topleft"
	  name="vicon"
	  mouse_opaque="false"
	  visible="true"
	  width="385"
   bottom="-1"
	  top="0"
	  left="5"/>
        <layout_stack
         animate="false" 
         default_tab_group="2"
         follows="all"
         right="-4"
         height="349"
         border_size="0"
         top="1"
         bottom="-3"
         left="7"
         layout="topleft"
         orientation="vertical"
         name="main_stack"
         tab_group="1">
            <layout_panel
             auto_resize="false"
             bg_visible="true"
             name="toolbar_panel"
             height="26"
             right="-1"
             left="1">
               <layout_stack
                 default_tab_group="2"
                 follows="all"
                 orientation="horizontal"
                 name="button_stack"
                 tab_group="1"
                 height="26"
                 border_size="0"
                 top="0"
                 left="0">
                    <layout_panel
                     name="view_options_btn_panel"
                     width="32"
                     height="26"
                     auto_resize="false">
                <menu_button
                 menu_filename="menu_im_session_showmodes.xml"
                 follows="top|left"
                 height="25"
                 image_hover_unselected="Toolbar_Middle_Over"
                 image_overlay="Conv_toolbar_sort"
                 image_selected="Toolbar_Middle_Selected"
                 image_unselected="Toolbar_Middle_Off"
                 layout="topleft"
                 left="0"
                 name="view_options_btn"
                 tool_tip="View/sort options"
                 top="1"
                 width="31" />
                    </layout_panel>
                    <layout_panel
                     name="gear_btn_panel"
                     width="32"
                     height="27"
                     visible="false"
                     auto_resize="false">
                <menu_button
                 menu_filename="menu_im_conversation.xml"
                 follows="top|left"
                 height="25"
                 image_hover_unselected="Toolbar_Middle_Over"
                 image_overlay="OptionsMenu_Off"
                 image_selected="Toolbar_Middle_Selected"
                 image_unselected="Toolbar_Middle_Off"
                 layout="topleft"
                 top="1"
                 left="0"
                 name="gear_btn"
                 tool_tip="Actions on selected person"
                 width="31"/>
                </layout_panel>
                    <layout_panel
                     name="add_btn_panel"
                     width="32"
                     height="27"
                     auto_resize="false">
                <button
                 enabled="false"
                 follows="top|left"
                 height="25"
                 image_hover_unselected="Toolbar_Middle_Over"
                 image_overlay="Conv_toolbar_add_person"
                 image_selected="Toolbar_Middle_Selected"
                 image_unselected="Toolbar_Middle_Off"
                 layout="topleft"
                 top="1"
                 left="0"
                 name="add_btn"
                 tool_tip="Add someone to this conversation"
                 width="31"/>
                </layout_panel>
						  <layout_panel
                     name="chat_history_btn_panel"
                     width="32"
                     height="27"
							visible="false"
                     auto_resize="false">
                <button
                 follows="top|left"
                 height="25"
                 image_hover_unselected="Toolbar_Middle_Over"
                 image_overlay="Conv_toolbar_history"
                 image_selected="Toolbar_Middle_Selected"
                 image_unselected="Toolbar_Middle_Off"
                 layout="topleft"
                 top="1"
                 left="0"
                 name="chat_history_btn"
                 tool_tip="Open chat history"
                 width="31">
						<button.commit_callback
						function="Avatar.GearDoToSelected"
						parameter="chat_history"/>
					 </button>
                </layout_panel>
						  <layout_panel
                     name="nearby_history_btn_panel"
                     width="32"
                     height="27"
							visible="false"
                     auto_resize="false">
                <button
                 follows="top|left"
                 height="25"
                 image_hover_unselected="Toolbar_Middle_Over"
                 image_overlay="Conv_toolbar_history"
                 image_selected="Toolbar_Middle_Selected"
                 image_unselected="Toolbar_Middle_Off"
                 layout="topleft"
                 top="1"
                 left="0"
                 name="local_history_btn"
                 tool_tip="Open local chat history"
                 width="31"/>
                </layout_panel>
                    <layout_panel
                     name="voice_call_btn_panel"
                     width="32"
                     height="27"
                     auto_resize="false">
                <button
                 follows="top|left"
                 height="25"
                 image_hover_unselected="Toolbar_Middle_Over"
                 image_overlay="Conv_toolbar_open_call"
                 image_selected="Toolbar_Middle_Selected"
                 image_unselected="Toolbar_Middle_Off"
                 layout="topleft"
                 top="1"
                 left="0"
                 name="voice_call_btn"
                 tool_tip="Open voice connection"
                 width="31"/>
                </layout_panel>
                    <layout_panel
                     name="dummy_panel"
                     width="32"
                     height="26"
                     auto_resize="true">
                <output_monitor
                 auto_update="true"
                 follows="top|left"
                 draw_border="false"
                 height="16"
                 layout="topleft"
                 top="6"
                 left="7"
                 mouse_opaque="true"
                 name="speaking_indicator"
                 visible="false"
                 width="20" />
                <icon
                  follows="top|left|right"
                  height="25"
                  name="bg"
                  mouse_opaque="false"
                  image_name="Toolbar_Middle_Off"
                  left="0"
                  top="1"
                  width="31" />
                </layout_panel>
                    <layout_panel
                     name="close_btn_panel"
                     width="32"
                     height="26"
                     auto_resize="false">
                <button
                 follows="left|top"
                 height="25"
                 image_hover_unselected="Toolbar_Middle_Over"
                 image_overlay="Conv_toolbar_close"
                 image_selected="Toolbar_Middle_Selected"
                 image_unselected="Toolbar_Middle_Off"
                 layout="topleft"
                 top="1"
                 left="0"
                 name="close_btn"
                 tool_tip="End this conversation"
                 width="31" />
                </layout_panel>
                    <layout_panel
                     name="expand_collapse_btn_panel"
                     width="32"
                     height="26"
                     auto_resize="false">
                <button
                 follows="left|top"
                 height="25"
                 image_hover_unselected="Toolbar_Middle_Over"
                 image_overlay="Conv_toolbar_collapse"
                 image_selected="Toolbar_Middle_Selected"
                 image_unselected="Toolbar_Middle_Off"
                 layout="topleft"
                 top="1"
                 left="0"
                 name="expand_collapse_btn"
                 tool_tip="Collapse/Expand this pane"
                 width="31" />
                </layout_panel>
                    <layout_panel
                     name="tear_off_panel"
                     width="32"
                     height="26"
                     auto_resize="false">
                <button
                 follows="right|top"
                 height="25"
                 image_hover_unselected="Toolbar_Middle_Over"
                 image_overlay="Conv_toolbar_arrow_ne"
                 image_selected="Toolbar_Middle_Selected"
                 image_unselected="Toolbar_Middle_Off"
                 layout="topleft"
                 left="0"
                 name="tear_off_btn"
                 top="1"
                 width="31" />
                    </layout_panel>
               </layout_stack>
            </layout_panel>
            <layout_panel
             name="body_panel"
             follows="all"
             height="300"
             layout="topleft">
                <layout_stack
                 default_tab_group="2"
                 follows="all"
                 orientation="horizontal"
                 name="im_panels"
                 tab_group="1"
                 border_size="0"
                 top="0"
                 height="300"
                 right="-1"
                 left="0">
                    <layout_panel
                     name="speakers_list_panel"
                     expanded_min_dim="115"
                     min_dim="0"
                     width="150" 
                     user_resize="true"
                     auto_resize="false" 
                     bottom="-1" />
                    <layout_panel
                     default_tab_group="3"
                     tab_group="2"
                     follows="all"
                     layout="topleft"
                     height="300"
                     name="right_part_holder"
                     min_width="221">
                        <layout_stack
                         animate="true" 
                         follows="all"
                         border_size="0"
                         orientation="vertical"
                         name="translate_and_chat_stack"
                         tab_group="1"
                         height="305"
                         top="0"
                         left="0">
                            <layout_panel
                             auto_resize="false"
                             height="20"
                             visible="false"
                             name="translate_chat_checkbox_lp">
                                <check_box
                                 top="3"
                                 control_name="TranslateChat"
                                 enabled="true"
                                 height="16"
                                 label="Translate chat"
                                 left="1"
                                 name="translate_chat_checkbox"
                                 width="230" />
                            </layout_panel>
                            <layout_panel
                             name="chat_holder">
                                <chat_history
                                 font="SansSerifSmall"
                                 follows="all"
                                 name="chat_history"
                                 parse_highlights="true"
                                 parse_urls="true"
                                 left="1"
                                 top="0" />
                            </layout_panel>
                        </layout_stack>
                    </layout_panel>
                </layout_stack>
            </layout_panel>
            <layout_panel
             height="25"
				 min_height="25"
             auto_resize="false"
             name="chat_layout_panel">
                <layout_stack
                 animate="false"
                 default_tab_group="2"
                 follows="all"
                 border_size="0"
                 orientation="horizontal"
                 name="input_panels"
                 top="0"
					  height="25"
                 left="0"
                 right="-1">
                    <layout_panel
                     name="input_editor_layout_panel"
                     auto_resize="true"
                     user_resize="false"
                     bottom="0">
                     
                     <icon
                  follows="all"
                  height="25"
                  name="bg"
                  mouse_opaque="false"
                  image_name="Toolbar_Middle_Off"
                  left="0"
                  top="0"
                  width="379" />
                     <avatar_icon
                         follows="left|bottom"
                         name="avatar_icon"
                         height="21"
                         default_icon_name="Generic_Person"
                         layout="topleft"
                         left="2"
                         bottom="-2"
                         visible="false"
                         width="21" />
                        <group_icon
                         follows="left|bottom"
                         name="group_chat_icon"
                         height="21"
                         default_icon_name="Generic_Group"
                         layout="topleft"
                         left="2"
                         bottom="-2"
                         visible="false"
                         width="21" />
                        <icon
                         follows="left|bottom"
                         height="21"
                         image_name="Nearby_chat_icon"
                         layout="topleft"
                         left="2"
                         bottom="-2"
                         name="nearby_chat_icon"
                         visible="false"
                         width="21"/>
                        <chat_editor
                         layout="topleft"
                         follows="bottom|left|right"
                         font="SansSerifBig"
                         height="21"
                         expand_lines_count="5"
                         is_expandable="false"
                         text_tentative_color="0.4 0.4 0.4 1"
                         cursor_color="TextCursorColor"
                         text_color="Black"
                         name="chat_editor"
                         max_length="1023"
                         spellcheck="true"
                         tab_group="3"
                         v_pad="2"
                         line_spacing.pixels="-1" 
                         bottom="-2"
                         left_delta="24"
                         width="352"
                         wrap="true" />
                    </layout_panel>
                    <layout_panel
                     auto_resize="false"
                     user_resize="false"
                     name="input_button_layout_panel"
                     height="25"
                     top="0"
                     width="32"
                     bottom="-1">
                        <button
                         layout="topleft"
                         left="1"
                         top="0"
                         height="25"
                         width="31"
                         follows="left|top|bottom"
                         image_hover_unselected="Toolbar_Middle_Over"
                         image_overlay="Conv_expand_one_line"
                         image_selected="Toolbar_Middle_Selected"
                         image_unselected="Toolbar_Middle_Off"
                         name="minz_btn"
                         tool_tip="Shows/hides message panel" />
                    </layout_panel>
                </layout_stack>
            </layout_panel>
        </layout_stack>
    </view>
</floater>
