<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<toggleable_menu
 layout="topleft"
 name="Settings">
    <menu_item_call
     label="Default"
     layout="topleft"
     name="default">
        <on_click function="Settings.SetRendering" parameter="default"/>
    </menu_item_call>
    <menu_item_call
     label="Always render"
     layout="topleft"
     name="always_render">
        <on_click function="Settings.SetRendering" parameter="always"/>
    </menu_item_call>
    <menu_item_call
     label="Never render"
     layout="topleft"
     name="never_render">
        <on_click function="Settings.SetRendering" parameter="never"/>
    </menu_item_call>  
</toggleable_menu>
