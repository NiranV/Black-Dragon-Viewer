   top="0"
   follows="all"
   height="768"
   mouse_opaque="false"
   name="Menu Holder"
   width="1024"/>
   <tooltip_view
   top="0"
   follows="all"
   height="768"
   mouse_opaque="false"
   name="tooltip view"
   tab_group="-2"
   width="1024"/>
