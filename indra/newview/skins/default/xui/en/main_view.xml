               follows="left|right|bottom"
               name="status_bar_container"
               tab_stop="false"
               height="50"
               left="0"
               top="-57"
               width="1024"
               visible="true">
                  <view
                  mouse_opaque="false"
                  follows="left|bottom"
                  name="menu_bar_holder"
                  left="10"
                  top="2"
                  width="60"
                  height="49"/>
