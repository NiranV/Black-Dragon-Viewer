         follows="all"
         height="768"
         left="0"
         mouse_opaque="false"
         name="world_view_rect"
         width="1024"/>
         <layout_stack
         name="topbar"
         orientation="vertical"
         left="0"
         top="0"
         width="1024"
         height="80"
         follows="top|left|right"
         mouse_opaque="false">
            <layout_panel
            name="bar_panel"
            height="50"
            min_height="24"
            auto_resize="true"
            width="1024"
            mouse_opaque="false">
