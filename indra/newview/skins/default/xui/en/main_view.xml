            </layout_panel>
            <layout_panel
            name="spacer_panel"
            invisibility_control="ShowNavbarFavoritesPanel"
            height="20"
            min_height="20"
            width="1024"
            mouse_opaque="false"/>
         </layout_stack>
         <panel
         top="0"
         left="0"
         follows="all"
         height="768"
         mouse_opaque="false"
         name="login_panel_holder"
         width="1024"/>
         <debug_view
         follows="all"
         left="0"
         top="0"
         mouse_opaque="false"
         height="768"
         name="DebugView"
         width="1024"/>
         <layout_stack
         name="world"
         orientation="vertical"
         left="0"
         top="26"
         width="1024"
         height="742"
         follows="all"
         mouse_opaque="false">
            <layout_panel
            name="spacer_panel"
            visibility_control="ShowNavbarFavoritesPanel"
            height="17"
            min_height="17"
            width="1024"
            mouse_opaque="false"/>
            <layout_panel
            name="spacer_panel"
            height="744"
            auto_resize="true"
            tab_stop="false" 
            width="1024"
            mouse_opaque="false">
            <floater_view
            follows="all"
            height="744"
            left="0"
            mouse_opaque="false"
            name="Floater View"
            tab_group="-1"
            tab_stop="false"
            top="0"
            width="1024"/>
            <panel
            name="toolbar_view_holder"
            follows="all"
            layout="topleft"
            height="744"
            width="1024"
            top="0"
            left="0"
            mouse_opaque="false"
            tab_stop="false"/>
            </layout_panel>
         </layout_stack>
      </layout_panel>
   </layout_stack>
   <panel
   top="0"
   follows="all"
   mouse_opaque="false"
   left="0"
   name="snapshot_floater_view_holder" 
   width="1024"
   height="798">
