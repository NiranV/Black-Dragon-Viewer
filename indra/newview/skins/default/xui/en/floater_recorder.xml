<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
positioning="centered"
legacy_header_height="18"
height="465"
layout="topleft"
name="floater_recorder"
save_rect="true"
can_resize="true"
title="Agent and Camera Recorder"
width="449">
	<tab_container
  follows="all"
  halign="center"
  height="445"
  layout="topleft"
  left="6"
  name="Picks Tabs"
  tab_min_width="118"
  tab_max_width="157"
  tab_height="20"
  tab_group="1"
  tab_position="top"
  top="20"
  width="449">
	<panel
    follows="all"
	background_visible="false"
    height="425"
    layout="topleft"
    left="0"
    title="Recorder"
    name="arc_panel"
    top="0"
    width="449">
		<icon
	follows="top|left|right"
	height="419"
	color="1 1 1 1"
	image_name="Panel_Background"
	layout="topleft"
	name="vicon"
	mouse_opaque="false"
	visible="true"
	width="435"
	top="0"
	left="0"/>
		<scroll_container
		color="Transparent"
		follows="all"
		layout="topleft"
		mouse_opaque="false"
		left="3"
		name="recorder_scroll_container"
		opaque="true"
		height="394"
		width="430"
		top="2">
			<panel
			follows="top|left"
			height="375"
			background_visible="false"
			layout="topleft"
			mouse_opaque="false"
			left="1"
			name="recorder_panel"
			top="0"
			width="429">
		<scroll_list
 column_padding="2"
 draw_heading="true"
 height="395"
 follows="left|top|right"
 layout="topleft"
 left="0"
 width="429"
 multi_select="true"
 name="recorder_scroll"
 top="0">
    <scroll_list.columns
     label="#"
     name="number"
   relative_width="0.06"  />
	<scroll_list.columns
     label="pos x"
     name="pos_x"
   relative_width="0.06"  />
	<scroll_list.columns
     label="pos y"
     name="pos_y"
   relative_width="0.06"  />
	<scroll_list.columns
     label="pos z"
     name="pos_z"
   relative_width="0.06"  />
	<scroll_list.columns
     label="rot x"
     name="rot_x_x"
   relative_width="0.0"  />
	<scroll_list.columns
     label="rot y"
     name="rot_x_y"
   relative_width="0.0"  />
	<scroll_list.columns
     label="rot z"
     name="rot_x_z"
   relative_width="0.0"  />
	<scroll_list.columns
     label="rot x"
     name="rot_y_x"
   relative_width="0.0"  />
	<scroll_list.columns
     label="rot y"
     name="rot_y_y"
   relative_width="0.0"  />
	<scroll_list.columns
     label="rot z"
     name="rot_y_z"
   relative_width="0.0"  />
	<scroll_list.columns
     label="rot x"
     name="rot_z_x"
   relative_width="0.0"  />
	<scroll_list.columns
     label="rot y"
     name="rot_z_y"
   relative_width="0.0"  />
	<scroll_list.columns
     label="rot z"
     name="rot_z_z"
   relative_width="0.0"  />
	<scroll_list.columns
     label="target x"
     name="target_x"
   relative_width="0.0"  />
	<scroll_list.columns
     label="target y"
     name="target_y"
   relative_width="0.0"  />
	<scroll_list.columns
     label="target z"
     name="target_z"
   relative_width="0.0"  />
<scroll_list.columns
     label="view"
     name="view"
   relative_width="0.05"  />
<scroll_list.columns
     label="type"
     name="type"
   relative_width="0.0"  />
<scroll_list.columns
     label="time"
     name="time"
   relative_width="0.05"  />
</scroll_list>
			</panel>
		</scroll_container>
		<button
 height="21"
 follows="top|left"
 layout="topleft"
	is_toggle="true"
 label="Record"
	label_selected="Stop"
 image_hover_unselected="Toolbar_Middle_Over"
	 image_selected="Toolbar_Middle_Selected"
	 image_unselected="Toolbar_Middle_Off"
 name="record_btn"
 width="50"
 top_pad="1"
 left_delta="0">
   <button.commit_callback
   function="Recorder.Command"
   parameter="Record"/>
 </button>
		<button
 height="21"
 follows="top|left"
 layout="topleft"
	is_toggle="true"
 label="Play"
	label_selected="Stop"
 image_hover_unselected="Toolbar_Middle_Over"
	 image_selected="Toolbar_Middle_Selected"
	 image_unselected="Toolbar_Middle_Off"
 name="playback_btn"
 width="35"
 top_delta="0"
 left_pad="1">
   <button.commit_callback
   function="Recorder.Command"
   parameter="Playback"/>
 </button>
		<button
 height="21"
 follows="top|left"
 layout="topleft"
	is_toggle="true"
 label="Loop"
	label_selected="Loop"
 image_hover_unselected="Toolbar_Middle_Over"
	 image_selected="Toolbar_Middle_Selected"
	 image_unselected="Toolbar_Middle_Off"
 name="loop_btn"
 width="40"
 top_delta="0"
 left_pad="1">
   <button.commit_callback
   function="Recorder.Command"
   parameter="Loop"/>
 </button>
		<button
 height="21"
 follows="top|left"
 layout="topleft"
 label="Refresh"
 image_hover_unselected="Toolbar_Middle_Over"
	 image_selected="Toolbar_Middle_Selected"
	 image_unselected="Toolbar_Middle_Off"
 name="refresh_btn"
 width="52"
 top_delta="0"
 left_pad="1">
   <button.commit_callback
   function="Recorder.Command"
   parameter="Refresh"/>
 </button>
		<button
 height="21"
 follows="top|left"
 layout="topleft"
 label="Waypoint"
 image_hover_unselected="Toolbar_Middle_Over"
	 image_selected="Toolbar_Middle_Selected"
	 image_unselected="Toolbar_Middle_Off"
 name="add_waypoint_btn"
 width="62"
 top_delta="0"
 left_pad="1">
   <button.commit_callback
   function="Recorder.Command"
   parameter="Save Camera"/>
 </button>
		<button
 height="21"
 follows="top|left"
 layout="topleft"
 label="Pos"
 image_hover_unselected="Toolbar_Middle_Over"
	 image_selected="Toolbar_Middle_Selected"
	 image_unselected="Toolbar_Middle_Off"
 name="save_position_btn"
 width="32"
 top_delta="0"
 left_pad="1">
   <button.commit_callback
   function="Recorder.Command"
   parameter="Save Position"/>
 </button>
		<button
 height="21"
 follows="top|left"
 layout="topleft"
 label="Rot"
 image_hover_unselected="Toolbar_Middle_Over"
	 image_selected="Toolbar_Middle_Selected"
	 image_unselected="Toolbar_Middle_Off"
 name="save_rotation_btn"
 width="32"
 top_delta="0"
 left_pad="1">
   <button.commit_callback
   function="Recorder.Command"
   parameter="Save Rotation"/>
 </button>
		<button
 height="21"
 follows="top|left"
 layout="topleft"
 label="Delete"
 image_hover_unselected="Toolbar_Middle_Over"
	 image_selected="Toolbar_Middle_Selected"
	 image_unselected="Toolbar_Middle_Off"
 name="remove_btn"
 width="45"
 top_delta="0"
 left_pad="1">
   <button.commit_callback
   function="Recorder.Command"
   parameter="Remove"/>
 </button>
		<button
 height="21"
 follows="top|left"
 layout="topleft"
 label="Save"
 image_hover_unselected="Toolbar_Middle_Over"
	 image_selected="Toolbar_Middle_Selected"
	 image_unselected="Toolbar_Middle_Off"
 name="export_btn"
 width="35"
 top_delta="0"
 left_pad="1">
   <button.commit_callback
   function="Recorder.Command"
   parameter="Export"/>
 </button>
		<button
 height="21"
 follows="top|left"
 layout="topleft"
 label="Load"
 image_hover_unselected="Toolbar_Middle_Over"
	 image_selected="Toolbar_Middle_Selected"
	 image_unselected="Toolbar_Middle_Off"
 name="import_btn"
 width="36"
 top_delta="0"
 left_pad="1">
   <button.commit_callback
   function="Recorder.Command"
   parameter="Import"/>
 </button>
		
	</panel>
	</tab_container>
</floater>
