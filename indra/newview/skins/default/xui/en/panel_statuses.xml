<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<!-- Width and height of this panel should be synchronized with "panel_modes" in the floater_moveview.xml-->
<panel
 height="23"
 layout="topleft"
 follows="all"
 name="panel_statuses"
 mouse_opaque="false"
 visible="true"
 width="768">
 <layout_stack
 follows="all"
 height="23"
 layout="topleft"
 left="0"
 border_size="1"
 mouse_opaque="false"
 name="status_stack"
 orientation="horizontal"
 top="2"
 width="768">
  <layout_panel
  follows="all"
  layout="topleft"
  height="21"
  mouse_opaque="false"
  auto_resize="true"
  name="stand_layout"
  width="10"/>
  <layout_panel
  follows="all"
  layout="topleft"
  height="23"
  auto_resize="false"
  background_visible="true"
  bg_alpha_color="0 0 0 0.5"
  visible="false"
  name="sitting_layout"
  width="110">
    <button
     follows="left|bottom"
     height="22"
     label="Sitting"
     layout="topleft"
     left="0"
     name="sitting_btn"
     tool_tip="You are currently sitting."
     image_overlay_alignment="left"
     image_overlay="Command_Move_Icon"
     image_hover_unselected="Toolbar_Middle_Over"
     image_selected="Toolbar_Middle_Selected"
     image_unselected="Toolbar_Middle_Off"
     top="0"
     width="110" />
  </layout_panel>
  <layout_panel
  follows="all"
  layout="topleft"
  height="23"
