<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 legacy_header_height="18"
 can_resize="true"
 default_tab_group="1"
 height="370"
 layout="topleft"
 min_height="190"
 min_width="285"
 name="objectcontents"
 save_rect="true"
 title="Object Contents"
 width="300">
		<icon
		follows="all"
		height="300"
		color="1 1 1 1"
		image_name="Panel_Background"
		layout="topleft"
		name="vicon"
		mouse_opaque="false"
		visible="true"
		width="283"
		top="37"
		bottom="-10"
		left="8"/>
		<text
		type="string"
		length="1"
		follows="left|top|right"
		font="SansSerif"
		text_color="White"
		height="16"
		layout="topleft"
		left="10"
		name="object_name"
		parse_urls="false"
		top="20"
		width="282">
		[DESC]:
		</text>
		<panel_inventory_object
		background_visible="false"
		draw_border="false"
		follows="all"
		height="296"
		layout="topleft"
		left="10"
		name="object_contents"
		top_pad="3"
		width="279" />
		<button
		image_hover_unselected="Toolbar_Middle_Over"
		image_selected="Toolbar_Middle_Selected"
		image_unselected="Toolbar_Middle_Off"
		follows="bottom|left"
		height="23"
		label="Add"
		tool_tip="Add these contents to your current outfit"
		layout="topleft"
		left="10"    
		name="copy_and_wear_button" 	
		bottom="-11"	
		width="71">
			<button.commit_callback
			function="OpenObject.MoveAndWear" />
		</button>
		<button
		image_hover_unselected="Toolbar_Middle_Over"
		image_selected="Toolbar_Middle_Selected"
		image_unselected="Toolbar_Middle_Off"
		follows="bottom|left"
		height="23"
		label="Replace"
		tool_tip="Replace your current outfit with these contents"
		layout="topleft"
		left_pad="1"
		name="copy_and_replace_button"
		width="65">
			<button.commit_callback
			function="OpenObject.ReplaceOutfit" />
		</button>  
		<button
		image_hover_unselected="Toolbar_Middle_Over"
		image_selected="Toolbar_Middle_Selected"
		image_unselected="Toolbar_Middle_Off"
		follows="bottom|left"
		height="23"
		label="Copy only"
		tool_tip="Copy the contents to your inventory"
		layout="topleft"
		left_pad="1"
		name="copy_to_inventory_button"
		tab_group="1"
		top_delta="0"
		width="70">
			<button.commit_callback
			function="OpenObject.MoveToInventory" />
		</button>
		<button
		image_hover_unselected="Toolbar_Middle_Over"
		image_selected="Toolbar_Middle_Selected"
		image_unselected="Toolbar_Middle_Off"
		follows="bottom|left"
		height="23"
		label="Cancel"
		layout="topleft"
		left_pad="1"
		name="cancel_button"
		width="70">
			<button.commit_callback
			function="OpenObject.Cancel" />
		</button>
</floater>
