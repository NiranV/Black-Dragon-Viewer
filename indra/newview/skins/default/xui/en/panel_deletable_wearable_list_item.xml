<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 follows="top|right|left"
 height="18"
 layout="topleft"
 left="0"
 name="deletable_wearable_item"
 top="0"
 width="380">
    <icon
     follows="top|right|left"
     height="17"
     image_name="ListItem_Over"
     layout="topleft"
     left="0"
     name="hovered_icon"
     top="1"
     visible="false"
     width="380" />
    <icon
     height="17"
     follows="top|right|left"
     image_name="ListItem_Select"
     layout="topleft"
     left="0"
     name="selected_icon"
     top="1"
     visible="false"
     width="380" />
    <button 
     name="btn_delete"
     layout="topleft"
     follows="top|left"
     image_unselected="TrashItem_Off"
     image_selected="TrashItem_Off"
     top="2"
     left="2"
     height="18"
     width="18"
     tab_stop="false"
     tool_tip="Remove from outfit"/>
    <icon
     height="16"
     follows="top|left"
     image_name="Inv_Object"
     layout="topleft"
     left="20"
     name="item_icon"
     top="4"
     width="16" />
    <text
     follows="left|right"
     height="14"
     layout="topleft"
     left_pad="5"
     parse_urls="false"
     use_ellipses="true"
     name="item_name"
     text_color="white"
     top="3"
     value="..."
     width="337" />
</panel>
