<?xml version="1.0" encoding="utf-8" standalone="yes" ?>

<panel
  layout="topleft"
  top="0"
  left="0"
  width="316"
  height="500"
  follows="all">
  <string
    name="loading_experiences"
    value="Loading experiences..."/>
  <string
    name="no_experiences"
    value="No experiences."/>
  <string
    name="acquire"
    value="Acquire an Experience" />
  <layout_stack
    layout="topleft"
    top="0"
    left="3"
    width="311"
    height="499"
    orientation="vertical"
    follows="all">
    <layout_panel
      layout="topleft"
      auto_resize="true"
      top="0"
      left="1"
      right="-1"
      height="474"
      follows="all">
      <flat_list_view
        name="experiences_list"
        color="Transparent"
        layout="topleft"
        top="0"
        left="0"
        follows="all">
      </flat_list_view>
    </layout_panel>
    <layout_panel
      layout="topleft"
      top="0"
      left="0"
      height="24"
      min_height="24"
      follows="all"
      name="button_panel"
      visible="true">
      <button
        layout="topleft"
        left="0"
        top="0"
        follows="top|left|right"
        name="btn_action2"
        enabled="true"/>
    </layout_panel>
  </layout_stack>
</panel>
