<?xml version="1.0" encoding="utf-8" standalone="yes" ?>

<floater
 bevel_style="in"
 bg_opaque_image="Toast_Background"
 can_close="false"
 can_minimize="false"
 height="42"
 layout="topleft"
 name="floater_voice_volume"
 single_instance="true"
 sound_flags="0"
 title="Voice Chat Volume"
 visible="true"
 width="245">
 <text
  follows="top|left|right"
  font="SansSerifSmall"
  height="14"
  left="10"
  name="slider_label"
  parse_urls="false"
  top="0"
  text_color="White"
  translate="false"
  use_ellipses="true"
  value="Voice Chat"
  width="226" />
	<slider
		control_name="AudioLevelVoice"
		disabled_control="MuteAudio"
		follows="left|top"
		height="23"
		increment="0.025"
		initial_value="0.5"
		layout="topleft"
		left="5"
		top_pad="0"
		name="chat_voice_volume"
		show_text="false"
		slider_label.halign="right"
		volume="true"
		width="215">
	</slider>
	<button
		control_name="MuteVoice"
		disabled_control="MuteAudio"
		follows="top|left"
		height="16"
		image_selected="AudioMute_Off"
		image_unselected="Audio_Off"
		is_toggle="true"
		layout="topleft"
		left_pad="0"
  top_delta="4"
		name="mute_audio"
		tab_stop="false"
		width="16" />
</floater>