<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
background_visible="false"
bevel_style="in"
follows="left|top|right|bottom"
height="549"
label="Edit Pick"
layout="topleft"
left="0"
min_height="350"
name="panel_edit_pick"
top="0"
width="333">
 <panel.string
 name="location_notice">
 (will update after save)
 </panel.string>
 <button
 follows="top|left"
 height="25"
 image_overlay="BackArrow_Off"
 image_hover_unselected="Toolbar_Middle_Over"
 image_selected="Toolbar_Middle_Selected"
 image_unselected="Toolbar_Middle_Off"
 layout="topleft"
 name="back_btn"
 left="11"
 tab_stop="false"
 top="2"
 width="31"
 use_draw_context_alpha="false" />
 <icon
 follows="top|left|right"
 height="25"
 image_name="Toolbar_Middle_Off"
 layout="topleft"
 left_pad="1"
 top_delta="0"
 name="dummy_icon"
 width="280"/>
 <text
 follows="top|left|right"
 height="14"
 layout="topleft"
 left_delta="3"
 name="title"
 top="3"
 halign="right"
 value="Edit Pick"
 use_ellipses="true"
 width="267" />
 <icon
 follows="all"
 height="517"
 image_name="Panel_Background"
 layout="topleft"
 left="9"
 top="26"
 name="dummy_icon"
 width="316"/>
 <scroll_container
 opaque="false"
 follows="all"
 height="489"
 layout="topleft"
 left="11"
 top="27"
 name="profile_scroll"
 width="312">
  <panel
  name="scroll_content_panel"
  follows="top|left|right"
  layout="topleft"
  top="0"
  height="455"
  left="0"
  width="290">
  <panel
  follows="left|top|right"
  height="20"
  bg_visible="true"
  bg_alpha_color="0.10 0.43 0.77 0.28"
  layout="topleft"
  mouse_opaque="false"
  left="0"
  name="title"
  top="1"
  width="312"/>
  <text
  follows="left|top"
  type="string"
  height="10"
  layout="topleft"
  left="5"
  top_delta="4"
  name="group_settngs_label"
  width="120">
  Image:
  </text>
  <texture_picker
  fallback_image="default_land_picture.j2c"
  follows="left|top|right"
  height="197"
  layout="topleft"
  left="4"
  name="pick_snapshot"
  top_pad="11"
  width="293" />
  <panel
  follows="left|top|right"
  height="20"
  bg_visible="true"
  bg_alpha_color="0.10 0.43 0.77 0.28"
  layout="topleft"
  mouse_opaque="false"
  left="0"
  name="title"
  top_pad="-15"
  width="312"/>
  <text
  type="string"
  length="1"
  follows="left|top|right"
  height="10"
  layout="topleft"
  left="5"
  top_delta="4"
  name="Name:"
  width="120">
  Title:
  </text>
  <line_editor
  follows="left|top|right"
  font="SansSerif"
  height="23"
  layout="topleft"
  left_delta="0"
  top_pad="17"
  text_pad_left="4"
  max_length_bytes="63"
  name="pick_name"
  width="292" />
  <panel
  follows="left|top|right"
  height="20"
  bg_visible="true"
  bg_alpha_color="0.10 0.43 0.77 0.28"
  layout="topleft"
  mouse_opaque="false"
  left="0"
  name="title"
  top_pad="10"
  width="312"/>
  <text
  type="string"
  length="1"
  follows="left|top|right"
  height="15"
  layout="topleft"
  left="5"
  top_delta="4"
  name="description_label"
  width="120">
  Description:
  </text>
  <text_editor
  follows="left|top|right"
  height="85"
  width="292"
  hide_scrollbar="false"
  layout="topleft"
  left_delta="0"
  top_pad="10"
  max_length="1023"
  name="pick_desc"
  spellcheck="true"
  word_wrap="true" />
  <panel
  follows="left|top|right"
  height="20"
  bg_visible="true"
  bg_alpha_color="0.10 0.43 0.77 0.28"
  layout="topleft"
  mouse_opaque="false"
  left="0"
  name="title"
  top_pad="8"
  width="312"/>
  <text
  type="string"
  length="1"
  follows="left|top|right"
  height="15"
  layout="topleft"
  left="5"
  name="location_label"
  top_delta="4"
  width="120">
  Location:
  </text>
  <button
  follows="left|top"
  height="15"
  label="⌊Set to current location⌉"
  pad_bottom="0"
  image_selected="Transparent"
  image_unselected="Transparent"
  layout="topleft"
  left_pad="38"
  top_delta="-2"
  name="set_to_curr_location_btn"
  width="136" />
  <text
  type="string"
  length="1"
  follows="left|top|right"
  height="28"
  layout="topleft"
  left="5"
  word_wrap="true"
  name="pick_location"
  top_pad="12"
  width="291">
  loading...
  </text>
  </panel>
 </scroll_container>
 <panel
 follows="left|right|bottom"
 height="25"
 label="bottom_panel"
 layout="topleft"
 left="10"
 name="bottom_panel"
 top_pad="1"
 width="316">
  <button
  follows="bottom|left"
  height="25"
  label="Save Pick"
  layout="topleft"
  image_hover_unselected="Toolbar_Middle_Over"
  image_selected="Toolbar_Middle_Selected"
  image_unselected="Toolbar_Middle_Off"
  name="save_changes_btn"
  top="0"
  left="1"
  width="155" />
  <button
  follows="bottom|right"
  height="25"
  label="Cancel"
  image_hover_unselected="Toolbar_Middle_Over"
  image_selected="Toolbar_Middle_Selected"
  image_unselected="Toolbar_Middle_Off"
  layout="topleft"
  name="cancel_btn"
  top="0"
  left_pad="1"
  width="156" />
 </panel>
</panel>
