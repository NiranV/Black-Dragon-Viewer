<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 follows="all"
 height="450"
 label="My Avatar"
 layout="topleft"
 left="0"
 name="script_limits_my_avatar_panel"
 top="0"
 width="510">
 <icon
				follows="all"
				height="415"
				color="1 1 1 1"
				image_name="Panel_Background"
				layout="topleft"
				name="vicon"
				mouse_opaque="false"
				visible="true"
				width="495"
				top="5"
				left="5"/>
    <scroll_list
     draw_heading="true"
     follows="all"
     height="415"
     layout="topleft"
     left_delta="1"
     multi_select="true"
     name="scripts_list"
     top_delta="1"
     width="493">
        <scroll_list.columns
         label="Size (kb)"
         name="size"
         width="70" />
        <scroll_list.columns
         label="URLs"
         name="urls"
         width="50" />
        <scroll_list.columns
         label="Object Name"
         name="name"
         width="200" />
        <scroll_list.columns
         label="Location"
         name="location"
         width="130" />
    </scroll_list>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="14"
     layout="topleft"
     left_delta="10"
     name="loading_text"
     top_delta="30"
     width="300">
        Loading...
    </text>
    <button
     follows="bottom|left"
     height="23"
     label="Refresh List"
     layout="bottomleft"
     left="6"
     image_hover_unselected="Toolbar_Middle_Over"
				image_selected="Toolbar_Middle_Selected"
				image_unselected="Toolbar_Middle_Off"
     name="refresh_list_btn"
     bottom="6"
     width="100" />
	 
  <text
	 type="string"
	 length="1"
	 follows="left|bottom"
	 height="14"
	 layout="topleft"
	 left_pad="7"
	 name="memory_used"
	 top_delta="-1"
	 width="620">

	</text>
	<text
	 type="string"
	 length="1"
	 follows="left|bottom"
	 height="14"
	 layout="topleft"
	 left_delta="0"
	 name="urls_used"
	 top_pad="-3"
	 width="620">

	</text>
</panel>
