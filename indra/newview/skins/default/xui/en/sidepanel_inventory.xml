<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
background_visible="false"
follows="all"
height="570"
label="Things"
layout="topleft"
min_height="200"
min_width="240"
name="objects panel"
width="320">
 <panel.string
   name="Itemcount">
  </panel.string>
  <panel.string
   name="ItemcountFetching">
   ([ITEM_COUNT])...
  </panel.string>
  <panel.string
   name="ItemcountCompleted">
   [ITEM_COUNT]
  </panel.string>
  <panel.string
   name="ItemcountUnknown">
			---
  </panel.string>
  <panel.string
   name="buycurrencylabel">
        [AMT] L$
  </panel.string>
 <panel
 follows="all"
 layout="topleft"
 left="0"
 name="sidepanel_inventory_panel"
 top="0"
 label=""
 height="570"
 visible="true"
 width="317">
  <layout_stack
  follows="left|right|top|bottom"
  layout="topleft"
  left="0"
  top="0"
  orientation="vertical"
  name="inventory_layout_stack"
  height="546"
  width="317">
   <layout_panel
   name="main_inventory_layout_panel"
   layout="topleft"
   min_dim="100"
   width="330"
   follows="bottom|left|right"
   auto_resize="true"
   height="295">
    <panel
    class="panel_main_inventory"
    filename="panel_main_inventory.xml"
    follows="all"
    layout="topleft"
    left="0"
    name="panel_main_inventory"
    top="0"
    label=""
    height="300"
    width="330" />
   </layout_panel>
   <layout_panel
   width="330"
   layout="topleft"
   auto_resize="false"
   user_resize="true"
   follows="left|right|top"
   name="inbox_layout_panel"
   visible="true"
   min_dim="25"
   expanded_min_dim="90"
   height="220">
    <panel
    follows="all"
    layout="topleft"
    left="0"
    name="marketplace_inbox"
    class="panel_marketplace_inbox"
    top="0"
    label=""
    height="220"
    width="330">
     <string name="InboxLabelWithArg">[NUM] Received items</string>
     <string name="InboxLabelNoArg">No Received items</string>
	 <icon
	  follows="all"
	  height="220"
	  image_name="Panel_Background"
	  layout="topleft"
	  name="vicon"
	  mouse_opaque="false"
	  visible="true"
	  width="313"
	  top="0"
	  bottom="-1"
	  left="9"/>
     <text
     type="string"
     length="1"
     font.style="BOLD"
     text_color="BeaconColor"
     follows="right|top"
     layout="topleft"
     height="13"
     top="4"
     right="-20"
     name="inbox_fresh_new_count"
     font="SansSerifMedium"
     halign="right"
     top_pad="0"
     width="300">
     [NUM] new
     </text>
      <panel
      follows="all"
      left="8"
      bottom="216"
      width="313"
      top="19"
      background_visible="false"
      tool_tip="Drag and drop items to your inventory to use them">
       <text
       name="inbox_inventory_placeholder"
       type="string"
       follows="all"
       layout="topleft"
       top="5"
       left_delta="0"
	   h_pad="5"
	   v_pad="4"
       width="313"
       height="193"
       wrap="true"
       halign="center">
       Purchases from the marketplace will be delivered here.
       </text>
      </panel>
	  <button
     control_name="InventoryInboxToggleState"
     label="Received items"
     font="SansSerifMedium"
     name="inbox_btn"
     height="23"
     width="309"
     image_hover_unselected="Toolbar_Middle_Over"
     image_overlay="Command_Marketplace_Icon"
     image_selected="Toolbar_Middle_Selected"
     image_unselected="Toolbar_Middle_Off"
     image_overlay_alignment="left"
        imgoverlay_label_space="10"
     handle_right_mouse="false"
     halign="left"
     image_bottom_pad="3"
     follows="top|left|right"
     is_toggle="true"
     tab_stop="false"
     pad_left="7"
     top="2"
     left="11" />
     </panel>
    </layout_panel>
  </layout_stack>
  <text
   follows="right|bottom"
   height="17"
   layout="bottomleft"
   halign="right"
   right="-34"
   word_wrap="false"
   name="ItemcountText"
   font="SansSerifSmall"
   font.style="BOLD"
   text_color="White_50"
   bottom="0"
   width="100">
       ---
   </text>
   <icon 
   scale_image="true"
   follows="right|bottom"
   layout="topleft"
   color="1 1 1 0.7"
   height="18"
   image_name="Command_PF_Characters_Icon"
   right="-12"
   top_delta="-3"
   width="18" />
   <icon 
   scale_image="true"
   follows="left|bottom"
   layout="topleft"
   color="1 1 1 1"
   height="18"
   image_name="Balance"
   left="13"
   top_delta="0"
   width="18" />
   <text
   follows="left|bottom"
   height="17"
   layout="topleft"
   halign="left"
   left_pad="5"
   word_wrap="false"
   tool_tip="Click to refresh your L$ balance"
   value="? L$" 
   name="balance"
   font="SansSerifSmall"
   font.style="BOLD"
   text_color="White_50"
   top_delta="3"
   width="100"/>
 </panel>
 <panel
 follows="all"
 layout="topleft"
 left="0"
 class="sidepanel_item_info"
 filename="sidepanel_item_info.xml"
 name="sidepanel__item_panel"
 top="0"
 label=""
 height="570"
 visible="false"
 width="330"/>
 <panel
 follows="all"
 layout="topleft"
 left="0"
 class="sidepanel_task_info"
 filename="sidepanel_task_info.xml"
 name="sidepanel__task_panel"
 top="0"
 label=""
 height="570"
 visible="false"
 width="330"/>
</panel>
