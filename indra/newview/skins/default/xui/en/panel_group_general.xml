<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 label="General"
 height="404"
 width="308"
 follows="left|top"
 layout="topleft"
 class="panel_group_general"
 name="general_tab">
    <panel.string
     name="help_text">
        The General tab contains general information about this group, a list of members, general Group Preferences and member options.

Hover your mouse over the options for more help.
    </panel.string>
    <panel.string
     name="group_info_unchanged">
        General group information has changed
    </panel.string>
    <panel.string
     name="incomplete_member_data_str">
        Retrieving member data
    </panel.string>
    <scroll_container
         follows="all"
         layout="topleft"
         mouse_opaque="false"
         left="0"
         name="general_scroll"
         height="402"
         width="307"
         top="1">
      <panel
         bevel_style="none"
         border="false"
         follows="top|left|right"
         height="335"
         width="292"
         layout="topleft"
         left="0"
         name="general_container"
         top="0">
       <panel
 follows="left|top|right"
 height="20"
 bg_visible="true"
 bg_alpha_color="0.10 0.43 0.77 0.28"
 layout="topleft"
 mouse_opaque="false"
 left="3"
 name="title"
 top="1"
 width="303"/>
    <text
      font="SansSerifSmall"
      width="55"
      follows="top|left|right"
      layout="topleft"
      mouse_opaque="false"
     type="string"
     height="10"
     length="1"
     left="8"
     name="prepend_founded_by"
     top_delta="4">
      Founder:
    </text>
     <text
      follows="left|top|right"
     height="10"
     layout="topleft"
     left_pad="0"
     name="founder_name"
     top_delta="0"
     use_ellipses="true"
     value="secondlife:///app/agent/a7fe20fa-1e95-4f87-aa8f-86496c78c1e5/about"
     width="167" />
    <text
    text_color="0.2 0.9 0.3 1"
     type="string"
     follows="top|right"
     height="10"
     layout="topleft"
     left_pad="10"
     halign="center"
     name="join_cost_text"
     top_delta="-5"
     visible="true"
     width="70">
      Free
    </text>
    <button
   label="⌊Join Now⌉"
   name="btn_join"
   layout="topleft"
   follows="top|right"
   top_delta="3"
   left_delta="0"
   halign="center"
   visible="true"
   pad_bottom="0"
   image_selected="Transparent"
   image_unselected="Transparent"
   width="72"/>
    <button
   label="⌊Can't join⌉"
   name="btn_no_join"
   layout="topleft"
   follows="top|right"
   top_delta="1"
   left_delta="-3"
   halign="left"
   enabled="false"
   visible="false"
   image_selected="Transparent"
   image_unselected="Transparent"
   width="72"/>
    <texture_picker
     default_image_name="Generic_Group_Large"
     follows="left|top"
     height="110"
     label=""
     layout="topleft"
     left="6"
     name="insignia"
     no_commit_on_selection="true"
     tool_tip="Click to choose a picture"
     top_pad="1"
     width="90" />
    <text_editor
     type="string"
     follows="left|top|right"
     left_pad="1"
     height="86"
     width="195"
     layout="topleft"
     max_length="511"
     name="charter"
     top_delta="1"
     word_wrap="true">
     Group Charter
    </text_editor>
    <panel
 follows="left|top|right"
 height="20"
 bg_visible="true"
 bg_alpha_color="0.10 0.43 0.77 0.28"
 layout="topleft"
 mouse_opaque="false"
 left="3"
 name="title"
 top_pad="7"
 width="303"/>
         <text
         follows="left|top|right"
         type="string"
         height="10"
         layout="topleft"
         left="8"
         name="my_group_settngs_label"
         top_delta="4"
         width="150">
           Settings regarding myself:
        </text>
         <text
         follows="left|top|right"
         type="string"
         height="10"
         layout="topleft"
         left="8"
         name="active_title_label"
         top_pad="15"
         width="63">
            My Title:
        </text>
        <combo_box
         follows="left|top|right"
         height="23"
         layout="topleft"
         left_pad="9"
         width="212"
         name="active_title"
         tool_tip="Sets the title that appears in your avatar&apos;s name tag when this group is active."
         top_delta="-4" >
            <combo_box.combo_button
            image_unselected="ToolbarDropDown_Off"
            image_selected="ToolbarDropDown_Press"
            image_disabled="ToolbarDropDown_Off" />
            <combo_box.drop_down_button
            image_unselected="ToolbarDropDown_Off"
            image_selected="ToolbarDropDown_Press"
            image_pressed="ToolbarDropDown_Press" 
            image_pressed_selected="ToolbarDropDown_Press"
            image_disabled="ToolbarDropDown_Off" />
        </combo_box>
        <check_box
         follows="left|top"
         height="14"
         font="SansSerifSmall"
         label="Receive group notices"
         layout="topleft"
         left="8"
         name="receive_notices"
         tool_tip="Sets whether you want to receive Notices from this group.  Uncheck this box if this group is spamming you."
         top_pad="5"
         width="300" />
        <check_box
         follows="left|top"
         height="14"
         label="Show in my profile"
         layout="topleft"
         left="8"
         name="list_groups_in_profile"
         tool_tip="Sets whether you want to show this group in your profile"
         top_pad="1"
         width="295" />
      <panel
 follows="left|top|right"
 height="20"
 bg_visible="true"
 bg_alpha_color="0.10 0.43 0.77 0.28"
 layout="topleft"
 mouse_opaque="false"
 left="3"
 name="title"
 top_pad="10"
 width="303"/>
        <text
         follows="left|top"
         type="string"
         height="10"
         layout="topleft"
         left="8"
         top_delta="4"
         name="group_settngs_label"
         width="120">
         Group Settings:
        </text>
        <text
         follows="left|top|right"
         type="string"
         height="10"
         layout="topleft"
         left="8"
         name="active_title_label"
         top_pad="15"
         width="63">
            Maturity:
        </text>
      <icons_combo_box
         follows="left|top|right"
         height="23"
         layout="topleft"
         left_pad="9"
         name="group_mature_check"
         tool_tip="Sets whether your group contains information rated as Moderate"
         top_delta="-4"
         width="212">
            <icons_combo_box.combo_button
            image_unselected="ToolbarDropDown_Off"
            image_selected="ToolbarDropDown_Press"
            image_disabled="ToolbarDropDown_Off" />
            <icons_combo_box.drop_down_button
            image_unselected="ToolbarDropDown_Off"
            image_overlay_alignment="left"
            image_selected="ToolbarDropDown_Press"
            image_pressed="ToolbarDropDown_Press" 
            image_pressed_selected="ToolbarDropDown_Press"
            image_disabled="ToolbarDropDown_Off" />
            <icons_combo_box.item
               label="General Content"
               name="pg"
               value="Not Mature">
               <item.columns
                 halign="right"
                 type="icon"
                 value="Parcel_PG_Light"
                 width="20"/>
             </icons_combo_box.item>
            <icons_combo_box.item
               label="Mature Content"
               name="mature"
               value="Mature">
               <item.columns
                 halign="right"
                 type="icon"
                 value="Parcel_M_Light"
                 width="20"/>
             </icons_combo_box.item>
        </icons_combo_box>
      <check_box
         follows="left|top"
         height="14"
         initial_value="true"
         label="Show in search"
         layout="topleft"
         left="10"
         name="show_in_group_list"
         tool_tip="Let people see this group in search results"
         top_pad="5"
         width="300" />
        <check_box
         follows="right|top|left"
         height="14"
         label="Anyone can join"
         layout="topleft"
         left="10"
         top_pad="1"
         name="open_enrollement"
         tool_tip="Sets whether this group allows new members to join without being invited."
         width="90" />
        <check_box
         follows="left|top"
         height="14"
         label="Cost to join"
         layout="topleft"
         left_delta="0"
         name="check_enrollment_fee"
         tool_tip="Sets whether to require an enrollment fee to join the group"
         top_pad="1"
         width="120" />
        <spinner
         decimal_digits="0"
         follows="left|top"
         halign="left"
         height="23"
         increment="1"
         layout="topleft"
         max_val="99999"
         left_pad="5"
         top_delta="-2"
         name="spin_enrollment_fee"
         tool_tip="New members must pay this fee to join the group when Enrollment Fee is checked."
         width="100" />

    </panel>
    </scroll_container>
</panel>
