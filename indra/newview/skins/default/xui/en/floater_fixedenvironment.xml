<floater
height="222"
layout="topleft"
name="Fixed Environment"
save_rect="true"
title="Fixed Environment"
save_visibility="false"
help_topic="fixed_environment"
single_instance="true" 
width="700">
    <string name="title_new">
    Create a new Sky Preset
    </string>
    <string name="title_edit">
    Edit Sky Preset
    </string>
    <string name="title_neutral">
    Edit a Sky Preset or create a new one.
    </string>
    <string name="combo_label">
    -Type or select a preset-
    </string>
    <string name="CantSave">
    Fixed Environment - Can't save! Please select a local preset first.
    </string>
    <string name="CanSave">
    Fixed Environment
    </string>
    <icon
    follows="top|left"
    height="193"
    color="1 1 1 1"
    image_name="Panel_Background"
    layout="topleft"
    name="vicon"
    mouse_opaque="false"
    visible="true"
    width="686"
    top="22"
    left="7"/>
    <tab_container
    follows="left|top"
    height="188"
    halign="center"
    layout="topleft"
    tab_height="20"
    left="5"
    name="tab_settings"
    tab_position="top"
    top="2"
    width="689">
    <!-- Tabs inserted here in code -->
    </tab_container>
    
    <combo_box
    follows="top|left"
    layout="topleft"
    allow_text_entry="true"
    left="9"
    max_chars="100"
    height="22"
    name="sky_preset_combo"
    top_pad="1"
    width="177">
    <combo_box.combo_editor
    text_pad_left="5"
    background_image="TextField_Off"
    background_image_disabled="TextField_Disabled"
    background_image_focused="TextField_Active"/>
    <combo_box.combo_button
    image_unselected="ComboToolbarButton_Off"
    image_selected="ComboToolbarButton_Selected"
    image_pressed="ComboToolbarButton_Selected" 
    image_pressed_selected="ComboToolbarButton_Selected"/>
    <combo_box.drop_down_button
    image_unselected="ComboToolbarButton_Off"
    image_selected="ComboToolbarButton_Selected"
    image_pressed="ComboToolbarButton_Selected" 
    image_pressed_selected="ComboToolbarButton_Selected"
    image_disabled="ComboToolbarButton_Disabled"/>
    </combo_box>
    <button
    follows="bottom|right"
    height="23"
    label="Commit"
    image_hover_unselected="Toolbar_Middle_Over"
    image_selected="Toolbar_Middle_Selected"
    image_unselected="Toolbar_Middle_Off"
    layout="topleft"
    left_pad="-7"
    top_delta="0"
    name="btn_commit"
    width="120"/>
    <button
    follows="bottom|right"
    height="22"
    name="btn_flyout"
    label=""
    layout="topleft"
    left_pad="1"
    tab_stop="false"
    top_delta="0"
    image_unselected="ComboToolbarButton_Off"
    image_selected="ComboToolbarButton_Selected"
    image_pressed="ComboToolbarButton_Selected" 
    width="19"/>
    <button
    follows="bottom|right"
    height="23"
    label="Import"
    image_hover_unselected="Toolbar_Middle_Over"
    image_selected="Toolbar_Middle_Selected"
    image_unselected="Toolbar_Middle_Off"
    layout="topleft"
    left_pad="0"
    top_delta="0"
    name="btn_import"
    width="80"/>
    <button
    follows="bottom|right"
    height="23"
    label="Delete"
    image_hover_unselected="Toolbar_Middle_Over"
    image_selected="Toolbar_Middle_Selected"
    image_unselected="Toolbar_Middle_Off"
    layout="topleft"
    left_pad="1"
    name="delete"
    width="80"/>
    <icon
    follows="left|bottom|right"
    image_name="Toolbar_Middle_Off"
    height="23"
    width="129"
    layout="topleft"
    name="sky_preset_icon"
    left_pad="1"
    top_delta="0"/>
    <button
    follows="bottom|right"
    height="23"
    label="Cancel"
    image_hover_unselected="Toolbar_Middle_Over"
    image_selected="Toolbar_Middle_Selected"
    image_unselected="Toolbar_Middle_Off"
    layout="topleft"
    left_pad="1"
    top_delta="0"
    name="btn_cancel"
    width="80"/>
</floater>
