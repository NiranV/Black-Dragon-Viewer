<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 legacy_header_height="8"
 can_resize="true"
 height="500"
 layout="topleft"
 min_height="320"
 min_width="450"
 name="worldmap"
 background_color="FloaterDefaultBackgroundColor"
 save_rect="true"
 save_visibility="true"
 single_instance="true"
 title="World Map"
 width="500">
   <text
   text_color="White"
   font="SansSerifLarge"
   type="string"
   length="1"
   follows="top|left"
   halign="left"
   height="16"
   layout="topleft"
   left="8"
   name="legend_label"
   top="10"
   width="215">
      Legend
   </text>
   <button
   follows="left|top"
   height="22"
   image_overlay="map_avatar_16.tga"
   image_selected="transparent.j2c"
   image_unselected="transparent.j2c"
   scale_image="true"
   left="4"
   layout="topleft"
   name="Show My Location"
   tool_tip="Center map on my avatar&apos;s location"
   top_pad="0"
   width="24" >
      <button.commit_callback
      function="WMap.ShowAgent" />
   </button>
   <text
   type="string"
   length="1"
   follows="top|left"
   halign="left"
   height="13"
   top_delta="6"
   left_pad="0"
   layout="topleft"
   name="me_label"
   width="90">
      Me
   </text>
   <button
   follows="top|left"
   height="22"
   image_overlay="map_home.tga"
   image_selected="transparent.j2c"
   image_unselected="transparent.j2c"
   scale_image="true"
   label_color="White"
   layout="topleft"
   left="4"
   top_pad="-2"
   name="Go Home"
   tool_tip="Teleport to my home location"
   width="24" >
      <button.commit_callback
      function="WMap.GoHome" />
   </button>
   <text
   type="string"
   length="1"
   follows="top|left"
   halign="left"
   height="13"
   top_delta="6"
   left_pad="0"
   layout="topleft"
   name="Home_label"
   width="70">
      Home
   </text>
   <check_box
   control_name="MapShowPeople"
   follows="top|left"
   height="16"
   layout="topleft"
   left="100"
   name="people_chk"
   top="15"
   width="22" />
   <icon
   color="0 1 0 1"
   follows="top|left"
   height="10"
   image_name="map_avatar_32.tga"
   layout="topleft"
   left_pad="2"
   mouse_opaque="true"
   name="people_chek"
   top_delta="2"
   width="10" />
   <text
   type="string"
   length="1"
   follows="top|left"
   halign="left"
   height="16"
   top_delta="-2"
   left_pad="6"
   layout="topleft"
   name="person_label"
   width="90">
   Person
   </text>
   <check_box
   control_name="MapShowInfohubs"
   follows="top|left"
   height="16"
   layout="topleft"
   left="100"
   name="infohub_chk"
   top_pad="0"
   width="22" />
   <icon
   follows="top|left"
   height="15"
   image_name="map_infohub.tga"
   layout="topleft"
   left_pad="0"
   mouse_opaque="true"
   name="infohub"
   top_delta="-1"
   width="15" />
   <text
   type="string"
   length="1"
   follows="top|left"
   halign="left"
   height="16"
   top_delta="1"
   left_pad="3"
   layout="topleft"
   name="infohub_label"
   width="90">
      Infohub
   </text>
   <check_box
   control_name="MapShowLandForSale"
   follows="top|left"
   height="16"
   layout="topleft"
   left="100"
   name="land_for_sale_chk"
   top_pad="0"
   width="22" />
   <icon
   follows="top|left"
   height="15"
   image_name="Icon_For_Sale"
   layout="topleft"
   mouse_opaque="true"
   name="landforsale"
   top_delta="0"
   left_pad="0"
   width="15" />
   <text
   type="string"
   length="1"
   follows="top|left"
   halign="left"
   height="16"
   top_delta="1"
   left_pad="3"
   layout="topleft"
   name="land_sale_label"
   width="90">
   Land Sale
   </text>
   <check_box
   control_name="MapShowEvents"
   follows="top|left"
   height="16"
   layout="topleft"
   left="225"
   top="15"
   name="event_chk"
   width="22" />
   <icon
   follows="top|left"
   height="16"
   image_name="Parcel_PG_Dark"
   layout="topleft"
   mouse_opaque="true"
   name="event"
   top_delta="-1"
   left_pad="0"
   width="18" />
   <text
   type="string"
   length="1"
   follows="top|left"
   halign="left"
   height="16"
   top_delta="2"
   left_pad="3"
   layout="topleft"
   name="pg_label"
   width="60">
      General
   </text>
   <check_box
   control_name="ShowMatureEvents"
   follows="top|left"
   height="16"
   initial_value="true"
   layout="topleft"
   left="225"
   name="events_mature_chk"
   top_pad="0"
   width="22" />
   <icon
   follows="top|left"
   height="16"
   image_name="Parcel_M_Dark"
   layout="topleft"
   mouse_opaque="true"
   name="events_mature_icon"
   top_delta="-1"
   left_pad="0"
   width="18" />
   <text
   type="string"
   length="1"
   follows="top|left"
   halign="left"
   height="16"
   top_delta="2"
   left_pad="3"
   layout="topleft"
   name="events_mature_label"
   width="66">
      Moderate
   </text>
   <check_box
   control_name="ShowAdultEvents"
   follows="top|left"
   height="16"
   layout="topleft"
   left="225"
   name="events_adult_chk"
   top_pad="0"
   width="22" />
   <icon
   follows="top|left"
   height="16"
   image_name="Parcel_R_Dark"
   layout="topleft"
   left_pad="0"
   mouse_opaque="true"
   name="events_adult_icon"
   top_delta="-1"
   width="18" />
   <text
   type="string"
   length="1"
   follows="top|left"
   halign="left"
   height="16"
   top_delta="2"
   left_pad="3"
   layout="topleft"
   name="events_adult_label"
   width="66">
      Adult
   </text>
   <icon
   color="0.5 0.25 1 1"
   follows="top|left"
   height="17"
   image_name="legend.tga"
   layout="topleft"
   mouse_opaque="true"
   name="square2"
   left="350"
   top="15"
   width="17" />
   <text
   type="string"
   length="1"
   follows="top|left"
   height="16"
   layout="topleft"
   left_pad="0"
   name="auction_label"
   top_delta="2"
   width="100">
      land auction
   </text>
   <icon
   color="1 1 0.25 1"
   follows="top|left"
   height="17"
   image_name="legend.tga"
   layout="topleft"
   mouse_opaque="true"
   name="square2"
   left="350"
   top_pad="-5"
   width="17" />
   <text
   type="string"
   length="1"
   follows="top|left"
   height="16"
   layout="topleft"
   left_pad="0"
   name="by_owner_label"
   top_delta="2"
   width="100">
      by owner
   </text>
   <button
	 control_name="ShowBigWorldMap"
	 follows="right|top"
	 height="18"
	 image_selected="TabIcon_Close_Off"
	 image_unselected="TabIcon_Open_Off"
	 scale_image="true"
	 toggle="true"
	 right="-5"
	 layout="topleft"
	 name="Worldmap Controls"
	 tool_tip="Show/Hide the worldmap controls"
	 top_pad="3"
	 width="18"/>
   <layout_stack
   border_size="0"
   clip="true"
   follows="all"
   height="432"
   layout="topleft"
   left="0"
   mouse_opaque="false"
   name="toolbar_stack"
   orientation="horizontal"
   top="68"
   width="495">
      <layout_panel
      auto_resize="true"
      follows="all"
      height="500"
      left="0"
      width="300"
      background_visible="false"
      bg_alpha_color="Black_75"
      layout="topleft"
      mouse_opaque="true"
      name="world_panel">
	 <panel
	 filename="panel_world_map.xml"
	 follows="all"
	 height="490"
	 layout="topleft"
	 left="5"
	 name="objects_mapview"
	 top="0"
	 width="295" />
	 <icon
	 follows="right|top"
	 height="16"
	 image_name="Zoom_Off"
	 layout="topleft"
	 right="-10"
	 mouse_opaque="true"
	 name="zoom_icon"
	 top="13"
	 width="16"/>
	 <slider
	 follows="right|top"
	 height="150"
	 increment="0.2"
	 orientation="vertical"
	 initial_value="-2"
	 left_delta="0"
	 layout="topleft"
	 max_val="0"
	 top_pad="5"
	 min_val="-8"
	 name="zoom slider"
	 show_text="false"
	 width="16" />
	 <icon
	 follows="right|top"
	 height="16"
	 image_name="UnZoom_Off"
	 layout="topleft"
	 right="-10"
	 mouse_opaque="true"
	 name="zoom_icon"
	 top_pad="5"
	 width="16"/>
      </layout_panel>
      <layout_panel
      visibility_control="ShowBigWorldMap"
      auto_resize="false"
      follows="right|top|bottom"
      height="580"
      right="0"
      min_width="200"
      width="200"
      background_visible="false"
      layout="topleft"
      mouse_opaque="true"
      name="controls_panel">
	 <panel
	 name="layout_panel_1"
	 width="200"
	 follows="left|top|bottom"
	 height="580"
	 top="0"
	 left="0"
	 background_visible="false">
	    <text
	    text_color="White"
	    font="SansSerifLarge"
	    type="string"
	    length="1"
	    follows="top|right"
	    halign="left"
	    height="16"
	    layout="topleft"
	    left="5"
	    name="find_on_map_label"
	    top="3"
	    width="190">
	       Find on Map
	    </text>
	    <icon
	    color="0.5 0 0 1"
	    follows="top|right"
	    height="16"
	    image_name="map_track_16.tga"
	    layout="topleft"
	    left="5"
	    top_pad="8"
	    mouse_opaque="true"
	    name="avatar_icon"
	    width="16" />
	    <combo_box
	    allow_text_entry="true"
	    follows="top|right"
	    height="23"
	    label="Online Friends"
	    layout="topleft"
	    top_delta="-4"
	    left_pad="5"
	    max_chars="60"
	    name="friend combo"
	    tool_tip="Show friends on map"
	    width="170">
	       <combo_box.item
	       label="My Friends Online"
	       name="item1"
	       value="None" />
	       <combo_box.commit_callback
	       function="WMap.AvatarCombo"/>
	    </combo_box>
	    <icon
	    color="0.5 0 0 1"
	    follows="top|right"
	    height="16"
	    image_name="map_track_16.tga"
	    layout="topleft"
	    left="5"
	    top_pad="5"
	    mouse_opaque="true"
	    name="landmark_icon"
	    width="16" />
	    <combo_box
	    allow_text_entry="true"
	    follows="top|right"
	    height="23"
	    label="My Landmarks"
	    layout="topleft"
	    top_delta="-3"
	    left_pad="5"
	    max_chars="64"
	    name="landmark combo"
	    tool_tip="Landmark to show on map"
	    width="170">
	       <combo_box.item
	       label="My Landmarks"
	       name="item1"
	       value="None" />
	       <combo_box.commit_callback
	       function="WMap.Landmark"/>
	    </combo_box>
	    <icon
	    color="0.5 0 0 1"
	    follows="top|right"
	    height="16"
	    image_name="map_track_16.tga"
	    layout="topleft"
	    left="5"
	    top_pad="5"
	    mouse_opaque="true"
	    name="location_icon"
	    width="16" />
	    <search_editor
	    follows="top|right"
	    search_button_visible="false"
	    height="22"
	    text_readonly_color="DkGray"
	    label="Regions by Name"
	    layout="topleft"
	    text_pad_left="25"
	    text_pad_right="25"
	    top_delta="-2"
	    left_pad="5"
	    name="location"
	    select_on_focus="true"
	    tool_tip="Type the name of a region"
	    width="165">
	       <search_editor.commit_callback
	       function="WMap.Location" />
	    </search_editor>
	    <button
	    follows="top|right"
	    height="15"
	    label=""
	    image_unselected="toolbar_icons/Search.png"
	    image_color="0 0 0 0.5"
	    layout="topleft"
	    left_delta="6"
	    top_delta="3"
	    name="DoSearch"
	    tool_tip="Search for region"
	    width="16">
	       <button.commit_callback
	       function="WMap.Location" />
	    </button>
	    <button
	    image_unselected="StopReload_Off"
	    image_color="0 0 0 0.75"
	    follows="top|right"
	    height="15"
	    layout="topleft"
	    left_pad="122"
	    name="Clear"
	    tool_tip="Clear tracking lines and reset map"
	    top_delta="0"
	    width="15">
	       <button.commit_callback
	       function="WMap.Clear" />
	    </button>
	    <scroll_list
	    draw_stripes="false"
	    follows="all"
	    height="380"
	    layout="topleft"
	    left="5"
	    name="search_results"
	    visible="true"
	    top_pad="5"
	    width="190"
	    sort_column="1">
	       <scroll_list.columns
	       label=""
	       name="icon"
	       width="16" />
	       <scroll_list.columns
	       label=""
	       name="sim_name"
	       width="193" />
	       <scroll_list.commit_callback
	       function="WMap.SearchResult" />
	    </scroll_list>
	    <text
	    type="string"
	    length="1"
	    follows="right|bottom"
	    halign="left"
	    height="16"
	    layout="topleft"
	    left="5"
	    name="events_label"
	    top_pad="3"
	    width="60">
	       Position:
	    </text>
	    <text
	    type="string"
	    length="1"
	    text_color="1 0.1 0.2 0.6"
	    follows="right|bottom"
	    halign="left"
	    height="16"
	    font="DejaVu"
	    font.style="BOLD"
	    layout="topleft"
	    left="5"
	    name="X_label"
	    top_pad="3"
	    width="10">
	       X
	    </text>
	    <spinner
	    control_name="Teleport_Coordinate_X"
	    decimal_digits="0"
	    follows="right|bottom"
	    height="23"
	    increment="1"
	    initial_value="128"
	    layout="topleft"
	    left_pad="0"
	    top_delta="-3"
	    max_val="255"
	    min_val="0"
	    name="teleport_coordinate_x"
	    width="47" >
	       <spinner.commit_callback 
	       function="WMap.Coordinates" />
	    </spinner>
	    <text
	    type="string"
	    length="1"
	    text_color="0.1 1 0.1 0.6"
	    follows="right|bottom"
	    halign="left"
	    height="16"
	    font="DejaVu"
	    font.style="BOLD"
	    layout="topleft"
	    left_pad="5"
	    name="Y_label"
	    top_delta="3"
	    width="10">
	       Y
	    </text>
	    <spinner
	    control_name="Teleport_Coordinate_Y"
	    decimal_digits="0"
	    follows="right|bottom"
	    height="23"
	    increment="1"
	    initial_value="128"
	    layout="topleft"
	    left_pad="0"
	    top_delta="-3"
	    max_val="255"
	    min_val="0"
	    name="teleport_coordinate_y"
	    width="47" >
	       <spinner.commit_callback
	       function="WMap.Coordinates" />
	    </spinner>
	    <text
	    type="string"
	    length="1"
	    text_color="0.4 0.4 1 0.6"
	    follows="right|bottom"
	    halign="left"
	    height="16"
	    font="DejaVu"
	    font.style="BOLD"
	    layout="topleft"
	    left_pad="5"
	    name="Z_label"
	    top_delta="4"
	    width="10">
	    Z
	    </text>
	    <spinner
	    control_name="Teleport_Coordinate_Z"
	    decimal_digits="0"
	    follows="right|bottom"
	    height="23"
	    increment="1"
	    initial_value="23"
	    layout="topleft"
	    left_pad="0"
	    top_delta="-3"
	    max_val="4096"
	    min_val="0"
	    name="teleport_coordinate_z"
	    width="52" >
	       <spinner.commit_callback
	       function="WMap.Coordinates" />
	    </spinner>
	    <button
	    follows="right|bottom"
	    height="23"
	    image_unselected="PushButton_On"
	    label="Teleport"
	    layout="topleft"
	    left="5"
	    name="Teleport"
	    tool_tip="Teleport to selected location"
	    top_pad="3"
	    width="90">
	       <button.commit_callback
	       function="WMap.Teleport" />
	    </button>
	    <button
	    follows="right|bottom"
	    height="23"
	    label="Copy SLurl"
	    layout="topleft"
	    left_pad="5"
	    name="copy_slurl"
	    tool_tip="Copies current location as SLurl to be used on the web."
	    top_delta="0"
	    width="90">
	       <button.commit_callback
	       function="WMap.CopySLURL" />
	    </button>
	    <button
	    enabled="false"
	    follows="right|bottom"
	    height="23"
	    label="Show Selection"
	    left="5"
	    top_pad="5"
	    name="Show Destination"
	    tool_tip="Center map on selected location"
	    width="185">
	       <button.commit_callback
	       function="WMap.ShowTarget" />
	    </button>
	 </panel>
      </layout_panel>
   </layout_stack>
</floater>
