<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 border="false"
 can_close="true"
 can_minimize="true"
 bottom="275"
 left="300"
 can_resize="false"
 height="120"
 width="325"
 name="spellcheck_import"
 background_color="FloaterColorPreferences"
 title="Import Dictionary">
  <text
   follows="top|left"
   height="16"
   layout="topleft"
   left="10"
   top="15"
   type="string"
   width="65">
   Dictionary:
  </text>
  <line_editor
   enabled="false"
   follows="left|top"
   height="23"
   layout="topleft"
   left_pad="10"
   max_length_bytes="255"
   name="dictionary_path"
   top_delta="-5"
   width="200" />
  <button
   follows="left|top"
   height="23"
   label="..."
   label_selected="Browse"
   layout="topleft"
   left_pad="5"
   name="dictionary_path_browse"
   top_delta="0"
   width="25" />
  <text
   follows="top|left"
   height="16"
   layout="topleft"
   left="10"
   top_pad="8"
   type="string"
   width="65">
   Name:
  </text>
  <line_editor
   enabled="false"
   follows="left|top"
   height="23"
   layout="topleft"
   left_pad="10"
   max_length_bytes="255"
   name="dictionary_name"
   top_delta="-5"
   width="200" />
  <text
   follows="top|left"
   height="16"
   layout="topleft"
   left="10"
   top_pad="8"
   type="string"
   width="65">
   Language:
  </text>
  <line_editor
   follows="left|top"
   height="23"
   layout="topleft"
   left_pad="10"
   max_length_bytes="255"
   name="dictionary_language"
   top_delta="-5"
   width="200" />
  <button
   top_pad="5"
   right="-110"
   height="22"
   width="90"
   enabled="true"
   follows="left|top"
   mouse_opaque="true"
   halign="center"
   scale_image="true"
   name="ok_btn"
   label="Import" />
  <button
   top_delta="0"
   right="-10"
   height="22"
   width="90"
   enabled="true"
   follows="left|top"
   mouse_opaque="true"
   halign="center"
   scale_image="true"
   name="cancel_btn"
   label="Cancel" />
</floater>