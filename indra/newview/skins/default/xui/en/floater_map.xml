    <!-- RLVa shows the anonymized name on the regular tooltip when @shownames=n restricted (rather than show the avatar inspector) --> 
    <floater.string
     name="ToolTipMsg">
        [AGENT][REGION](Double-click to open Map, shift-drag to pan)
    </floater.string>
