<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 positioning="cascading"
 can_minimize="false" 
 can_resize="true"
 chrome="false"
 height="240"
 layout="topleft"
 min_height="128"
 min_width="128"
 legacy_header_height="25"
 name=""
 title=""
 save_rect="true"
 save_visibility="true"
 width="220">
    <!-- RLVa shows the anonymized name on the regular tooltip when @shownames=n restricted (rather than show the avatar inspector) --> 
    <floater.string
     name="ToolTipMsg">
        [AGENT][REGION](Double-click to open Map, shift-drag to pan)
    </floater.string>
    <floater.string
     name="AltToolTipMsg">
      [REGION](Double-click to teleport, shift-drag to pan)
    </floater.string>
	<floater.string
     name="RegionCoords">
      [X_COORD] | [Y_COORD] | [Z_COORD]
    </floater.string>
    <floater.string name="mini_map_caption">
	Mini-map
    </floater.string>
    <net_map
     follows="top|left|bottom|right"
     layout="topleft"
     left="4"
     mouse_opaque="false"
     name="Net Map"
     width="212"
     height="214"
     bg_color="Transparent"
     top="22"/>
    <text
     type="string"
     length="1"
     bottom="218"
     label="N"
     layout="topleft"
     left="0"
     name="floater_map_north"
     right="10"
	 bg_writeable_color="Black_75"
     bg_visible="true"
     text_color="1 0.3 0 1"
     top="189">
        N
    </text>
    <text
     type="string"
     length="1"
     bottom="218"
     label="E"
     layout="topleft"
     left="0"
     name="floater_map_east"
     right="10"
	 bg_writeable_color="Black_75"
     bg_visible="true"
     text_color="1 1 0 1"
     top="189">
        E
    </text>
    <text
     type="string"
     length="1"
     bottom="205"
     label="W"
     layout="topleft"
     left="0"
     name="floater_map_west"
     right="11"
	 bg_writeable_color="Black_75"
     bg_visible="true"
     text_color="0 1 0 1"
     top="175">
        W
    </text>
    <text
     type="string"
     length="1"
     bottom="218"
     label="S"
     layout="topleft"
     left="0"
     name="floater_map_south"
     right="10"
	 bg_writeable_color="Black_75"
     bg_visible="true"
     text_color="0 0.75 0.9 1"
     top="189">
        S
    </text>
	<text
   follows="top|left|right"
   height="17"
   layout="topleft"
   halign="center"
   word_wrap="false"
   bg_writeable_color="Black_75"
   bg_visible="true"
   name="sim_coords"
   font="SansSerifSmall"
   top="21"
   width="220"/>
	<text
   follows="top|left|right"
   height="17"
   layout="topleft"
   halign="left"
   word_wrap="false"
   bg_writeable_color="Black_75"
   bg_visible="false"
   name="sim_name"
   font="SansSerifSmall"
   left="5"
   top="5"
   width="220">
      Region Name
   </text>


</floater>
